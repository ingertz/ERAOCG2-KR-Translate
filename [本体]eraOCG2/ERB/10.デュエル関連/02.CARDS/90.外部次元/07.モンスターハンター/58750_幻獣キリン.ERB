@CARDNAME_58750,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 환상수 키린
	CASE "略称"
RESULTS = 환상수 키린
	CASE "カテゴリ"
RESULTS:0 = 수렵 조수
RESULTS:1 = 환상수
RESULTS:2 = 몬스터 헌터
ENDSELECT


@CARD_58750(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 3000
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0


@CARDSUMMON_58750(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6

IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

素材条件종족:0 = 드래곤족
素材条件종족:1 = 야수족
素材条件종족:2 = 비행야수족
素材条件종족:3 = 비늘야수족
素材条件종족:4 = 악마족
素材条件종족:5 = 바다생물족
素材条件종족:6 = 미확인족
素材条件종족:7 = 원소정령족
IF EX参照 == 1
	;PRINTL サモンまでは言ってるよ
	CALL SYNCAN(듀얼리스트,소환カード,58750) 
	RETURN RESULT
ENDIF



