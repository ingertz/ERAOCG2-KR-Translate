@CARD_EXPLANATION_39681(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①② 효과는 각각 1턴에 1번만 사용할 수 없다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 "박테리안" 이외의 몬스터가 존재하지 않는 경우에 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 서로의 턴에, 필드의 이 카드를 묘지로 보내고,
PRINTL 상대 필드의 기계족 암컷 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터의 공격력은 500 내린다.
PRINTL 그 후, 상대 필드에 "B 옵션" 1장을 특수 소환한다.
PRINTL 그 몬스터의 속성은 대상으로 한 몬스터와 같게 된다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39681)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,39681)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39681,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件固カテゴリ = 박테리안
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,39681)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 牝性
	효과条件종족 = 기계족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_39681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カード속성
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39681,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39681,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별 = 牝性
		효과条件종족 = 기계족
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所,39681)
		選択カード = RESULT
		IF 選択カード != 9999
			選択カード_ID = RESULT:99
			TRYCALLFORM CARD_{選択カード_ID},"속성"
			選択カード속성 = RESULT
			TRYCALLFORM CARDNAME_{選択カード_ID},"名前"
			選択カード名前 = %RESULTS%
			PRINTFORMW バ-ムは%選択カード名前%の体内に入り込むと、そのデ-タを基にオプションを作り、출산させた！
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"永攻",-500)
			CALL CARD_EROTICS(選択カード_ID)
			IF RESULT != -1
				조교대상 = RESULT
				CALL EROTIC_MOVE("疑似출산",조교대상)
			ENDIF
			IF モンスター数(対面者) < 5
				PRINTL
				PRINTFORMW 生まれたオプションは%選択カード名前%の傍を漂い始めた。
				CALL FULLRESET
				효과추가종류 = 効追永属 + 選択カード속성
				CALL CARD_GENERATE(対面者,139681)
			ENDIF
		ENDIF
	ENDIF
ENDIF


