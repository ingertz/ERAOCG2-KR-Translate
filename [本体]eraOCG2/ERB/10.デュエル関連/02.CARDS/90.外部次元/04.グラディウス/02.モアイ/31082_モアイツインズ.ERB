@CARDNAME_31082,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 모아이 트윈즈
	CASE "略称"
RESULTS = 모아이 트윈즈
	CASE "カテゴリ"
RESULTS:0 = 모아이
RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_31082(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 600
	CASE "守備力"
		RETURN 1800
ENDSELECT

RETURN 0

@CARD_EXPLANATION_31082(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,특수소환時)
PRINTL ①: 이 카드가 특수 소환했을 경우에 발동할 수 있다.
PRINTL 묘지에서 「모아이 트윈즈」 이외의 「모아이」 카드 1장을 패에 더한다.
CALLFORM CARD_ABLE(2,필드起動 ,フリー誘発)
PRINTL ②: 자신 · 상대의 메인 페이즈에 발동할 수 있다.
PRINTL 자신의 패 / 필드에서 「모아이」 몬스터 2장을 묘지로 보내고,
PRINTL 덱 외부에서 「빅 모아이」 1장을 생성한다.

@CARDCAN_31082(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,31082)
SIF RESULT == 1
	RETURN 0

IF 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,31082)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 모아이
	효과条件固名前 = 모아이 트윈즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,31082)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,131082,"",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件カテゴリ = 모아이
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_31082(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,31082,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 모아이
		효과条件固名前 = 모아이 트윈즈
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,31082,2)
	IF RESULT == 0
		CALL FULLRESET
		SIF モンスター数(듀얼리스트) < 5
			효과範囲손패 = 1
		효과範囲필드 = 1
		효과条件カテゴリ = 모아이
		효과条件종류 = モン全般
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,131082)
		ENDIF
	ENDIF
ENDIF


