@CARD_EXPLANATION_64884(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①② 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動)
PRINTL ①: 자신 메인 페이즈에 자신 필드의 "철혈" 엑스 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 그 카드의 엑스 소재를 1개 늘린다.
CALL CARD_ABLE(2,마함起動)
PRINTL ②: 자신 메인 페이즈에 자신 필드의 토큰 이외의 "철혈" 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 「전승 의장」 토큰을 특수 소환한다.
PRINTL 이 효과로 특수 소환한 토큰의 레벨은 대상 몬스터의 레벨 / 랭크 / 링크와 같아진다.
PRINTL 이 효과 발동 후, 자신은 턴 종료시까지 "철혈" 몬스터밖에 특수 소환할 수 없다.

@CARDCAN_64884(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,64884)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 철혈
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1 && !발동済_1(듀얼리스트,64884)
		条件Ａ = 1
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = トークン以外
	효과条件カテゴリ = 철혈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1 && !발동済_2(듀얼리스트,64884)
		CALL CARD_SS_CHECK(듀얼리스트,164884)
		SIF RESULT != 1 && モンスター数(듀얼리스트) < 5
			条件Ｂ = 1
	ENDIF
	SIF 条件Ａ == 0 && 条件Ｂ == 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_64884(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과
#DIM DYNAMIC 選択カード星

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 철혈
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1 && !발동済_1(듀얼리스트,64884)
		条件Ａ = 1
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 철혈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1 && !발동済_2(듀얼리스트,64884)
		CALL CARD_SS_CHECK(듀얼리스트,164884)
		SIF RESULT != 1 && モンスター数(듀얼리스트) < 5
			条件Ｂ = 1
	ENDIF
	IF 条件Ａ == 1 && 条件Ｂ == 1
		CALL YN_CHOICE(듀얼리스트,"②자신필드의 「철혈」Ｘモンスター１体のＸ素材＋１","③자신필드의 トークン 이외의 「철혈」モンスター１体のレベル ランク 링크を持つ伝承艤装トークンを특수소환")
		選択효과 = RESULT
	ELSEIF 条件Ｂ == 1
		選択효과 = いいえ
	ENDIF
	IF 選択효과 == はい
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64884,1)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件カテゴリ = 철혈
			효과条件종류 = 엑시즈
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			選択カード = RESULT
			IF RESULT != 9999
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"Ｘ",1)
			ENDIF
		ENDIF
	ELSEIF 選択효과 == いいえ
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64884,2)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件종류 = トークン以外
			효과条件カテゴリ = 철혈
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			選択カード = RESULT
			IF RESULT != 9999
				選択カード星 = GETVAR(@"%듀얼리스트%_MZ_星:選択カード")
				CALL CARD_GENERATE(듀얼리스트,164884)
				SIF 選択カード星 - 10 != 0
					CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"永星",選択カード星 - 10)
			ENDIF
			CALL CARD_SS_RESTRICTION(듀얼리스트,"철혈")
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64884)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ENDIF

