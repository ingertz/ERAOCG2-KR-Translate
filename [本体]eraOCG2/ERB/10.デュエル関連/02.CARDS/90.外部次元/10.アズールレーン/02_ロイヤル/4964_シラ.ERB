@CARDNAME_4964,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 경순양함 시라
	CASE "略称"
RESULTS = 시라
	CASE "カテゴリ"
RESULTS:0 = 벽람항로
RESULTS:1 = 로얄 (AL)
ENDSELECT

@CARD_4964(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1400
	CASE "성별"
		RETURN 牝性
	CASE "튜너"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4964(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,발동誘発)
PRINTL ①: 상대가 마법 카드의 효과를 발동했을 경우에, 자신 필드에 「전함 퀸 엘리자베스」가 존재한다면 발동할 수 있다.
PRINTL 묘지의 이 카드와 튜너 이외의 몬스터 1장을 덱으로 되돌리고, 그 효과를 무효로 한다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신 메인 페이즈에 자신 필드에 「벽람항로」 EX 몬스터가 존재할 경우에,
PRINTL 자신 필드의 이 카드명 이외의 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 패로 되돌리고, 묘지의 이 카드를 특수 소환한다.

@CARDCAN_4964(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4964)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,4964)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마법全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件튜너 = -1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,4964)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,4964,"묘지",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 벽람항로
	효과条件종류 = EXモン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4964(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4964,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,덱行き)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件튜너 = -1
			효과条件종류 = モン全般
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4964,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件固名前 = 경순양함 시라
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"손패")
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF