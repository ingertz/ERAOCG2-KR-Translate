@CARD_EXPLANATION_64911(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 묘지에서 「로열(AL)」 튜너 1장을 특수 소환한다.
PRINTL 자신 필드에 「전함 퀸 엘리자베스」가 존재하지 않을 경우, 대신에 덱에서 「로열(AL)」 튜너 1장을 특수 소환한다.
PRINTL 그 후, 자신 필드의 카드 1장을 덱으로 되돌린다.
CALL CARD_ABLE(0,효과後強制誘発)
PRINTL ②: 자신의 「로열(AL)」 튜너가 패 / 필드 / 묘지에서 패 / 덱으로 되돌아갈 때마다 발동한다.
PRINTL 턴 종료시까지 자신 필드의 「벽람항로」 몬스터의 공격력 / 수비력을 300 올린다.

@CARDCAN_64911(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,64911)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,64911)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT != 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 로열(AL)
		효과条件튜너 = 1
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 로열(AL)
		효과条件튜너 = 1
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 로열(AL)
	효과条件튜너 = 1
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"손패덱")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_64911(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カード星
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64911,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT != 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ELSE
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
		IF RESULT != 0
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲마함 = 1
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"덱")
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64911)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 효과後強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64911)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 벽람항로
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一攻",300)
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一守",300)
	ENDIF
ENDIF