RETURN 0


@CARD_EXPLANATION_39789(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 서로의 필드에 존재하는 드래곤족 땅속성 몬스터 1장(이 카드와 동명 이외)과 자신에게 700 데미지
CALL CARD_ABLE(3,EX덱起動)
PRINTL 이 카드는 1턴에 1번, 상기의 소재를 사용해 수비 표시로 X 소환할 수 있다.
PRINTL 「드래곤메이드 에르데」를 소재로 X 소환했을 경우, 이 카드는 각성 상태가 된다.
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번 밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時)
PRINTL ①：자신 필드에 이 카드 이외의『드래곤메이드』몬스터가 존재해며, 이 카드가 X 소환될 경우 발동할 수 있다.
PRINTL 자신은 1장 드로우한다.
CALL CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ②：자신 / 상대 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드의 X 소재를 1개 늘려, 덱에서 레벨 4 이하의 드래곤족 몬스터 1장을 특수 소환한다.
PRINTL 이 카드가 각성 상태일 경우, 레벨 5 이상의 몬스터를 특수 소환할 수 있다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39789(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39789)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF 발동済_3(듀얼리스트,39789)
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,39789)
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") <= 700
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	SIF モンスター数(듀얼리스트) < 5
		효과範囲상대필드 = 1
	효과条件종족 = 드래곤족
	효과条件속성 = 땅속성
	효과条件固名前 = 루 드래곤메이드『너서리』
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,39789)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 드래곤메이드
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,39789)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 드래곤메이드
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		CALL 효과条件レベル_ADV(4,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_39789(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 듀얼元ライフ
#DIM DYNAMIC 소환素材
#DIM DYNAMIC 소환素材レベル
#DIMS DYNAMIC 소환素材名前
#DIMS DYNAMIC 대상者
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 対面カード使用
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39789,3)
	IF RESULT == 0
		;소재를 選択
		CALL FULLRESET
		효과範囲필드 = 1
		SIF モンスター数(듀얼리스트) < 5
			효과範囲상대필드 = 1
		효과条件종족 = 드래곤족
		효과条件속성 = 땅속성
		효과条件固名前 = 루 드래곤메이드『너서리』
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			소환素材 = RESULT:1
			対面カード使用 = RESULT:10
			TRYCALLFORM CARD_{소환素材},"レベル"
			소환素材レベル = RESULT
			;ライフコスト
			CALL LIFE_CHANGE(듀얼리스트,"減少","효과",800)
			;フレ-バ-
			IF 듀얼리스트 == "자신"
				IF 소환素材 == 2059 && 対面カード使用 == 0 || 소환素材 == 12059 && 対面カード使用 == 0
					PRINTFORML
					SETCOLOR C_PINK
					PRINTFORMW 너서리가 음란한 옷차림으로 나타났다.
					RESETCOLOR
					CALL CARD_EROTICS(소환素材)
					IF RESULT != -1
						CALL EROTIC_MOVE("노출単発",RESULT,-1,소환素材レベル*100)
						WAIT
					ENDIF
				ELSE
					TRYCALLFORM CARDNAME_{소환素材},"名前"
					소환素材名前 = %RESULTS%
					PRINTFORML
					PRINTFORML %CALLNAME:MASTER%는 %소환素材名前%덮어 눌렀다!
					PRINTFORMW 저항하는%소환素材名前%를 구속한 채로, 조교를 시작한다!
					CALL CARD_EROTICS(소환素材)
					조교대상 = RESULT
					IF 조교대상 != -1
						FOR カウンタ,0,소환素材レベル
							CALL EROTIC_MOVE("全身凌辱",조교대상,MASTER,100)
						NEXT
					ENDIF
					PRINTFORML
					PRINTFORMW ………
					PRINTFORMW %소환素材名前%는 %CALLNAME:MASTER%에게 굴복하여, 음란한 의상을 입은 메이드가 되었다!
					PRINTFORML 
				ENDIF
			ENDIF
			;특수소환
			CALL FULLRESET
			효과추가종류:0 = 効追Ｘ素
			;SIF 소환素材レベル >= 7
			SIF 소환素材 == 12059
				효과추가종류:1 = 効追条達
			SIF !GROUPMATCH(소환素材, 2059, 12059)
				효과추가종류:2 = 効追変化ID + 소환素材
			CALL CARD_EX_SUMMON(듀얼리스트,ゾーン,場所,"守備표시")
			リザルト：XYZ소환 = 1
		ENDIF
	ENDIF
ELSEIF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39789,1)
	IF RESULT == 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39789,2)
	IF RESULT == 0
		IF 듀얼리스트 == "자신"
			소환素材 = GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所")
			IF 소환素材 == -1
				PRINTFORML
				SETCOLOR C_PINK
				PRINTFORMW 너서리는%NAME:MASTER%으로 부터 정기를 받았다!
				RESETCOLOR
				PRINTFORML
				CALL EROTIC_MOVE("펠라치오",NO_TO_CHARANUM(2059),MASTER,100)
			ELSE
				CALL CARD_EROTICS(소환素材)
				조교대상 = RESULT
				IF 조교대상 != -1
					PRINTFORML
					SETCOLOR C_PINK
					PRINTFORMW 루 드래곤메이드  %CALLNAME:조교대상%는 %NAME:MASTER%으로 부터 정기를 받았다!
					RESETCOLOR
					PRINTFORML
					CALL EROTIC_MOVE("펠라치오",조교대상,MASTER,100)
				ELSE
					TRYCALLFORM CARDNAME_{소환素材},"略称"
					PRINTFORML
					SETCOLOR C_PINK
					PRINTFORMW 루 드래곤메이드 %RESULTS%는 %NAME:MASTER%으로 부터 정기를 받았다!
					RESETCOLOR
				ENDIF
			ENDIF
		ENDIF
		
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 드래곤메이드
		SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
			CALL 효과条件レベル_ADV(4,"以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

