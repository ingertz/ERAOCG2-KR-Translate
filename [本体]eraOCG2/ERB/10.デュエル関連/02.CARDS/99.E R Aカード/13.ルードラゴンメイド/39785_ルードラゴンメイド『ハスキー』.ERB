@CARDNAME_39785,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 루 드래곤메이드『하스키』
	CASE "略称"
		RESULTS = 『하스키』
	CASE "カテゴリ"
		RESULTS:0 = 드래곤메이드
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39785(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		RETURN 2055
ENDSELECT
RETURN 0


