@CARDNAME_39610,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 환혹의 진자
	CASE "略称"
		RESULTS = 환혹의 진자
	CASE "カテゴリ"
		RESULTS:0 = 환혹
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_39610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39610(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  효과 ①②는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,발동誘発)
PRINTL ①：상대 필드의 암컷 몬스터가 효과를 발동했을 때에 발동할 수 있다.
PRINTL 　이 카드를 패에서 묘지로 보내고, 그 효과의 발동을 무효로 한다.
PRINTL 　그 후, 다음 자신의 턴 종료시까지, 무효로 한 몬스터의 컨트롤을 얻는다.
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②：이 카드가 묘지에 존재하는 상태에서, 레벨 1 / 빛 속성 몬스터가 묘지에 보내졌을 경우에 발동할 수 있다.
;PRINTL 
PRINTL 　이 카드를 패에 넣는다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_39610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39610)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,39610)
		RETURN 0
	SIF ゾーン != "손패"  
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,39610)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0

	CALL FULLRESET
	효과条件속성 = 빛속성
	효과条件レベル = 1
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"묘지送り全般+H")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF



@CARDEFFECT_39610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39610,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,효과묘지行き)
		CALL FULLRESET
		효과추가종류 = 6
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"強奪")
		IF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",コントロール先,"接可",2,1)
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",コントロール先)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML %CALLNAME:조교대상%가 기술을 발동하려고 한 순간, 갑자기 나타난 허접한 진자의 움직임에 눈이 가기 시작한다.
				PRINTFORMW 그것을 성가시다는 듯이 손으로 쳐낸 후, %CALLNAME:조교대상%는 한숨을 내쉬며 저쪽 필드로 나아가기 시작했다.
				PRINTFORMW 그런 허접한 최면에 현혹되지 않아, 라고 말하려는 듯이,
				PRINTFORMW %CALLNAME:조교대상%는、무기를 현재의 상대에게 겨누었다.
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39610,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,손패行き)
	ENDIF
ENDIF
