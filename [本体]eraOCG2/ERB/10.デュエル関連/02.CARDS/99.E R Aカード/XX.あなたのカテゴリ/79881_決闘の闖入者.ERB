@CARDNAME_79881,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 듀얼의 침입자－듀얼 인트루더
	CASE "略称"
RESULTS = 듀얼의 침입자
	CASE "カテゴリ"
RESULTS:0 = 당신
RESULTS:1 = E.R.A
	CASE "生産불가"
		RETURN 1
	CASE "추가日"
		RETURN 250507
ENDSELECT


@CARD_79881(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79881(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTFORML ①：자신 필드에 몬스터가 존재하지 않는 경우에 발동할 수 있다。
PRINTFORML  덱외에서「듀얼리스트 %CALLNAME:MASTER%」１장을 공격표시로 생성한다。
IF TALENT:MASTER:매지컬자지 == 1
	PRINTFORML 그 후、덱외에서「매지컬 자지」를 １장 패에 생성한다。
ELSE
	SETCOLOR C_GRAY
	PRINTFORML %CALLNAME:MASTER%가 매지컬자지를 갖고있을 때、진정한 효과가 나타난다…
	RESETCOLOR
ENDIF
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79881(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79881)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0	
	RETURN 1
ENDIF

@CARDEFFECT_79881(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79881)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_GENERATE(듀얼리스트,9990,"攻撃표시")
		PRINTFORMW %CALLNAME:MASTER%의 엔트리다！
	ENDIF
	IF TALENT:MASTER:매지컬자지 == 1
		CALL CARD_GENERATE_HAND(듀얼리스트,79882)
		PRINTFORMW %CALLNAME:MASTER%의 손에 매지컬 자지가 내려왔다…
	ENDIF
ENDIF