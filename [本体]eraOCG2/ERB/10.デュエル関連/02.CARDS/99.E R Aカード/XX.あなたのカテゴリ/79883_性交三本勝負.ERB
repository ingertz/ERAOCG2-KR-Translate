@CARDNAME_79883,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 섹스 매치 배틀
	CASE "略称"
RESULTS = 섹스매치배틀
	CASE "カテゴリ"
RESULTS:0 = 당신
RESULTS:1 = E.R.A
	CASE "生産불가"
		RETURN 1
	CASE "추가日"
		RETURN 250507
ENDSELECT


