VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 긴급 기획 마법 발동할 때마다 강제 전라 스쿼트! ~이머전시 프로젝트 매지컬 엑서사이즈~
	CASE "略称"
RESULTS = 마법 유발 헬스
	CASE "カテゴリ"
RESULTS:0 = 당신
		RESULTS:1 = E.R.A
		;RESULTS:2 = アライブツイン
		;アライブツインとは関係ないです！！！！
		;でもそれはそれとして違法サイトで筋トレ動画をうｐします！！！！
		RESULTS:3 = MC
	CASE "生産不可"
		RETURN 1
	CASE "추가日"
		RETURN 250607
ENDSELECT


@CARD_79884(参照先)

