ENDSELECT


@CARD_79604(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79604(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①: 상대 필드의 마법 / 함정 카드 1장을 파괴한다.
PRINTL 　그 후, 필드에 공격 표시의 암컷 몬스터가 존재할 경우,
PRINTL 　필드에 존재하는 공격 표시 암컷 몬스터 1장을 수비 표시로 한다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79604(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79604)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF

	SIF 마함数(対面者) == 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79604(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 조교대상

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79604)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함単体",場所,"묘지")

		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件성별 = 牝性
		효과条件표시 = 攻撃표시
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"表",3,1)
			CALL CARD_EROTICS_FIELD(대상者（CS）,参照先（CS）,選択カード（CS）)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML %CALLNAME:조교대상%는 얼굴을 붉히며 떠오른 스커트를 누르고,
				PRINTFORML 비열한 마법 카드를 발동한 플레이어를 노려보았다.
				CALL EROTIC_MOVE("팬티晒し",조교대상,-1,50)
			ENDIF
		ENDIF
	ENDIF
ENDIF