@CARDNAME_59892,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 유혹의 흑수 바이콘
	CASE "略称"
RESULTS = 유혹의 흑수
	CASE "カテゴリ"
RESULTS:0 = 없음
RESULTS:1 = E.R.A
ENDSELECT


@CARD_59892(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 2200
	CASE "성별"
		RETURN ふたなり
	CASE "추가日"
		RETURN 250401
ENDSELECT


RETURN 0


