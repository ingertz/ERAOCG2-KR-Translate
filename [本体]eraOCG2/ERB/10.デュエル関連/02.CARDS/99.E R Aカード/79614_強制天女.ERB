@CARDNAME_79614,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 강제천녀
	CASE "略称"
RESULTS = 강제천녀
	CASE "カテゴリ"
RESULTS:0 = 없음
RESULTS:1 = E.R.A
ENDSELECT


@CARD_79614(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79614(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：필드의 암컷 이외의 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 　그 카드의 성별을 암컷으로 한다. 그 몬스터는 이 턴에 공격할 수 없다.

CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79614(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79614)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件성별 = 雌以外
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79614(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 攻撃力상승値
#DIM DYNAMIC 조교대상
#DIM DYNAMIC レイプ횟수
#DIM DYNAMIC 寝取り成功
#DIM DYNAMIC 元持ち主
#DIM DYNAMIC 절정횟수
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79614)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별 = 雌以外
		효과추가종류 = 96
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드単体",場所,"戦無効")
		IF RESULT


			TRYCALLFORM CARDNAME_{他参照選択カードID}, "名前"
			PRINTFORML 아무런 예고도 없이, %RESULTS%는 여자의 몸이 되어 영문도 모르겠다는 듯이 얼굴을 붉히고,
			PRINTFORMW 몸 깊은 곳에서 솟아오르는 감정대로, 그 지체를 감추려 했다.
			選択カード = 他参照選択カードID
			CALL CARD_EROTICS(選択カード)
			IF RESULT != -1
				조교대상 = RESULT
				CALL EROTIC_MOVE("晒しもの",조교대상,MASTER,200)
			ENDIF
		ENDIF
	ENDIF
ENDIF


