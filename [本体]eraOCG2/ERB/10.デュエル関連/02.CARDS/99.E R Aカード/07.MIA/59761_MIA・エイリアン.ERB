PRINTL 무덤의 이 카드를 EX덱으로 되돌리고, 자신은 300 데미지를 받으며,
PRINTL 덱에서 기계족·어둠 속성 몬스터 1장을 패에 넣는다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_59761(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59761)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,59761)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대묘지 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_2(듀얼리스트,59761)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 어둠속성
	효과条件종족 = 기계족
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59761(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 無効カード
#DIM DYNAMIC 설정타입
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59761,1,X消費1)
	IF RESULT == 0
		; FOR カウンタ,0,3
		; 	CALL FULLRESET
		; 	효과条件任意 = 1
		;     CALL CARD_ADD_GY_BASIC(듀얼리스트,"상대묘지",ゾーン,場所)
		; NEXT
		CALL FULLRESET
		효과条件複数 = 97
		효과条件行先플래그 = 完全제외行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"상대묘지",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과範囲상대제외 = 1
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT >= 3
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"条達",3,上書きモード)

ELSEIF 종류 == 攻撃時強制誘発
	CALL FULLRESET
	無効カード = 戦闘場所(듀얼리스트,1)
	CALL FULLRESET
	효과추가종류 = 23
	CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",無効カード,"지속無効",99999)

ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59761,2)
	IF RESULT == 0
        CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,덱行き)
		CALL SHUFFLE(듀얼리스트)
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",300)
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件종족 = 기계족
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
    ENDIF
ELSEIF 종류 == リアクション효과
	PRINTFORML 
	PRINTFORML MIA 에일리언이 무기질적인 구동음을 울리면서、파워유닛에서 에너지를 흡수하고있다…
	PRINTFORML 
	FOR カウンタ,0,紙束最大値
		VARSET RESULT
		SIF GETVAR(@"%対面者%_제외:カウンタ") != 39600
			CONTINUE
		CALL FULLRESET
		조교대상 = GETVAR(@"%対面者%_除_変化元ID:カウンタ")
		CALL CARD_EROTICS(조교대상)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML
			PRINTFORML 에너지 추출 장치에 삼켜진 %NAME:조교대상%는 고통과 음락이 섞인 목소리를 흘리고있다！
			CALL EROTIC_MOVE("생체파츠",조교대상,-1,200)
		ENDIF
		PRINTFORML
	NEXT
ENDIF