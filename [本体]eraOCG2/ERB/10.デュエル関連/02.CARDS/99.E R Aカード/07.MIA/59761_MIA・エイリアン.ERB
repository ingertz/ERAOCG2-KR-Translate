@CARDNAME_59761,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = MIA 에일리언
	CASE "略称"
		RESULTS = MIA에일리언
	CASE "カテゴリ"
		RESULTS:0 = MIA
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59761(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 1800
ENDSELECT
RETURN 0

@CARDSUMMON_59761(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 4
素材条件속성 = 어둠속성
素材条件종족 = 기계족


IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,59761) 
	RETURN RESULT
ENDIF



