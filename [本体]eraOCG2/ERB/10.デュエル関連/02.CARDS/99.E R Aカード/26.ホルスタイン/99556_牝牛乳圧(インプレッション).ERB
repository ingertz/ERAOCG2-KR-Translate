@CARD_EXPLANATION_99556(종류)
#DIM DYNAMIC 종류
#DIMS DYNAMIC 듀얼리스트

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ②의 효과는 １턴에 １번밖에 발동할 수 없다。
CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTL ①：자신의 마법＆함정 존에 "호루스타인" 카드의 효과에 의해 놓여진 카드가 존재하는 경우、배틀 페이즈 / 자신 메인 페이즈에 발동할 수 있다。
PRINTL "호루스타인" 카드 이외의、자신 필드의 앞면 표시인 카드를 묘지로 보낸다。
PRINTL 그 후、선택한 카드가 암컷몬스터였을 경우、이하의 효과를 적용할 수 있다。
PRINTL ●：패 / 묘지 / 덱 / EX덱 / 상대의 묘지에서、묘지로 보내진 카드와 같은 카드를 지속마법카드 취급으로 자신의 마법＆함정 존에 앞면표시로 두어
PRINTL 　　이 듀얼중、아직 발동하지 않은 "호루스타인" 통상마법을 패 / 덱에서 제외하고、그 효과를 전부 적용한다。
PRINTL 자신의 마법＆함정 존에 세트된 카드가 존재하지 않을 경우、이 카드는 패에서 버리고 발동할 수 있으며
PRINTL １턴에 １번、자신의 마법＆함정 존에 카드가 ２장 이상 존재하는 경우、상대 필드의 카드도 선택할 수 있다。
CALLFORM CARD_ABLE(1,묘지起動)
PRINTL ②：자신 메인 페이즈에 발동할 수 있다。
PRINTL 묘지의 이 카드를 제외하고、묘지의 "호루스타인" 카드를 패에 넣는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 마함数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99556)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 
	SIF !(メインフェイズ == 1 && ターンプレイヤー == 듀얼리스트) && バトルフェイズ != 1
		RETURN 0
	
	IF ゾーン == "손패"
		;セット카드가 あれば使えない
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件표시 = 裏側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT > 0
			RETURN 0
	ELSEIF ゾーン == "마함"
		;세트 한 턴에 は使えない
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	
	;자신 필드에 호루스타인 카드의 효과で置いた카드가 無ければ使えない。
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件カテゴリ:0 = 登録情報：호루스타인─미유
	효과条件カテゴリ:1 = 登録情報：호루스타인─애유
	효과条件カテゴリ:2 = 登録情報：호루스타인─지유
	효과条件カテゴリ:3 = 登録情報：호루스타인─EX유
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 0
		RETURN 0
	
	;필드에 "호루스타인" 이외의 表側の카드가 なければ使えない
	;마법함정존에 카드가 2枚上あれば상대의 카드も대상로할 수 있다。
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	마함数 = RESULT
	
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	IF 마함数 >= 2 && !발동済_1(듀얼리스트,99556)
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
	ENDIF
	효과条件표시 = 表側표시
	효과条件固カテゴリ:0 = 호루스타인
	효과条件固カテゴリ:1 = 登録情報：호루스타인─미유
	효과条件固カテゴリ:2 = 登録情報：호루스타인─애유
	효과条件固カテゴリ:3 = 登録情報：호루스타인─지유
	효과条件固カテゴリ:4 = 登録情報：호루스타인─EX유
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 0
		RETURN 0
	
	RETURN 1
ELSEIF 종류 == 묘지起動
	
	SIF 발동済_2(듀얼리스트,99556)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	
	RETURN 1
ENDIF

@CARDEFFECT_99556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 参照場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 마함数
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 攻め암소参照カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 암소대상
#DIMS DYNAMIC 암소名称
#DIM DYNAMIC 암소乳サイズ
#DIM DYNAMIC 被암소대상
#DIMS DYNAMIC 被암소名称
#DIM DYNAMIC 被암소乳サイズ
#DIMS DYNAMIC カテゴリ名称
#DIM DYNAMIC 미유チェック
#DIM DYNAMIC 애유チェック
#DIM DYNAMIC 지유チェック
#DIM DYNAMIC EX유チェック


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99556)
	IF RESULT == 0
		
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件カテゴリ:0 = 登録情報：호루스타인─미유
		효과条件カテゴリ:1 = 登録情報：호루스타인─애유
		효과条件カテゴリ:2 = 登録情報：호루스타인─지유
		효과条件カテゴリ:3 = 登録情報：호루스타인─EX유
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			;選択肢なし
			RESULT = 0
		ELSE
			PRINTFORML 誰が유압を掛けますか？
			PRINTFORML 
				
			미유チェック = -1
			애유チェック = -1
			지유チェック = -1
			EX유チェック = -1
			
			FOR カウンタ,0,5
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─미유"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179556)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【미유】%RESULTS:0%
					미유チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─애유"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179557)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【애유】%RESULTS:0%
					애유チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─지유"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179558)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【지유】%RESULTS:0%
					지유チェック = カウンタ
					CONTINUE
				ENDIF
				IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─EX유"
					CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,179559)
					암소名称 = %RESULTS:0%
					PRINTFORML [{カウンタ}] - 【EX유】%RESULTS:0%
					EX유チェック = カウンタ
					CONTINUE
				ENDIF
				
			NEXT
			
			PRINTFORML 
			PRINTFORML [9] - 맡긴다
			IF 듀얼리스트 == "자신"
				BINPUT
			ELSE
				RESULT = 9
			ENDIF
		ENDIF
		
		IF RESULT == 9
			SIF 미유チェック != -1
				RESULT = 미유チェック
			SIF 애유チェック != -1
				RESULT = 애유チェック
			SIF 지유チェック != -1
				RESULT = 지유チェック
			SIF EX유チェック != -1
				RESULT = EX유チェック
			
			;雑ランダム処理
			SIF 지유チェック != -1 && RAND:2 == 1
				RESULT = 지유チェック
			SIF 애유チェック != -1 && RAND:2 == 1
				RESULT = 애유チェック
			SIF 미유チェック != -1 && RAND:2 == 1
				RESULT = 미유チェック
		ENDIF
		
		FOR カウンタ,RESULT,5
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─미유"
				RESULT = カウンタ
				攻め암소参照カードID = 179556
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─애유"
				RESULT = カウンタ
				攻め암소参照カードID = 179557
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─지유"
				RESULT = カウンタ
				攻め암소参照カードID = 179558
				BREAK
			ENDIF
			IF GETVARS(@"%듀얼리스트%_魔_追カテ:カウンタ") == "登録情報：호루스타인─EX유"
				RESULT = カウンタ
				攻め암소参照カードID = 179559
				BREAK
			ENDIF
		NEXT
		
		;호루스타인情報の취득
		CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,攻め암소参照カードID)
		암소名称 = %RESULTS:0%
		参照場所 = RESULT:0
		암소대상 = RESULT:1
		암소乳サイズ = GETVAR(@"%듀얼리스트%_魔_個Ｃ:参照場所");마법함정の乳サイズ
		CALL HORUSTEIN_GET_BUST(RESULT:99,암소乳サイズ)
		암소乳サイズ = RESULT:2
		
		미유チェック = 0
		애유チェック = 0
		지유チェック = 0
		EX유チェック = 0
		
		;묘지送り
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		마함数 = RESULT
		
		효과範囲필드 = 1
		효과範囲마함 = 1
		IF 마함数 >= 2 && !발동済_1(듀얼리스트,99556)
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
		ENDIF
		효과条件표시 = 表側표시
		효과条件固カテゴリ:0 = 호루스타인
		효과条件固カテゴリ:1 = 登録情報：호루스타인─미유
		효과条件固カテゴリ:2 = 登録情報：호루스타인─애유
		효과条件固カテゴリ:3 = 登録情報：호루스타인─지유
		효과条件固カテゴリ:4 = 登録情報：호루스타인─EX유
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	
		IF RESULT != -1
			選択カードID = RESULT:1
			
			;상대의  카드를 選んだら、使用済みにする
			SIF 듀얼리스트 != 대상者（CS）
				CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99556,1)
			
			;호루스타인情報の취득(カードIDから名前を취득)
			CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,選択カードID)
			被암소名称 = %RESULTS:0%
			
			;今回は乳の大きさは마법함정を参照して出している。
			;MZの乳は「호루스타인" 이외의  카드의 효과で変動してれば値がずれてしまうが、面倒なので今回はふわっとした表現に
			;貧乳(-1)以下、巨乳(1)以下、巨乳より大きい
			
			PRINTFORM %암소名称%는 %被암소名称%의 위를 덮치니、
			IF 암소乳サイズ <= -1
				PRINTFORMW 귀여운 가슴으로 열심히 %被암소名称%를 짓눌렀다…
			ELSEIF 암소乳サイズ <= 1
				;애유以外は巨乳以上になる
				PRINTFORMW 커다란 가슴으로 %被암소名称%를 짓눌러버렸다…
			ELSE
				PRINTFORMW 무척이나 커다란 가슴으로 %被암소名称%를 짓눌러버렸다…
			ENDIF
			PRINTFORML
			
			;選択カードIDが자신의 덱 / EX덱 패에 존재하는 場合
			;かつ、마법함정존에 空きがある場合
			
			CALL FULLRESET
			효과範囲상대묘지 = 1
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과範囲덱 = 1
			효과範囲EX덱 = 1
			효과条件성별 = 牝性
			효과条件ID = 選択カードID
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT != 0 && 마함数(듀얼리스트) < 5
				;존재하는 
				
				FOR カウンタ,0,5
					;손패と덱에 발동していない〇〇登録があるか
					CALL FULLRESET
					효과範囲손패 = 1
					효과範囲덱 = 1
					효과条件ID = 79556 + カウンタ
					CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
					
					;발동可能ﾁｪｯｸと存在チェック
					IF RESULT != -1
						SELECTCASE 79556 + カウンタ
						CASE 79556
							미유チェック = 1
							SIF 발동済_D1(듀얼리스트,79556)
								미유チェック = -1
						CASE 79557
							애유チェック = 1
							SIF 발동済_D1(듀얼리스트,79557)
								애유チェック = -1
						CASE 79558
							지유チェック = 1
							SIF 발동済_D1(듀얼리스트,79558)
								지유チェック = -1
						CASE 79559
							EX유チェック = 1
							SIF 발동済_D1(듀얼리스트,79559)
								EX유チェック = -1
						ENDSELECT
					ENDIF
				NEXT
				
				IF 미유チェック == 1 || 애유チェック == 1 || 지유チェック == 1 || EX유チェック == 1
					
					;발동可能な登録카드가 ある
					PRINTFORML 
					PRINTFORML 패 / 덱 / EX덱에 암소로 등록가능한 카드가 존재합니다。등록하시겠습니까？
					PRINTFORML 
					IF  미유チェック == 1
						PRINTFORML [0] - 【미유】등록
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【미유】등록
						RESETCOLOR
					ENDIF
					IF  애유チェック == 1
						PRINTFORML [1] - 【애유】등록
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【애유】등록
						RESETCOLOR
					ENDIF
					IF  지유チェック == 1
						PRINTFORML [2] - 【지유】등록
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【지유】등록
						RESETCOLOR
					ENDIF
					IF  EX유チェック == 1
						PRINTFORML [3] - 【EX유】등록
					ELSE
						SETCOLOR C_GRAY
						PRINTFORML     - 【EX유】등록
						RESETCOLOR
