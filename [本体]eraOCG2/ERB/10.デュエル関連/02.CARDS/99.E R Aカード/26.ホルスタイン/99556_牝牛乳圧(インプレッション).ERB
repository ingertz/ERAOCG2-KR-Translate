				효과条件종류 = 通常마법
			SIF 지유チェック == 2
				효과条件종류 = 함정全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				효과範囲덱 = 1
				효과条件カテゴリ = 호루스타인
				SIF 미유チェック == 2
					효과条件ID = 79555
				SIF 애유チェック == 2
					효과条件종류 = 通常마법
				SIF 지유チェック == 2
					효과条件종류 = 함정全般
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ELSE
				PRINTFORML
				PRINTFORMW 덱에서 패에 넣어진 "호루스타인"가 없는것같다…
				PRINTFORML
			ENDIF
			
			;특수소환효과
			IF 미유チェック == 2 || 애유チェック == 2 || 지유チェック == 2
				;패에서 특수소환
				;選択時に【〇乳】호루스타인…と표시されるとｷﾞｮｯっとするので、패에서 探して특수 소환한다
				FOR カウンタ,0,現손패(듀얼리스트)
					SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 書換先カードID
						CONTINUE
					CALL CARD_SP_SUMMON(듀얼리스트,"손패",カウンタ)
					BREAK
				NEXT
				
				IF RESULT != 0
					IF 미유チェック == 2
						PRINTFORMW %被암소名称%의 아름다운 가슴이 탄력있게 흔들렸다…
						CALL HORUSTEIN_RAND_COW_TXT(1,0)
					ELSEIF 애유チェック == 2
						PRINTFORMW %被암소名称%의 귀여운 유두가 탱글하고 흔들렸다…
						CALL HORUSTEIN_RAND_COW_TXT(1,1)
					ELSEIF 지유チェック == 2
						PRINTFORMW %被암소名称%의 무의식적으로 빨고싶어지는 가슴은 촉촉해 요염해져있다…
						CALL HORUSTEIN_RAND_COW_TXT(1,2)
					ENDIF
				ENDIF
			ELSEIF EX유チェック == 2
				CALL FULLRESET
				효과範囲덱 = 1
				효과範囲묘지 = 1
				효과条件カテゴリ = 호루스타인
				효과条件종류 = モン全般
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				
				IF RESULT == 1
					;덱 / 묘지에서 "호루스타인" 몬스터를 특수 소환한다
					CALL FULLRESET
					효과範囲덱 = 1
					효과範囲묘지 = 1
					효과条件カテゴリ = 호루스타인
					CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
					
					IF RESULT != -1
						選択カードID = RESULT:1
						CALL HORUSTEIN_GET_DATA(듀얼리스트,カード소환先,選択カードID)
						;◆小さな가슴でも違和感のない描写にする
						SELECTCASE 選択カードID
						CASE 179556
							;미유
							PRINTFORMW %RESULTS:0%의 아름다운 가슴이 탄력있게 흔들렸다…
							CALL HORUSTEIN_RAND_COW_TXT(1,0)
						CASE 179557
							;애유
							PRINTFORMW %RESULTS:0%의 귀여운 유두가 탱글하고 흔들렸다…
							CALL HORUSTEIN_RAND_COW_TXT(1,1)
						CASE 179558
							;지유
							PRINTFORMW %RESULTS:0%의 무의식적으로 빨고싶어지는 가슴은 촉촉해 요염해져있다…
							CALL HORUSTEIN_RAND_COW_TXT(1,2)
						ENDSELECT
						PRINTFORMW 
					ENDIF
				ENDIF
			ENDIF
			
		ENDIF
		;テスト時はコメント化
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99556,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		
		CALL FULLRESET
		효과条件カテゴリ = 호루스타인
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",場所)
	ENDIF
ENDIF
