@CARDNAME_99611,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 비하인드 오나홀
	CASE "略称"
RESULTS = 뒷면의 고기구멍
	CASE "カテゴリ"
RESULTS:0 = 없음
RESULTS:2 = E.R.A
ENDSELECT


@CARD_99611(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99611(종류)
#DIM DYNAMIC 종류
CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：자신의 덱 / 묘지에서 “매지컬 오나홀” 1장을 선택해 자신 필드에 세트한다。
PRINTL 덱에서 세트한 경우, 해당 카드는 세트한 턴에도 발동할 수 있다。
PRINTL ②：자신이 “매지컬 오나홀”을 발동했을 때, 묘지의 이 카드를 제외하고 발동할 수 있다。
PRINTL 그 효과로 상대에게 주는 대미지는 두 배가 된다.
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_99611(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99611)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
효과条件名前 = 매지컬 오나홀
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_99611(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC 選択カード
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99611)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
효과条件名前 = 매지컬 오나홀
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"使化裏側표시")

	ENDIF
ENDIF
