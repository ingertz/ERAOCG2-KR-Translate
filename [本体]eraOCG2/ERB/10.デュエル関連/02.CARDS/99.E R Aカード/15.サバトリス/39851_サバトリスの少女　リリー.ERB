@CARDNAME_39851,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 사바트리스의 소녀 릴리
	CASE "略称"
		RESULTS = SB 소녀
	CASE "カテゴリ"
		RESULTS:0 = 사바트리스
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_39851(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "튜너"
		RETURN 1
	CASE "성별"
		RETURN 牝性	
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39851(종류)
#DIM DYNAMIC 종류


CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다。
CALL CARD_ABLE(1,필드起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다。
PRINTL 필드의 이 카드를 릴리스하고,
PRINTL 덱에서 ４이하의「사바트리스」 몬스터 1장을 특수 소환한다。
CALL CARD_ABLE(2,묘지起動)
PRINTL ②：이 카드가 묘지에 존재할 경우、패１장을 덱의 맨 아래으로 되돌리고 발동할수 있다。
PRINTL 이 카드를 특수 소환한다。
PRINTL 이 효과로 특수소환된 이 카드는 필드에서 벗어날 경우 제외된다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_39851(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39851)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,39851)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 사바트리스
	효과条件종류 = モン全般
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,39851)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39851,ゾーン)
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_39851(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39851,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 사바트리스
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39851,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT 
			CALL FULLRESET
			효과추가종류 = 2
			CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)
		ENDIF
	ENDIF
ENDIF
