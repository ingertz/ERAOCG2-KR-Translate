@CARDCAN_79634(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79634)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_2(듀얼리스트,79634)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대마함 = 1
	효과条件종류 = フィールド
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件성별 = 牝性
	효과条件표시 = 表側표시
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ELSEIF 종류 == 손패起動
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 발리언츠
		효과条件종류 = フィールド
		효과条件固名前 = 밸리언츠＝월드－육혈구벽
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 他효과起動
	RETURN 1
ENDIF

@CARDEFFECT_79634(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상
#DIM DYNAMIC レイプ횟수
#DIM DYNAMIC 寝取り成功
#DIM DYNAMIC 元持ち主
#DIM DYNAMIC 절정횟수
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79634,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과範囲상대마함 = 1
		효과条件성별 = 牝性
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT:1
		元持ち主 = GETVAR(@"%듀얼리스트%_MZ_所有:カード소환先")
		IF 듀얼리스트 == "자신"
			CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"接可",2,1)
			IF HAS_PENIS(MASTER)
				CALL CARD_EROTICS(選択カード)
					조교대상 = RESULT
				IF RESULT != -1
					SETCOLOR C_PINK
					PRINTFORML %CALLNAME:MASTER%는 하반신을 내민 자세로 벽에 구속되었다 %CALLNAME:조교대상%의
					PRINTFORML 배후에 서고、엉덩이를 잡고 마음껏 범했다。
					PRINTFORMW
					RESETCOLOR
					IF TALENT:MASTER:형태 == 0
						レイプ횟수 = 4
					ELSE
						レイプ횟수 = (TALENT:MASTER:형태) - 1
					ENDIF
					;PRINTFORML {レイプ횟수}
					FOR カウンタ,0,レイプ횟수
						CALL EROTIC_MOVE("前穴凌辱",조교대상,MASTER,200)
						;PRINTFORMW 절정횟수:{RESULT:1}		
						절정횟수 += RESULT:1
						SIF 절정횟수 >= 10
							寝取り成功 = 1
					NEXT
					SETCOLOR C_PINK
					IF 元持ち主 == 2
						IF 寝取り成功 == 1
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 자지에 몸도 마음도 떨어져、
							PRINTFORMW 직접 %CALLNAME:MASTER%의 말이 되어 싸우는것을 선택했다。
							CALL SET_FIELD_INFO(듀얼리스트,"필드",コントロール先,"所有",1,1)
							IF 選択カード < 100000
								IF 듀얼리스트 == "자신" && 元持ち主 == 2
									CALL CARD_GET_SINGLE(選択カード,1)
									PRINTFORML
								ENDIF
							ENDIF
						ELSE
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 자지에 굴복하여、
							PRINTFORMW %CALLNAME:MASTER%의 군문에 들어왔다。
						ENDIF
					ELSE
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 총애를 듬뿍 받아、
						PRINTFORMW 허벅지에서 정액을 흘리면서 출격하러갔다。
					ENDIF
					RESETCOLOR
				ENDIF
			ENDIF
		ENDIF

		CALL FULLRESET
		효과条件カテゴリ = 발리언츠
		효과条件종류 = フィールド
		효과条件固名前 = 밸리언츠＝월드－육혈구벽
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대마함",ゾーン,場所)
		選択カード = RESULT
		IF RESULT != 9999
			選択カード_ID = GETVAR(@"%対面者%_마함:選択カード")
			CALLFORM CARD_STANBY(選択カード_ID,듀얼리스트,他효과起動,"마함",場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79634)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과条件종류 = フィールド
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함全体",場所,"묘지")
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 발리언츠
		효과条件종류 = フィールド
		효과条件固名前 = 밸리언츠＝월드－육혈구벽
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시",2)

		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件성별 = 牝性
		효과条件任意 = 1
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시",1)
		조교대상 = RESULT:1
		CALL CARD_EROTICS(조교대상)
		IF RESULT != -1
			조교대상 = RESULT
				SETCOLOR C_PINK
			PRINTFORML
			PRINTFORMW %CALLNAME:조교대상%은 갑자기 나타난 벽에 파묻혀버렸다！
			PRINTFORML
				RESETCOLOR
			CALL EROTIC_MOVE("拘束晒し",조교대상,-1,100)
		ENDIF
	ENDIF
ELSEIF 종류 == 他효과起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79634)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과範囲상대마함 = 1
		효과条件성별 = 牝性
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT:1
		元持ち主 = GETVAR(@"%듀얼리스트%_MZ_所有:カード소환先")
		IF 듀얼리스트 == "자신"
			CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"接可",2,1)
			IF HAS_PENIS(MASTER)
				CALL CARD_EROTICS(選択カード)
				IF RESULT != -1
					조교대상 = RESULT
					SETCOLOR C_PINK
					PRINTFORML %CALLNAME:MASTER%는 하반신을 내민 자세로 벽에 묻힌 %CALLNAME:조교대상%의
					PRINTFORML 배후에 서고、엉덩이를 잡고 마음껏 범했다
					PRINTFORMW
					RESETCOLOR
					IF TALENT:MASTER:형태 == 0
						レイプ횟수 = 4
					ELSE
						レイプ횟수 = (TALENT:MASTER:형태) - 1
					ENDIF
					;PRINTFORML {レイプ횟수}
					FOR カウンタ,0,レイプ횟수
						CALL EROTIC_MOVE("前穴凌辱",조교대상,MASTER,200)
						;PRINTFORMW 절정횟수:{RESULT:1}		
						절정횟수 += RESULT:1
						SIF 절정횟수 >= 10
							寝取り成功 = 1
					NEXT
					SETCOLOR C_PINK
					IF 元持ち主 == 2
						IF 寝取り成功 == 1
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 자지에 몸도 마음도 떨어져、
							PRINTFORMW 직접 %CALLNAME:MASTER%의 말이 되어 싸우는것을 선택했다。
							CALL SET_FIELD_INFO(듀얼리스트,"필드",コントロール先,"所有",1,1)
							IF 選択カード < 100000
								IF 듀얼리스트 == "자신" && 元持ち主 == 2
									CALL CARD_GET_SINGLE(選択カード,1)
									PRINTFORML
								ENDIF
							ENDIF
						ELSE
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 자지에 굴복하여、
							PRINTFORMW %CALLNAME:MASTER%의 군문에 들어왔다。
						ENDIF
					ELSE
						PRINTFORML
						PRINTFORML %CALLNAME:조교대상%는 %CALLNAME:MASTER%의 총애를 듬뿍 받아、
						PRINTFORMW 허벅지에서 정액을 흘리면서 출격하러갔다。
					ENDIF
					RESETCOLOR
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "VV－육혈구벽")
ENDIF
