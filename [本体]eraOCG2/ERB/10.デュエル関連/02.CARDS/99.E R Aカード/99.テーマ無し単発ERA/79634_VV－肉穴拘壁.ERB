@CARDNAME_79634,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 밸리언츠＝월드－육혈구벽
	CASE "略称"
RESULTS = VV－육혈구벽
	CASE "カテゴリ"
RESULTS:0 = 밸리언츠
RESULTS:1 = E.R.A
ENDSELECT


@CARD_79634(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
ENDSELECT

RETURN 0

