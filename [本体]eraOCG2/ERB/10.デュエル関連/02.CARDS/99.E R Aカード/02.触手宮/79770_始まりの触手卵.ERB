@CARDNAME_79770,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 시작의 촉수 알
	CASE "略称"
		RESULTS = 시작의 촉수 알
	CASE "カテゴリ"
		RESULTS:0 = 촉수
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79770(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79770(종류)
#DIM DYNAMIC 종류


CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ①②효과는 각각 1턴에 1번 밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패를 1장 버리고 발동할 수 있다。
PRINTL 자신의 덱에서 레벨 4이하의 "촉수" 몬스터 1장을 특수 소환한다。
CALL CARD_ABLE(2,묘지起動)
PRINTL ②：묘지의 이 카드를 제외하고 발동할 수 있다。
PRINTL 묘지의 "촉수」 카드 3장을 덱으로 되돌리고 셔플한다。
PRINTL 그 후, 상대의 여성 몬스터 1장을 「묘상」 카드로 취급하여 자신의 마법 / 함정 존에 둔다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79770(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79770)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	;효과①발동済みなら침묵
	SIF 발동済_1(듀얼리스트,79770)
		RETURN 0
	;コントロ-ラ-のターンじゃないなら침묵
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	;メインフェイズじゃないなら침묵
	SIF メインフェイズ != 1
		RETURN 0
	;특수 소환할 수 없다なら침묵
	CALL CARD_SS_CHECK(듀얼리스트,79770,ゾーン)
	SIF RESULT == 1
		RETURN 0
	;손패1枚では발동할 수 없다
	IF 종류 == 마함起動
		SIF 손패数(듀얼리스트) == 0
			RETURN 0
	ELSEIF 종류 == 손패起動
		SIF 손패数(듀얼리스트) <= 1
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 촉수
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	;それ以外ならOK
	RETURN 1
ELSEIF 종류 == 묘지起動
	;효과②발동済みなら침묵
	SIF 발동済_2(듀얼리스트,79770)
		RETURN 0
	;コントロ-ラ-のターンじゃないなら침묵
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	;メインフェイズじゃないなら침묵
	SIF メインフェイズ != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	;묘지에 촉수が３장ないなら침묵
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 촉수
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 4
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79770(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 元参照先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79770,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			;덱에서 レベル4以下슬라임をサーチ
			효과範囲덱 = 1
			효과条件カテゴリ = 촉수
			CALL 효과条件レベル_ADV(4, "以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79770,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;묘지의 촉수を選択、덱에 Ｘ３
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 촉수
		효과条件複数 = 3
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		
		;상대 필드의 카드 １장을 묘상に
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件성별 = 牝性 
		효과추가カテ = 묘상
		효과条件任意 = 1
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시",79770)

		;フレ-バ-
		조교대상 = RESULT:1
		CALL CARD_EROTICS(조교대상)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML 
			PRINTFORML 사망 했다고 생각한 촉수들의 시체가 갑자기 움직여、
			PRINTFORML %CALLNAME:조교대상%을 기습해 묘상으로 만들어 버렸다。
			PRINTFORML 
			CALL EROTIC_MOVE("疑似출산",조교대상)
		ENDIF
	ENDIF
ENDIF
