@CARDNAME_39714,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = A섬도예희－로제
	CASE "略称"
		RESULTS = A섬도예희－로제
	CASE "カテゴリ"
		RESULTS:0 = A섬도희
		RESULTS:1 = A섬도
		RESULTS:2 = E.R.A
		RESULTS:3 = 섬도
		RESULTS:4 = 섬도희
ENDSELECT


@CARD_39714(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		RETURN 2001
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39714(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
PRINTL 이 카드는 「A섬도희」 카드로도 취급된다。
CALL CARD_ABLE(1,효과後誘発)
PRINTL ①：이 카드가 묘지에 있는 상태에서 자신의、자신의 "A섬도희」 카드가 파괴될 경우 발동할 수 있다。
PRINTL 이 카드를 묘지에서 특수 소환한다。
PRINTL 그 후, 묘지에서「A섬도희」 카드 1장을 패에 넣을 수 있다。
CALL CARD_ABLE(2,필드起動)
PRINTL ②：자신의 메인 페이즈에 발동할 수 있다。
PRINTL 상대 필드의 몬스터 1체의 공격력을 1500 감소시킨다。
PRINTL 그 후, 자신의 필드에 "A섬도예희－로제 클론」 토큰（레벨 ４ 어둠속성 기계족 공／수１０００） １장을 특수 소환한다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_39714(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39714)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,39714)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39713,ゾーン)
	SIF RESULT == 1
		RETURN 0

		
	CALL FULLRESET
	효과条件カテゴリ = A섬도희
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,39714)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,139714,"")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF


@CARDEFFECT_39714(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 필드単体

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39714,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン, 場所)
		CALL FULLRESET
		효과条件カテゴリ = A섬도
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ENDIF
	
IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39714,2)
	IF RESULT == 0	
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"永攻DOWN",1500)
		IF RESULT == 1
			CALL CARD_EROTICS(他参照選択カードID)
			IF RESULT != -1
				조교대상 = RESULT
				IF TALENT:조교대상:성별 == 1
					PRINTFORMW 클론 제조 노예로 조교된 로제는 무표정한 얼굴로 %CALLNAME:조교대상%를 밀어 넘어뜨렸다。
					PRINTFORMW 그대로 겹쳐진 질구에서 에너지를 빨아들여, 주인인 아자레아를 위한 새로운 병사를 탄생시켰다。
					CALL EROTIC_MOVE("레즈疑似출산",NO_TO_CHARANUM(2001),조교대상,200)
				ELSEIF TALENT:조교대상:성별 >= 2
					PRINTFORMW 클론 제조 노예로 조교된 로제는 무표정한 얼굴로 %CALLNAME:조교대상%를 밀어 넘어뜨렸다。
					PRINTFORMW 그대로 자신의 질구에 겹쳐진 육봉에서 에너지를 빨아들여, 주인인 아자레아를 위한 새로운 병사를 탄생시켰다。
					CALL EROTIC_MOVE("疑似출산",NO_TO_CHARANUM(2001),조교대상,200)
				ELSE
					PRINTFORMW 클론 제조 노예로 조교된 로제는 무표정한 얼굴로 %CALLNAME:조교대상%를 밀어 넘어뜨렸다。
					PRINTFORMW 그대로 겹쳐진 질구에서 에너지를 빨아들여, 주인인 아자레아를 위한 새로운 병사를 탄생시켰다。
					CALL EROTIC_MOVE("疑似출산",NO_TO_CHARANUM(2001),조교대상,200)
				ENDIF
			ENDIF
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,139714)
		ENDIF
	ENDIF
ENDIF



