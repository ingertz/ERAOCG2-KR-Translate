@CARDNAME_79775,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 드래그마라 사바트
	CASE "略称"
		RESULTS = 음교의 야연
	CASE "カテゴリ"
		RESULTS:0 = 드래그마라
		RESULTS:1 = 드래그마
		RESULTS:2 = 의식마법
		RESULTS:3 = E.R.A
ENDSELECT


@CARD_79775(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79775(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL “드래그마라” 의식 몬스터의 강림에 필요.
PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：자신 / 상대 필드에서 암컷 몬스터를 릴리스하고
PRINTL 릴리스한 수× 4와 같은 레벨이 되는 ‘드래그마라’ 의식 몬스터 1장을 자신의 패 / 묘지에서 의식 소환한다.
CALL CARD_ABLE(2,ターン終了時)
PRINTL ②：상대의 엔드 페이즈에 발동할 수 있다.
PRINTL 묘지에서 암컷 몬스터 1장을 덱의 맨 아래로 되돌리고, 묘지에 있는 이 카드를 패에 추가한다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79775(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 素材候補数

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79775)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79775)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件성별 = 牝性
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	素材候補数 = RESULT
	SIF 素材候補数 == 0
		RETURN 0
	CALL FULLRESET
	IF 素材候補数 == 1
		효과条件レベル:0 = 4
	ELSEIF 素材候補数 == 2
		효과条件レベル:0 = 4
		효과条件レベル:1 = 8
	ELSEIF 素材候補数 >= 3
		효과条件レベル:0 = 4
		효과条件レベル:1 = 8
		효과条件レベル:2 = 12
	ENDIF
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件종류 = 의식
	효과条件カテゴリ = 드래그마라
	효과条件소환可 = 2
	효과条件特不無視 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_2(듀얼리스트,79775)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79775(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 出所
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 素材候補数
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79775,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件종류 = モン全般
		효과条件성별 = 牝性
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		素材候補数 = RESULT
		CALL FULLRESET
		IF 素材候補数 == 0
			PRINTL 素材が足りません
			RETURN 0
		ELSEIF 素材候補数 == 1
			효과条件レベル:0 = 4
		ELSEIF 素材候補数 == 2
			효과条件レベル:0 = 4
			효과条件レベル:1 = 8
		ELSEIF 素材候補数 >= 3
			효과条件レベル:0 = 4
			효과条件レベル:1 = 8
			효과条件レベル:2 = 12
		ENDIF
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件종류 = 의식
		효과条件カテゴリ = 드래그마라
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		SIF 選択カード == 9999
			RETURN 0

		CALL CARD_SELECT(듀얼리스트,選択カード)
		選択カード = RESULT
		出所 = %参照先（CS）%
		IF GETVAR(@"%듀얼리스트%_%出所%:{選択カード}") == 39770
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲상대필드 = 1
			효과条件성별 = 牝性
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			SIF RESULT != 1
				RETURN 0
			CALL CARD_EROTICS(RESULT:1)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML
				PRINTFORML
				SETCOLOR C_PINK
				PRINTFORML %CALLNAME:조교대상%는 사바트에 휘말렸다…！
				RESETCOLOR
				PRINTFORML
				CALL EROTIC_MOVE("全身凌辱",조교대상,-1,500)
			ENDIF
			TRYCALLFORM RITSUMMON(듀얼리스트, 選択カード,出所,1)
		ELSEIF GETVAR(@"%듀얼리스트%_%出所%:{選択カード}") == 39771
			FOR カウンタ,0,2
				CALL FULLRESET
				효과範囲필드 = 1
				효과範囲상대필드 = 1
				효과条件성별 = 牝性
				CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				SIF RESULT != 1
					RETURN 0
				CALL CARD_EROTICS(RESULT:1)
				IF RESULT != -1
					조교대상 = RESULT
					PRINTFORML
					PRINTFORML
					SETCOLOR C_PINK
					PRINTFORML %CALLNAME:조교대상%는 사바트에 휘말렸다…！
					RESETCOLOR
					PRINTFORML
					CALL EROTIC_MOVE("全身凌辱",조교대상,-1,500)
				ENDIF
			NEXT
			TRYCALLFORM RITSUMMON(듀얼리스트, 選択カード,出所,1)
		ELSEIF GETVAR(@"%듀얼리스트%_%出所%:{選択カード}") == 39772
			FOR カウンタ,0,3
				CALL FULLRESET
				효과範囲필드 = 1
				효과範囲상대필드 = 1
				효과条件성별 = 牝性
				CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				SIF RESULT != 1
					RETURN 0
				CALL CARD_EROTICS(RESULT:1)
				IF RESULT != -1
					조교대상 = RESULT
					PRINTFORML
					PRINTFORML
					SETCOLOR C_PINK
					PRINTFORML %CALLNAME:조교대상%는 사바트에 휘말렸다…！
					RESETCOLOR
					PRINTFORML
					CALL EROTIC_MOVE("全身凌辱",조교대상,-1,500)
				ENDIF
			NEXT
			TRYCALLFORM RITSUMMON(듀얼리스트, 選択カード,出所,1)
		ELSE
			RETURN 0
		ENDIF
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79775,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件성별 = 牝性
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,손패行き)
		ENDIF
		CALL SHUFFLE(듀얼리스트)
	ENDIF
ENDIF

