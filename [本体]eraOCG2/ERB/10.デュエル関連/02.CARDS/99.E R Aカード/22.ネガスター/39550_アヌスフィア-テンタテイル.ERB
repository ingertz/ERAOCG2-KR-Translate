		CALL FULLRESET
		효과추가종류:0 = (140 + GETVAR(@"%듀얼리스트%_MZ_星:装備先"))
		CALL CARD_SP_SUMMON(듀얼리스트,"마함",場所)
		
	ENDIF
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_BASIC(듀얼리스트,"装備",場所,-100000,-100000)
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"不材")
ENDIF

