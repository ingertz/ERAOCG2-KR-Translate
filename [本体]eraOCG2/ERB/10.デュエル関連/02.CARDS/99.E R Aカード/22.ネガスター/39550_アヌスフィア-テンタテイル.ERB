		CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備先)
		IF RESULT != -1
			조교대상 = RESULT
			CALL EROTIC_MOVE("애널解し",조교대상,-1,300)
		ENDIF
		
		CALL SET_FIELD_INFO(듀얼리스트,"필드",装備先,"永攻",-100000)
		
		PRINTFORML 
