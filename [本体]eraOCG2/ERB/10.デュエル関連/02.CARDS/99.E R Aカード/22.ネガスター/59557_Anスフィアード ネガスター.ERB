@CARDNAME_59557,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = An스피어드 네가스타
	CASE "略称"
		RESULTS = An스피어드
	CASE "カテゴリ"
		RESULTS:0 = 가스타
		RESULTS:1 = 네가스타
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_59557(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족 
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1300
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
@CARDSUMMON_59557(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 5
지정素材名前 = 가스타의 질풍 리즈

IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ:0 = 가스타
	素材条件カテゴリ:1 = 아누스피어
ELSEIF EX参照 >= 10
ENDIF
IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,59557) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_59557(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL "가스타" 또는 "아누스피어" 레벨５ 몬스터×２
PRINTL 이 카드명의  ③의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：이 카드가 "가스타의 질풍 리즈"를 소재로 X소환했을 경우、이하의 효과를 얻는다。
PRINTL ●이 카드가 몬스터와 전투를 행하는 경우、그 ２장은 전투로는 파괴되지 않는다。
PRINTL ●이 카드는 전투로 자신 / 상대는 데미지를 받지 않으며、자신은 데미지의 수치만큼 LP를 회복한다。
CALLFORM CARD_ABLE(0,EX소환時強制,ターン開始時強制)
PRINTL ②：이 카드가 X소환했을 때、또는 자신 메인 페이즈 개시시、
PRINTL 자신의 패에 "리즈의 장난감"이 존재하지 않을 경우에 발동한다。
PRINTL 덱 밖에서 "리즈의 장난감" １장을 패에 넣는다。
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ③：자신 메인 페이즈에、이 카드의 X소재를 １개 제거하고 발동할 수 있다。
PRINTL 자신의 EX덱에서 "네가스타" X몬스터 １장을 、X소환 취급으로 특수 소환한다。
PRINTL 이 효과의 발동 후、턴 종료시 까지 자신은 바람 속성 이외의 몬스터의 효과를 발동할 수 없다。
CALL TEXT_DECORATION("ROGUE")

;※ X素材の판정は仕様的に出来ない？ようなので、텍스트の一部はフレイバ-です。
;X소재를 出したり入れたりでA삽입を再現したかっただけの処理

@CARDCAN_59557(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59557)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時強制 || 종류 == ターン開始時強制
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	;既に패에 ある場合は渡さない
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件ID = 159557
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT > 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,59557)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 네가스타
	효과条件소환可 = 1
	효과条件特不無視 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘カードID(듀얼리스트) != 59557
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_条達:場所") == 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_59557(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 戦闘カード
#DIMS DYNAMIC 戦闘カード名
#DIM DYNAMIC 대상カード
#DIMS DYNAMIC 대상カード名
#DIM DYNAMIC 대상カード성별
#DIM DYNAMIC 攻守合計
#DIMS DYNAMIC 攻守合計文字
#DIM DYNAMIC 조교대상 = -1
#DIM DYNAMIC 자신조교 = -1

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF  종류 == EX소환時強制 || 종류 == ターン開始時強制
	PRINTFORML 리즈는 %CALLNAME:MASTER%「장난감」을 넘겨주었다...
	CALL CARD_GENERATE_HAND(듀얼리스트,159557)
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59557,2,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 네가스타
		효과条件소환可 = 1
		효과条件特不無視 = 1
		;CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		選択カードID = RESULT:99
		IF 選択カード != 9999
			
			SETCOLOR C_PINK
			PRINTFORML 
			SELECTCASE 選択カードID
			CASE 59555
				;"윈다":
				PRINTFORML 「기쁘네、리즈씨한테 불려버렸어❤」
				
			CASE 59556
				;"캄":
				PRINTFORML 「같이 가죠…리즈❤」
				
			CASE 59557
				;"리즈":
				PRINTFORML 「이런…즐거워졌네❤」
			
			CASE 59558
				;피리카
				PRINTFORML 「좀-더…놀아요❤ 리즈씨❤ 」
			ENDSELECT				
			RESETCOLOR
			PRINTFORML 
			CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",選択カード)
			SIF 듀얼리스트 == "자신"
				リザルト：XYZ소환 = 1
			
			CALL SET_PLAYER_INFO(듀얼리스트,"속성효과無効",바람속성)
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃時強制誘発
	
	戦闘カード = 戦闘場所(듀얼리스트)
	대상カード = 戦闘場所(듀얼리스트,1)
	TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:戦闘カード")},"略称"
	戦闘カード名 = %RESULTS%
	TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:대상カード")},"略称"
	대상カード名 = %RESULTS%
	대상カード성별 = GETVAR(@"%対面者%_MZ_性:대상カード")
	
	;CALL SET_FIELD_INFO(듀얼리스트,"필드",無効カード,"戦耐",2,1,1)
	

	IF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 1
		;자신이 攻撃표시の時
		IF GETVAR(@"%対面者%_MZ_表:대상カード") == 1
			;상대も攻撃표시
			攻守合計 = GETVAR(@"%듀얼리스트%_MZ_攻:戦闘カード") - GETVAR(@"%対面者%_MZ_攻:대상カード") 
			
			PRINTFORM 공: 
			PRINTVL GETVAR(@"%듀얼리스트%_MZ_攻:戦闘カード")
			PRINTFORM 공: 
			PRINTVL GETVAR(@"%対面者%_MZ_攻:대상カード")
			PRINTFORM 
			
			IF 攻守合計 < 0 
				攻守合計 = -攻守合計
			ENDIF
		ELSE
			;상대는 守備표시
			攻守合計 = GETVAR(@"%対面者%_MZ_守:대상カード") - GETVAR(@"%듀얼리스트%_MZ_攻:戦闘カード")
			
			PRINTFORM 상대수: 
			PRINTVL GETVAR(@"%対面者%_MZ_守:대상カード")
			PRINTFORM 자신공: 
			PRINTVL GETVAR(@"%듀얼리스트%_MZ_攻:戦闘カード")
			PRINTFORM 
			
			IF 攻守合計 < 0 
				攻守合計 = 0
			ENDIF
		ENDIF
	ELSE
		;자신이 守備표시の時
		IF GETVAR(@"%対面者%_MZ_表:대상カード") == 1
			;상대가 攻撃표시なら
			攻守合計 = GETVAR(@"%듀얼리스트%_MZ_守:戦闘カード")  - GETVAR(@"%対面者%_MZ_攻:대상カード") 
			
			PRINTFORM 자신수: 
			PRINTVL GETVAR(@"%듀얼리스트%_MZ_守:戦闘カード")
			PRINTFORM 상대공: 
			PRINTVL GETVAR(@"%対面者%_MZ_攻:대상カード") 
			PRINTFORM 
			
			IF 攻守合計 < 0 
				攻守合計 = 0
			ENDIF
		ENDIF
	ENDIF
	
	;直接攻撃時は、자신의 공격력을 그대로 参照する
	IF 戦闘場所(対面者) == 5 
		攻守合計 = GETVAR(@"%듀얼리스트%_MZ_攻:戦闘カード")
		
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		조교대상 = RESULT
		IF RESULT != -1
			대상カード名 = %NAME:조교대상%
			대상カード성별 = TALENT:조교대상:성별
		ELSE
			대상カード名 = "대상상대"
			대상カード성별 = 0
		ENDIF
	ELSE		
		CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
		조교대상 = RESULT
		;CARD_EROTICS仕組みが分からず、仕方なくID直打ち。コレどこに紐づいてるんだ…？
		CALL CARD_EROTICS(1817)
		자신조교 = RESULT
	ENDIF
	
	;LPを回復
	IF 攻守合計 > 0
		IF 대상カード성별 == 牝性
			PRINTFORML %戦闘カード名%는 %대상カード名%의 엉덩이구멍을 혀로 봉사했다…
			PRINTFORMW %戦闘カード名%「예쁜 구멍이네…핥아줄게에…❤」
			IF 조교대상 != -1
				CALL EROTIC_MOVE("애널解し",조교대상,-1,300)
			ENDIF
			
		ELSEIF 대상カード성별 == 雄性
			PRINTFORML %戦闘カード名%는 엉덩이구멍을 사용해、%대상カード名%의 남근에 봉사했다…
			PRINTFORMW %戦闘カード名%「어때❤ 잘조이지…？❤」
			
			IF 조교대상 != -1
				CALL EROTIC_MOVE("애널解し",자신조교,-1,300)
			ENDIF
			
		ELSEIF 대상カード성별 == ふたなり
			PRINTFORML %戦闘カード名%는 %대상カード名%의 원래 없을터인것에 엉덩이구멍을 대주니、몰아세우는듯이 허리를 흔들기 시작했다…
			PRINTFORMW %戦闘カード名%「훌륭한…❤ …물건❤ 갖고…있네…❤❤」
			
			IF 조교대상 != -1
				CALL EROTIC_MOVE("애널解し",자신조교,-1,300)
			ENDIF
		ELSE
			PRINTFORML %戦闘カード名%는 엉덩이구멍을 손가락으로 벌려、%대상カード名%에게 신음소리를 보냈다…
			PRINTFORMW %戦闘カード名%「기분좋은 소리…❤ …들리고있니❤」
			
			IF 조교대상 != -1
				CALL EROTIC_MOVE("애널解し",자신조교,-1,300)
			ENDIF
		ENDIF
		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",攻守合計)
	ELSE
		PRINTFORML %戦闘カード名%는 조금 불만인것 같다。
		PRINTFORMW 「아-…다음은、좀 더 좋은곳을 보여줬으면 해…」
	ENDIF
	
	;バトルを終了する
	CALL EVADE_ATTACK(듀얼리스트)
	
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"戦耐")
ENDIF

