@CARDNAME_99610,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 매지컬 오나홀
	CASE "略称"
RESULTS = 마법의 고기구멍
	CASE "カテゴリ"
RESULTS:0 = 없음
RESULTS:2 = E.R.A
ENDSELECT


@CARD_99610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99610(종류)
#DIM DYNAMIC 종류
CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,攻撃時誘発)
PRINTL ①：상대 수컷 몬스터가 공격을 선언했을 때, 공격 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 그 공격 몬스터의 공격을 무효화하고,
PRINTL 그 몬스터의 공격력만큼의 대미지를 상대에게 입힌다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99610)
SIF RESULT == 1
	RETURN 0
IF 종류 == 攻撃時誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	TRYCALLFORM CARD_{戦闘カードID(듀얼리스트,1)}("성별")
	SIF RESULT != 雄性
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_99610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상

#DIM DYNAMIC カウンタ
#DIM DYNAMIC ダメージ量
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻撃時誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99610)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		ダメージ量 = GETVAR(@"%対面者%_MZ_攻:(%対面者%_戦闘カード)")

		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 비하인드 오나홀
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1 && GETVAR(@"%듀얼리스트%_제외NG置換") == 0
PRINTL 묘지의 비하인드 오나홀을 제외하시겠습니까？
			CALL YN_CHOICE(듀얼리스트)
		ELSE
			RESULT = いいえ
		ENDIF
		IF RESULT == はい
			CALL FULLRESET
효과条件名前 = 비하인드 오나홀
			CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
			IF RESULT == 1
				ダメージ量 = ダメージ量 * 2
				条件달성 = 1
			ENDIF
		ENDIF

		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		IF RESULT != -1
			조교대상 = RESULT
			IF 条件달성 == 0
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML 몬스터의 성기가 마법의 고기구멍에 빨려들어가, 고기구멍의 감각이 %CALLNAME:조교대상%과 연결된다！
				RESETCOLOR
				CALL EROTIC_MOVE("前穴凌辱",조교대상,,ダメージ量)
			ELSE
				PRINTFORML
				SETCOLOR C_PINK 
				PRINTFORML 몬스터의 성기가 마법의 고기구멍에 빨려 들어가며, 고기구멍의 감각이 %CALLNAME:조교대상%의 두 구멍과 연결된다！
				RESETCOLOR
				CALL EROTIC_MOVE("両穴凌辱",조교대상,,ダメージ量)
			ENDIF
		ENDIF
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",ダメージ量,1)

		CALL EVADE_ATTACK(듀얼리스트)
	ENDIF
ENDIF
