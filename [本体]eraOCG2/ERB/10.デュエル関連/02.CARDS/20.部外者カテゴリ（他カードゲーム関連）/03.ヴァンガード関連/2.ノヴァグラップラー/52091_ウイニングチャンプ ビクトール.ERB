@CARDNAME_52091,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 위닝 챔프 빅토르
	CASE "略称"
RESULTS =  W 빅토르
	CASE "カテゴリ"
RESULTS:0 = 노바 그래플러
RESULTS:1 = 빅토르
RESULTS:4 = 뱅가드
ENDSELECT


@CARD_52091(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 초차원
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 0
	CASE "특수불가"
		RETURN 1
ENDSELECT

RETURN 0


@CARDSUMMON_52091(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件


CALL RESETCONDITION_MAT
必要素材数 = 1
素材条件カテゴリ = ビクト-ル

IF EX参照 == 1
	CALL STRIDECAN(듀얼리스트,소환カード,52091) 
	RETURN RESULT
ENDIF


