@CARDCAN_52092(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52092)
SIF RESULT == 1
	RETURN 0

IF 	종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 손패数(듀얼리스트) < 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트)}("カテゴリ")
	IF !MATCH(RESULTS,"노바 그래플러")
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_52092(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カード추가先
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 条件合致数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

IF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"不材")
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52092)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦",1)
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一攻",500)
ENDIF