@CARD_57815(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 2800
	CASE "추가日"
		RETURN 250608
ENDSELECT
RETURN 0

@CARDSUMMON_57815(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件속성 = 어둠속성
	素材条件종족 = 미확인족
ELSEIF EX参照 >= 10
	素材条件속성 = 어둠속성
	素材条件종족 = 미확인족
ENDIF
IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,57815) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_57815(종류)
#DIM DYNAMIC 종류

PRINTL 「미확인족/어둠 속성 몬스터 × 2
PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,身代효과)
PRINTL ①: 이 카드가 융합 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 덱 / 묘지에서 레벨 4 이하의 어둠 속성 / 미확인족 몬스터를 특수 소환한다.
PRINTL 자신 묘지에 「블랙홀」이 있을 경우,
PRINTL 추가로 자신 묘지의 몬스터(레벨 8) 1장을 고르고 필드에 앞면 표시로 특수 소환할 수 있다.

@CARDCAN_57815(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57815)
SIF RESULT == 1
	RETURN 0
IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,57815)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 어둠속성
	효과条件종족 = 미확인족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_57815(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57815,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件속성 = 어둠속성
		효과条件종족 = 미확인족
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 블랙홀
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			CALL 효과条件レベル_ADV(8, "以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF