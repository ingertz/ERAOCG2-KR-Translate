@CARDNAME_33312,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 미래풍사
	CASE "略称"
RESULTS = 미래풍사
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_33312(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 1300
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33312(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,소환時,특수소환時)
PRINTL ①: 이 카드의 소환 / 특수 소환 시에, 자신 묘지에 바람 속성 몬스터가 2장 이상 존재할 경우에 발동할 수 있다.
PRINTL 자신 덱 위에서부터 카드 3장을 묘지로 보낸다.
PRINTL 그 후, 자신 묘지에서 사이킥족 몬스터 또는 장착 마법 카드 1장을 골라 패에 넣을 수 있다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 자신 묘지의 카드 2장을 덱으로 되돌리고 발동할 수 있다.
PRINTL 패에서 사이킥족 몬스터 1장을 특수 소환한다.


@CARDCAN_33312(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33312)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,33312)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	효과条件속성 = 바람속성
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 3
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,33312)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 의식以外
	효과条件종족 = 사이킥족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33312(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33312,1)
	IF RESULT == 0
		CALL CARD_DECK_DISPOSE(듀얼리스트,"덱",3)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = モン全般
		효과条件종족 = 사이킥족
		효과条件スーパー종류 = 装備마법
		효과条件任意 = 1
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33312,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件複数 = 2
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件종족 = 사이킥족
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF

