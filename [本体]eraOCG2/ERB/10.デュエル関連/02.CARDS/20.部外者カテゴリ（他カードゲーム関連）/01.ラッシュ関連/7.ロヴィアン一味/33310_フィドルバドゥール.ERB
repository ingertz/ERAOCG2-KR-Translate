@CARDNAME_33310,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 피들바두루
	CASE "略称"
RESULTS = 돌바두
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_33310(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 100
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33310(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번만 발동할 수 있다.
CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①: 1턴에 1번, 손패를 1장 버리고, 엑스트라 덱의 바람 속성 사이킥족 융합 몬스터 1장을 보여주고 발동할 수 있다.
PRINTL 덱 / 묘지에서 "사이큅 퓨전" 1장과, 보여준 몬스터에 카드명이 기재된 몬스터 1장을 패에 넣는다.
CALLFORM CARD_ABLE(2,F素材時효과)
PRINTL ②: 이 카드가 융합 소재로서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.


@CARDCAN_33310(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33310)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF 손패数(듀얼리스트) < 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 융합
	효과条件속성 = 바람속성
	효과条件종족 = 사이킥족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
효과条件名前 = 사이큅 융합
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == F素材時효과
	SIF 발동済_2(듀얼리스트,33310)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33310,"묘지")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33310(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33310,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件종류 = 융합
			효과条件속성 = 바람속성
			효과条件종족 = 사이킥족
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			SIF RESULT == 9999
				RETURN 0
			選択カード = RESULT
			選択カード_ID = RESULT:99
			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲묘지 = 1
효과条件名前 = 사이큅 융합
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件名前:0 = ダミ-
			TRYCALLFORM CARDSUMMON_{選択カード_ID}(듀얼리스트,選択カード,0)
			효과条件名前:1 = %素材条件名前:0%
			효과条件名前:2 = %素材条件名前:1%
			효과条件名前:3 = %素材条件名前:2%
			TRYCALLFORM CARDSUMMON_{選択カード_ID}(듀얼리스트,選択カード,10)
			효과条件名前:4 = %素材条件名前:0%
			효과条件名前:5 = %素材条件名前:1%
			효과条件名前:6 = %素材条件名前:2%
			TRYCALLFORM CARDSUMMON_{選択カード_ID}(듀얼리스트,選択カード,11)
			효과条件名前:7 = %素材条件名前:0%
			효과条件名前:8 = %素材条件名前:1%
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == F素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33310,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 피들 디 두
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

