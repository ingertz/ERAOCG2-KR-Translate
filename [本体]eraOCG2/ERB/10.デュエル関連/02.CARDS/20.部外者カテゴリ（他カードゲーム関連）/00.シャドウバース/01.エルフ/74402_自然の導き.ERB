@CARD_EXPLANATION_74402(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 발동할 수 없다.
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①: 자신의 턴 중에 발동할 수 있다.
PRINTL 자신의 앞면 표시의 「엘프 클래스」 몬스터 마법 / 함정 카드 1장을 패로 되돌리고, 1장 드로우한다.
PRINTL 그 후, 패에서 레벨 3 이하의 「엘프 클래스」 몬스터 1장을 소환할 수 있다.


@CARDCAN_74402(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,74402)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,74402)
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF ゾーン == "손패"
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ:0 = 엘프 클래스
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF



@CARDEFFECT_74402(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,74402,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ:0 = 엘프 클래스
		효과条件표시 = 10
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,1)
			CALL FULLRESET
			CALLFORM 효과条件レベル_ADV(3,"以下")
			효과条件カテゴリ:0 = 엘프 클래스
			효과条件종류 = モン全般
			효과条件任意 = 1
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"손패",ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				CALL CARD_SUMMON_SHORI(듀얼리스트,選択カード,"攻撃표시",1)
			ENDIF
		ENDIF
	ENDIF
ENDIF