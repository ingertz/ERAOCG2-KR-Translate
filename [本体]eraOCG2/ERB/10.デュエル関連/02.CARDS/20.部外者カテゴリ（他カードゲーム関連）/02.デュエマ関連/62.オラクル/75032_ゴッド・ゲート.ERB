@CARDNAME_75032,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 갓 게이트
	CASE "略称"
RESULTS = 신문
	CASE "カテゴリ"
RESULTS:0 = 갓
RESULTS:4 = DUEL MASTERS
ENDSELECT


@CARD_75032(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"


		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_75032(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①의 효과는 1턴에 1번만 사용할 수 있다.
CALLFORM CARD_ABLE(1,마함起動 ,フリー誘発)
PRINTL ①：자신의 덱 위에서 카드를 5장 넘긴다.
PRINTL 그 중에서 레벨 9 이하의 「갓」 몬스터 1장을 특수 소환할 수 있다.
PRINTL 남은 카드는 덱으로 되돌린다.
PRINTL 그 후, 패에서 「갓」 몬스터 1장을 특수 소환할 수 있다.
PRINTL 이 효과는 자신이 직접 공격을 받았을 경우에 패에서도 발동할 수 있다.

@CARDCAN_75032(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,75032)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,75032)
		RETURN 0
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,75032)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_CHECK_FIN_BIT(듀얼리스트,"被弾種",2)
	SIF RESULT != 1
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_75032(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75032,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		CALL 효과条件レベル_ADV(9, "以下")
		효과条件カテゴリ = 갓
		효과条件任意 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,,5)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 갓
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75032,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		CALL 효과条件レベル_ADV(9, "以下")
		효과条件カテゴリ = 갓
		효과条件任意 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,,5)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 갓
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
