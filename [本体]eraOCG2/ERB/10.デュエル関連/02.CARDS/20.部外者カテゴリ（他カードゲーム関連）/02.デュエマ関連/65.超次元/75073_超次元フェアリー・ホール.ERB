@CARD_EXPLANATION_75073(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 카드는 1턴에 1장밖에 발동할 수 없다.
CALL CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①: 자신 필드에 바람 속성 또는 땅 속성 몬스터가 2체 이상 존재할 경우에 발동할 수 있다.
PRINTL 레벨 7 이하의 「사이킥」 바람 속성 또는 땅 속성 초차원 몬스터 1장을 EX덱에서 특수 소환한다.
PRINTL 그 후, 덱에서 레벨 2 이하의 바람 속성 또는 땅 속성 몬스터 1장을 특수 소환할 수 있다.
PRINTL 이 효과로 특수 소환한 몬스터의 효과는 무효화된다.

@CARDCAN_75073(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,75073)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,75073)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	효과条件속성:0 = 바람속성
	효과条件속성:1 = 땅속성
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 초차원
	효과条件속성:0 = 바람속성
	효과条件속성:1 = 땅속성
	효과条件カテゴリ = 사이킥
	CALLFORM 효과条件レベル_ADV(7, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_75073(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75073,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件종류 = 초차원
		효과条件속성:0 = 바람속성
		효과条件속성:1 = 땅속성
		효과条件カテゴリ = 사이킥
		CALLFORM 효과条件レベル_ADV(7, "以下")
		CALL DIMSUMMON(듀얼리스트,ゾーン,場所,8)

;		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)	
;		選択カード = RESULT
;		IF 選択カード != 9999
;			CALL DIMSUMMON(듀얼리스트,ゾーン,場所,選択カード,7,8)
;		ENDIF

	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件속성:0 = 바람속성
	효과条件속성:1 = 땅속성
	CALLFORM 효과条件レベル_ADV(2, "以下")
	효과추가종류 = 効追無効
	효과条件任意 = 1
	CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

	ENDIF
ENDIF

