@CARDEFFECT_150963(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタ数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 破壊カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,150963)
	IF RESULT == 0
	破壊カード = 戦闘場所(듀얼리스트,1)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",破壊カード,"제외")
	ENDIF
ELSEIF 종류 == 攻撃後強制誘発
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"戦",1)
ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,150963)
	IF RESULT == 0
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"条達",0,1)
		CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,50963)
		RETURN 1
	ENDIF
	RETURN 0
ENDIF