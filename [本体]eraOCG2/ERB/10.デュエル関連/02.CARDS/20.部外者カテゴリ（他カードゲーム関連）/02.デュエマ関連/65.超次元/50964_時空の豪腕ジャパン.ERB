@CARD_50964(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 초차원
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2700
	CASE "守備力"
		RETURN 2100

ENDSELECT
RETURN 0

@CARD_EXPLANATION_50964(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,ターン開始時)
PRINTL ②: 자신의 턴 개시시에, 자신의 필드에 몬스터가 4체 이상 존재할 경우에 발동할 수 있다.
PRINTL 이 카드는 각성하고 "난타의 각성자 재패니카"가 된다.
PRINTL 각성은 1턴에 1번밖에 진행할 수 없다.

@CARDCAN_50964(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50964)
SIF RESULT == 1
	RETURN 0

IF 종류 == ターン開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 발동済_1(듀얼리스트,覚醒발동)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 3
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_50964(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタ数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,覚醒발동,1)
	IF RESULT == 0
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"条達",2,1)
		CALL CARD_RECONSTRUCT(듀얼리스트,ゾーン,場所,150964)
	ENDIF
ENDIF

