@CARDNAME_73920,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = D의 기관 올 포 원
	CASE "略称"
RESULTS = D의 기관 AFO
	CASE "カテゴリ"
RESULTS:0 = 침략자
RESULTS:1 = 금단
RESULTS:4 = DUEL MASTERS
ENDSELECT


@CARD_73920(参照先)
;名前専用
#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"


		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73920(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 「올 포 원」이 된다.
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 자신 필드의 물 속성 몬스터 1장을 파괴하고,
PRINTL 패 / 묘지에서 물 속성 몬스터 1장을 특수 소환하거나, 덱 / EX덱에서 「금단기관 VV─8」 1장을 마법 & 함정 존에 놓는다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 앞면 표시로 존재하는 한 1번만,
PRINTL 자신이 물 속성 몬스터를 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 이 효과는, 그 몬스터가 특수 소환되었을 때 발동하는 효과가 된다.
PRINTL

@CARDCAN_73920(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 효과使用불가,3

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73903)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,73920)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件속성 = 물속성
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT != 1
		SIF 마함数(듀얼리스트) >= 5
			RETURN 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲EX덱 = 1
효과条件名前 = 금단기관 VV─8
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,73920)
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF




@CARDEFFECT_73920(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 효과先





IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73920,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件속성 = 물속성
		효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲EX덱 = 1
효과条件名前 = 금단기관 VV─8
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT != 1
				RESULT = いいえ
			ELSE
				CALL FULLRESET
				효과範囲마함 = 1
효과条件名前 = 금단기관 VV─8
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT == 1
					RESULT = いいえ
				ELSE
					CALL YN_CHOICE(듀얼리스트,"●덱 ＥＸ덱の「금단기관VV─8」を마법＆함정 존에 置く","●손패 묘지の물속성몬스터를 특수소환")
				ENDIF
			ENDIF
			IF RESULT == はい
				CALL FULLRESET
				효과範囲덱 = 1
				효과範囲EX덱 = 1
효과条件名前 = 금단기관 VV─8
				CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
			ELSEIF RESULT == いいえ
				CALL FULLRESET
				효과範囲손패 = 1
				효과範囲묘지 = 1
				효과条件속성 = 물속성
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF


		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73920)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "オ-ル フォ- ワン")
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73920,11)
	IF RESULT == 0
		CALL CARD_EFFECT_USE(듀얼리스트,73920,2)
		효과先 = FINAL_SUMMON_TARGET(듀얼리스트)
		TRYCALLFORM CARDEFFECT_{GETVAR(@"%듀얼리스트%_필드:효과先")}(듀얼리스트,특수소환時,"효과処理")
	ENDIF
ENDIF
