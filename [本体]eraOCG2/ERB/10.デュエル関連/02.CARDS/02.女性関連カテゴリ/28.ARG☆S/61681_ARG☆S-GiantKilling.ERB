@CARDNAME_61681,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = ARG☆S─GiantKilling
	CASE "略称"
		RESULTS = GiantKill
	CASE "カテゴリ"
		RESULTS:0 = ARG☆S
ENDSELECT


@CARD_61681(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_61681(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의  ①②의 효과는 1턴에 1번, 어느 1개 밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動,마함起動,フリー誘発)
PRINTL ①：덱에서 "ARG☆S" 몬스터 1장을 패에 넣는다.。
PRINTL 자신의 몬스터 존에  지속 함정 카드가 존재할 경우,、
PRINTL 또는 자신을 몬스터로서 특수 소환하는 효과를 가지는 지속 함정 카드가 자신의 마법 & 함정 존에 존재할 경우,
PRINTL 추가로 전사족 몬스터 1장의 일반 소환을 실행할 수 있다。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：자신 메인 페이즈에 묘지의 이 카드를 제외하고 발동할 수 있다る。
PRINTL 자신 필드의 "ARG☆S」지속함정 카드를 포함한 필드의 카드 2장을 패에 되돌린다.。


@CARDCAN_61681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,61681)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,61681)
		RETURN 0
	SIF 발동済_2(듀얼리스트,61681)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = ARG☆S
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,61681)
		RETURN 0
	SIF 발동済_2(듀얼리스트,61681)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件종류 = 지속함정
	효과条件カテゴリ = ARG☆S
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_61681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,61681,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = ARG☆S
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과条件종류 = 지속함정
			효과条件分割 = 효과モン
			효과条件표시 = 10
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과条件종류 = モン全般
				효과条件종족 = 전사족
				CALL CARD_CHOOSE_BASIC(듀얼리스트,ゾーン,場所)
				IF RESULT != 9999
					選択カード = RESULT
					CALL FULLRESET
					CALL CARD_SUMMON(듀얼리스트,選択カード)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,61681,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件종류 = 지속함정
		효과条件カテゴリ = ARG☆S
		효과条件표시 = 10
		;효과条件行先플래그 = 손패行き
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両全場単体",場所,"손패")
		ENDIF
	ENDIF
ENDIF