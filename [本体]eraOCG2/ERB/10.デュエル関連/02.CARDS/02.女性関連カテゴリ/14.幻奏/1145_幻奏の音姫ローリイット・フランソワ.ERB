@CARDNAME_1145,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 환주의 음희 로리이트 프랑수아
	CASE "略称"
RESULTS = 음희 프랑수아
	CASE "カテゴリ"
RESULTS:0 = 환주
RESULTS:1 = 환주의 음희
ENDSELECT


@CARD_1145(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2300
	CASE "守備力"
		RETURN 1700
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1145(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①: 1턴에 1번, 자신 묘지의 천사족 빛 속성 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 패에 넣는다.
PRINTL 이 턴, 자신은 빛 속성 이외의 몬스터의 효과를 발동할 수 없다.



@CARDCAN_1145(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1145)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 천사족
	효과条件속성 = 빛속성
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1145(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1145,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 천사족
		효과条件속성 = 빛속성
		효과条件종류 = モン全般
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		CALL SET_PLAYER_INFO(듀얼리스트,"속성효과無効",빛속성)
	ENDIF
ENDIF