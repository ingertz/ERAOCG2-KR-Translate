@CARDNAME_93530,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 시- 스텔스 어택
	CASE "略称"
RESULTS = 잠해기습
	CASE "カテゴリ"
RESULTS:0 = 해
RESULTS:1 = 시- 스텔스
ENDSELECT


@CARD_93530(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93530(종류)
#DIM DYNAMIC 종류
CALL CARD_ABLE(0,마함起動)
PRINTL ①: 이 카드의 발동 시의 효과 처리로서,
PRINTL 자신의 패 / 묘지에서 「해」 필드 마법을 1장 고르고 발동할 수 있다.
PRINTL ②: 필드에 「해」가 존재할 경우, 앞면 표시의 이 카드는 이하의 효과를 얻는다.
CALL CARD_ABLE(10,발동誘発)
PRINTL ● 1턴에 1번, 상대가 카드의 효과를 발동했을 때,
PRINTL 자신 필드의 앞면 표시의 물 속성 몬스터 1장을 엔드 페이즈까지 제외하고 발동할 수 있다.
PRINTL 이 턴, 자신 필드의 앞면 표시의 마법 / 함정 카드는 파괴되지 않는다.
CALL CARD_ABLE(0,攻撃時誘発)
PRINTL ● 원래 레벨이 5 이상의 자신의 물 속성 몬스터가 상대 몬스터와 전투를 실행할 경우에 발동한다.
PRINTL 그 상대 몬스터를 파괴한다.


@CARDCAN_93530(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93530)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 물속성
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) == 5
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	CALLFORM ATTACK_CARD(듀얼리스트,"속성")
	SIF !MATCH(RESULT,물속성)
		RETURN 0
	TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:戦闘場所(듀얼리스트)")}("レベル")
	SIF RESULT <= 4
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_93530(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 속성数
#DIM DYNAMIC 戦闘カード


IF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93530)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 바다
		효과条件종류 = フィールド
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93530,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과条件속성 = 물속성
		CALL CARD_CHOOSE_BASIC(듀얼리스트, "필드",ゾーン,場所)	
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,選択カード,제외行き,1)
			CALL FULLRESET
			효과条件표시 = 表側표시
			CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"破耐",1)
		ENDIF
	ENDIF
	RETURN 0
ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93530)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",戦闘場所(듀얼리스트,1),"묘지")
	ENDIF
ENDIF
