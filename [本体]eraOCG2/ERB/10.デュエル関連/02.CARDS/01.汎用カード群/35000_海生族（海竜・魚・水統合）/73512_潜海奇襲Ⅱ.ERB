@CARDNAME_73512,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 해 스텔스 Ⅱ
	CASE "略称"
RESULTS = 잠해기습Ⅱ
	CASE "カテゴリ"
RESULTS:0 = 해
RESULTS:1 = 해 스텔스
ENDSELECT


@CARD_73512(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73512(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(0,마함起動 ,손패起動)
PRINTL ①：자신 필드의 물 속성 몬스터는, 몬스터 효과 내성을 얻는다.
CALL CARD_ABLE(0,バトル開始時)
PRINTL ②：자신 / 상대의 배틀 페이즈 개시시에 발동할 수 있다.
PRINTL 자신의 패/묘지에서, 「해」 몬스터 또는 물 속성 일반 몬스터 1장을 고르고 수비 표시로 특수 소환한다.
PRINTL 이 효과로 특수 소환한 몬스터는, 그 배틀 페이즈 종료시에 파괴되며, 소재로 할 수 없다.

@CARDCAN_73512(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73512)
SIF RESULT == 1
	RETURN 0



IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == バトル開始時
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_2(듀얼리스트,73512)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件속성 = 물속성
	효과条件종류 = 通常モン
	효과条件スーパーカテゴリ = 바다
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_73512(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード



IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73512)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73512,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件속성 = 물속성
		효과条件종류 = 通常モン
		효과条件スーパーカテゴリ = 바다
		효과추가종류:0 = 20
		효과추가종류:1 = 21
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"モ耐")

ENDIF