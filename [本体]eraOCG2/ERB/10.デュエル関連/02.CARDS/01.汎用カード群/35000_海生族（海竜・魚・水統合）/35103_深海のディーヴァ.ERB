@CARDNAME_35103,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 심해의 디바
	CASE "略称"
RESULTS = 심해의 디바
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT

@CARD_35103(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 200
	CASE "守備力"
		RETURN 400
	CASE "튜너"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_35103(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(0,소환時)
PRINTL ①: 이 카드가 소환에 성공했을 때 발동할 수 있다.
PRINTL 덱에서 레벨 3 이하의 바다생물족 몬스터 1장을 특수 소환한다.


@CARDCAN_35103(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,35103)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	효과条件소환可 = 1
	CALL 효과条件レベル_ADV(3, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_35103(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35103)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종족 = 바다생물족
		효과条件소환可 = 1
		CALL 효과条件レベル_ADV(3, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

