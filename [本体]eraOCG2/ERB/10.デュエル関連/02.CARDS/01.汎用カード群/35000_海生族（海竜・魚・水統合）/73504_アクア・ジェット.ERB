@CARDNAME_73504,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 아쿠아 제트 서페이스
	CASE "略称"
RESULTS = 아쿠아 제트
	CASE "カテゴリ"
RESULTS:0 = 아쿠아 제트
ENDSELECT


@CARD_73504(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73504(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①: 자신 패/묘지에서 레벨 4 이하의 어류족/해룡족/물족 몬스터 1장을 특수 소환한다.
PRINTL 이 효과의 발동 후, 턴 종료시까지 자신은 엑스 몬스터밖에 엑스트라 덱에서 특수 소환할 수 없다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 상대 필드에 공격 표시 몬스터가 존재할 경우, 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 자신 필드의 바다생물족 몬스터 1장의 공격력은 1000 올린다.


@CARDCAN_73504(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73504)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73504)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件소환可 = 1
	효과条件종족 = 바다생물족
	CALLFORM 효과条件レベル_ADV(4,"以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,73504)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0

	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 攻撃표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 바다생물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73504(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73504,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件소환可 = 1
		효과条件종족 = 바다생물족
		CALLFORM 효과条件レベル_ADV(4,"以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			EX소환可能종류 = 엑시즈
			CALL CARD_EX_RESTRICTION(듀얼리스트)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73504,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 바다생물족
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",RESULT,"永攻",1000)
		ENDIF
	ENDIF
ENDIF
