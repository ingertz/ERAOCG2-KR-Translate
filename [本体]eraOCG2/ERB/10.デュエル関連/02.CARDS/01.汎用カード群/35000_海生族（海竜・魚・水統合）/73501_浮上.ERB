@CARD_EXPLANATION_73501(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(0,마함起動 ,손패起動)
PRINTL ①: 자신 묘지의 레벨 3 이하 바다생물족 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 몬스터를 수비 표시로 특수 소환한다.


@CARDCAN_73501(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73501)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	;最低でもモンスター1枚あればOK
	CALL FULLRESET
	효과条件종족 = 바다생물족
	효과範囲묘지 = 1
	효과条件소환可 = 1
	CALL 효과条件レベル_ADV(3, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73501(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 소환カード

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73501)
	IF RESULT == 0
		;通常마법汎用발동後묘지送り処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 바다생물족
		효과条件소환可 = 1
		CALL 효과条件レベル_ADV(3, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
	ENDIF
ENDIF
