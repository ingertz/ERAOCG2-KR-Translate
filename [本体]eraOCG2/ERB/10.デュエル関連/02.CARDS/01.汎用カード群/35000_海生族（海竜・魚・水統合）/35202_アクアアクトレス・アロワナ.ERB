@CARDNAME_35202,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 아쿠아 액트리스 아로와나
	CASE "略称"
RESULTS = AA 아로와나
	CASE "カテゴリ"
RESULTS:0 = 아쿠아 액트리스
ENDSELECT


@CARD_35202(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 2000
ENDSELECT

RETURN 0

