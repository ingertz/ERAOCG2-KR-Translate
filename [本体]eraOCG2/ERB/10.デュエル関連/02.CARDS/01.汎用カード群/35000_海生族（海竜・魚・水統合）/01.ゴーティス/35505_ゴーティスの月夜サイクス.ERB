@CARDNAME_35505,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 고티스의 달밤 사이키스
	CASE "略称"
RESULTS = 사이키스
	CASE "カテゴリ"
RESULTS:0 = 고티스
ENDSELECT

@CARD_35505(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARD_EXPLANATION_35505(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,소환時 ,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 "고티스의 달밤 사이키스" 이외의 바다 생물족 몬스터 1장을 패에 넣는다.
PRINTL 그 후, 자신 패/필드의 바다 생물족 몬스터 1장을 제외한다.
CALL CARD_ABLE(2,제외時)
PRINTL ②: 이 카드가 제외되었을 경우,
PRINTL 자신 패/필드 / 묘지에서 "고티스의 달밤 사이키스" 이외의 바다 생물족 몬스터 1장을 제외하고 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.

@CARDCAN_35505(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,35505)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,35505)
		RETURN 0

	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	효과条件固名前 = 고티스의 달밤 사이키스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 제외時
	SIF 발동済_2(듀얼리스트,35505)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,35505,"제외")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件固名前 = 고티스의 달밤 사이키스
	효과条件종족 = 바다생물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_35505(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ


IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35505,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 바다생물족
		효과条件固名前 = 고티스의 달밤 사이키스
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲손패 = 1
			효과条件종족 = 바다생물족
			효과条件行先플래그 = 제외行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35505,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件종족 = 바다생물족
		효과条件固名前 = 고티스의 달밤 사이키스
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)	
		IF RESULT == 1
			CALL FULLRESET
			효과範囲제외 = 1
효과条件名前 = 고티스의 달밤 사이키스
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF

