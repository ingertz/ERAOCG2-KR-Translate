@CARDNAME_57702,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 고티스의 대사 아리온포스
	CASE "略称"
RESULTS = 아리온포스
	CASE "カテゴリ"
RESULTS:0 = 고티스
ENDSELECT


@CARD_57702(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2100
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARD_EXPLANATION_57702(종류)
#DIM DYNAMIC 종류


PRINTL 튜너＋튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時)
PRINTL ①：이 카드가 S소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 레벨 6 이하의 바다생물족 몬스터 1장을 제외한다.
CALL CARD_ABLE(2,S素材時효과)
PRINTL ②：이 카드가 S 소재가 되었을 경우, 자신 묘지의 바다생물족 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 제외한다.
PRINTL 그 후, 그 몬스터의 레벨 이하의 레벨을 가지는 바다생물족 몬스터 1장을 덱에서 패에 추가할 수 있다.


@CARDSUMMON_57702(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,57702) 
	RETURN RESULT
ENDIF


@CARDCAN_57702(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57702)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,57702)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	CALL 효과条件レベル_ADV(6, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK(1)
	RETURN 1


ELSEIF 종류 == S素材時효과
	SIF 발동済_2(듀얼리스트,57702)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 바다생물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL CARD_DRAW_CHECK
	RETURN 1
ENDIF

@CARDEFFECT_57702(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC レベル数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57702,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 바다생물족
		CALL 효과条件レベル_ADV(6, "以下")
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == S素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57702,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 바다생물족
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARD_{RESULT:1}, "レベル"
			レベル数 = RESULT
			CALL FULLRESET
			효과条件종족 = 바다생물족
			효과条件任意 = 1
			CALL 효과条件レベル_ADV(レベル数, "以下")
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF



