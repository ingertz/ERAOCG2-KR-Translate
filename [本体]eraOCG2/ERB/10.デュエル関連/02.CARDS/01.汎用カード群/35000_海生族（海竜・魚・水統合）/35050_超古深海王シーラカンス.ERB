@CARDNAME_35050,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 초고 심해왕 실러캔스
	CASE "略称"
RESULTS = 실러캔스
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT

@CARD_35050(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 2200
ENDSELECT

RETURN 0

@CARD_EXPLANATION_35050(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(10,필드起動)
PRINTL ①: 1턴에 1번, 손패를 1장 버리고 발동할 수 있다.
PRINTL 덱에서 레벨 4 이하의 어류족 몬스터를 가능한 한 특수 소환한다.
PRINTL 이 효과로 특수 소환한 몬스터는 공격 선언할 수 없으며, 효과는 무효화된다.
CALL CARD_ABLE(1,발동誘発)
PRINTL ②: 상대가 카드의 효과를 발동했을 때, 이 카드 이외의 바다생물족을 릴리스하고 발동할 수 있다.
PRINTL 이 카드는 다음 페이즈까지 대상 내성을 얻는다.

@CARDCAN_35050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,35050)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	효과추가종류:0 = 1
	효과추가종류:1 = 6
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,35050)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 바다생물족
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_35050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所



IF 종류 == 필드起動 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35050,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종족 = 바다생물족
			효과추가종류:0 = 1
			효과추가종류:1 = 6
			CALL 효과条件レベル_ADV(4, "以下")
			효과条件複数 = 任意の数
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35050,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 바다생물족
		효과条件他所 = 1
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"対耐",5)
		ENDIF
	ENDIF
	RETURN 0
ENDIF

