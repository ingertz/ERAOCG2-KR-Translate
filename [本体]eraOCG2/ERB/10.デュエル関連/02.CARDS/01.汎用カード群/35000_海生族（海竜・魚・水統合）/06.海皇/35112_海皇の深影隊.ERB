@CARDNAME_35112,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 해황의 심영대
	CASE "略称"
RESULTS = 해황의 심영대
	CASE "カテゴリ"
RESULTS:0 = 해황
RESULTS:1 = 머메일
ENDSELECT


@CARD_35112(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_35112(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 손패를 1장 묘지로 버리고 발동할 수 있다.
PRINTL 자신 필드의 모든 물 속성 몬스터의 레벨은 턴 종료시까지 7이 된다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 물 속성 몬스터의 효과를 발동하기 위해 묘지로 보내졌을 경우에 발동한다.
PRINTL "머메일의 심영대"를 제외하고, 레벨 4 이하의, "해황" 몬스터 또는 "머메일" 몬스터 1장을 덱에서 특수 소환한다.
PRINTL 이 턴에, 자신은 물 속성 몬스터밖에 EX덱에서 특수 소환할 수 없다.

@CARDCAN_35112(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,35112)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,35112)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,35112)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
효과条件名前 = 해황의 심연대
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"물효과墓送")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件固名前 = 해황의 심영대
	효과条件カテゴリ:0 = 해황
	효과条件カテゴリ:1 = 머메일
	CALLFORM 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_35112(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35112,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件속성 = 물속성
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一星",7)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35112,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件固名前 = 해황의 심영대
		효과条件カテゴリ:0 = 해황
		효과条件カテゴリ:1 = 머메일
		CALLFORM 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		EX소환可能속성 = 물속성
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF

