@CARDNAME_57650,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 해황룡신 포세이도라 어비스
	CASE "略称"
RESULTS = PA포세이도라
	CASE "カテゴリ"
RESULTS:0 = 해황
RESULTS:1 = 포세이도라
ENDSELECT


@CARD_57650(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0
@CARDSUMMON_57650(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
IF ALT条件 == 0
	必要素材数 = 3
	素材条件レベル = 7
ELSEIF ALT条件 == 1
	必要素材数 = 1
	素材条件종류 = 엑시즈
	素材条件カテゴリ:0 = 해황
	素材条件カテゴリ:1 = 머메일
	重ねてＸ = 1
ENDIF

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,57650) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_57650(종류)
#DIM DYNAMIC 종류

PRINTL 레벨 ７ 몬스터 × ３
PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX덱起動)
PRINTL ①: 이 카드는 자신 필드의 「해황」 X 몬스터나 「머메일」 X 몬스터 위에 겹쳐 X 소환할 수도 있다.
PRINTL ②: １턴에 １번, 이 카드의 X 소재를 ２개 제거하고, 패/덱에서 물 속성 몬스터 １장을 묘지로 보내 발동할 수 있다.
CALL CARD_ABLE(10,필드起動)
PRINTL 상대 필드의 카드를 ３장까지 패로 되돌린다.
PRINTL ③: 이 카드가 묘지로 보내졌을 경우, 패를 １장 묘지로 버리고 발동할 수 있다.
CALL CARD_ABLE(3,필드→묘지,破壊→묘지,戦闘→묘지)
PRINTL 자신 패/묘지에서 레벨 ３ 이하의 해룡족 몬스터 ３장을 특수 소환한다.
PRINTL 자신의 손패 묘지からレベル３이하의 바다생물족モンスター３体を특수소환する。

@CARDCAN_57650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57650)
SIF RESULT == 1
	RETURN 0
IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,57650)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 엑시즈
	효과条件カテゴリ:0 = 해황
	효과条件カテゴリ:1 = 머메일
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0

	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= 0
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 필드→묘지  || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL FULLRESET
	효과範囲손패 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_57650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57650,1)
	IF RESULT == 0
		ALT条件 = 1
		TRYCALLFORM CARDSUMMON_57650(듀얼리스트,場所)
		TRYCALLFORM XYZSUMMON(듀얼리스트,場所)
	ENDIF


ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57650,10,X消費2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件속성 = 물속성
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件複数 = 97
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"손패")
		ENDIF
	ENDIF
ELSEIF 종류 == 필드→묘지  || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57650)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과条件속성 = 물속성
			CALLFORM 효과条件レベル_ADV(3, "以下")
			효과条件複数 = 97
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF

