@CARDNAME_57652,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 머메일-가이오아비스
	CASE "略称"
RESULTS = MM가이오아비스
	CASE "カテゴリ"
RESULTS:0 = 머메일
ENDSELECT


@CARD_57652(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 1600
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0
@CARDSUMMON_57652(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 7
素材条件속성 = 물속성


IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,57652) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_57652(종류)
#DIM DYNAMIC 종류

PRINTL 물속성 레벨 7 몬스터 × 2
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 엑시즈 소재를 가지고 있는 이 카드가 필드 위에 앞면 표시로 존재하는 한,
PRINTL 상대의 레벨 5 이상의 몬스터는 공격할 수 없다.
CALL CARD_ABLE(10,필드起動)
PRINTL ②: 자신 / 상대 턴에 1번, 이 카드의 엑시즈 소재를 1개 제거하고 발동할 수 있다.
PRINTL 이 카드의 공격력보다 낮은 공격력을 가진 상대 필드 위의 몬스터의 효과를 턴 종료 시까지 무효로 한다.


@CARDCAN_57652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57652)
SIF RESULT == 1
	RETURN 0
IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_57652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 지속효과
	CALL FULLRESET
	효과条件他所 = 1
	효과条件속성 = 물속성
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57652,10,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件攻撃力 = GETVAR(@"%듀얼리스트%_MZ_攻:場所") - 1
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"無効")
	ENDIF
ENDIF

