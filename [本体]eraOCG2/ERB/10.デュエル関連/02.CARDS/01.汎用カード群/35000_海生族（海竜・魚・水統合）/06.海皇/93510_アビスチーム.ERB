#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 어비스팀
	CASE "略称"
RESULTS = 어비스팀
	CASE "カテゴリ"
RESULTS:0 = 어비스
ENDSELECT


@CARD_93510(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93510(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 한 번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①: 자신 필드의 바다생물족 몬스터의 종류 수만큼 자신은 드로우한다. (최대 3장)
PRINTL 이 카드의 발동 후, 턴 종료시까지 자신은 물 속성 몬스터밖에 EX덱에서 특수 소환할 수 없다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 묘지에 존재하는 상태에서, 자신 필드의 앞면 표시의 물 속성 몬스터가 전투 / 효과로 파괴되었을 경우,
PRINTL 이 카드를 제외하고 발동할 수 있다.
PRINTL 자신의 묘지 또는 제외되어 있는 「물정령」 몬스터 1장을 패로 가져온다.

@CARDCAN_93510(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93510)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,93510)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 바다생물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,93510)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"묘지")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 머메일
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_93510(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93510,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件固名前起動 = 1
		효과条件종류 = モン全般
		효과条件종족 = 바다생물족
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,93510)
		SIF RESULT >= 3
			RESULT = 3
		CALL CARDDRAW_BASIC(듀얼리스트,RESULT)
		EX소환可能속성 = 물속성
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93510,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 머메일
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
