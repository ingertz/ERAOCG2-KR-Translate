@CARDNAME_11485,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 머메일 - 살라키어비스
	CASE "略称"
RESULTS = MM살라키어비스
	CASE "カテゴリ"
RESULTS:0 = 머메일
ENDSELECT


@CARD_11485(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 800
	CASE "성별"
		RETURN 牝性	
ENDSELECT

RETURN 0
@CARDSUMMON_11485(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족 = 바다생물족

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,11485) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_11485(종류)
#DIM DYNAMIC 종류

PRINTL 바다생물족 몬스터 2체
PRINTL 이 카드명의 ②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ①: 이 카드 이외의 물속성 몬스터의 공격력은 500 포인트 올린다.
CALLFORM CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ②: 자신 / 상대 턴에 손패를 1장 묘지로 보내고 발동할 수 있다.
PRINTL 덱에서 「머메일」 몬스터 1장을 패에 넣는다.
CALL CARD_ABLE(3,破壊→묘지,戦闘→묘지)
PRINTL ②: 이 카드가 파괴되었을 경우에 발동할 수 있다.
PRINTL 덱에서 물속성 몬스터 1장을 묘지로 보내고,
PRINTL 자신의 묘지의 물속성 몬스터 1장을 수비 표시로 특수 소환한다.



@CARDCAN_11485(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,11485)
SIF RESULT == 1
	RETURN 0
IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,11485)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 머메일
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_3(듀얼리스트,11485)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 물속성
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_11485(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID 


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
IF 종류 == 攻守変動
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500)	

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11485,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)		
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = 머메일
			효과条件종류 = モン全般
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11485,3)
	IF RESULT == 0

		CALL FULLRESET
		효과条件속성 = 물속성
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)		
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件속성 = 물속성
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
		ENDIF
	ENDIF
ENDIF

