@CARDNAME_57602,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 화이트 오라 포포이즈
	CASE "略称"
RESULTS = 백투기사활
	CASE "カテゴリ"
RESULTS:0 = 백투기
ENDSELECT


@CARD_57602(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 1300
ENDSELECT

RETURN 0

@CARD_EXPLANATION_57602(종류)
#DIM DYNAMIC 종류

PRINTL 물속성 튜너 ＋ 튜너 이외의 몬스터 1체 이상
PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,필드起動)
PRINTL ①: 이 카드가 S소환했을 경우에 발동할 수 있다.
PRINTL 자신의 패 / 묘지에서 레벨 4 이하의 바다생물족 몬스터 1장을 수비 표시로 특수 소환한다.
PRINTL 그 후, 그 동명 몬스터 1장을 자신의 묘지에서 특수 소환할 수 있다.
CALL CARD_ABLE(0,戦破壊→묘지)
PRINTL ②: 이 카드가 상대에 의해 파괴되어 묘지로 보내졌을 경우,
PRINTL 자신의 묘지에서 다른 물속성 몬스터 1장을 제외하고 발동할 수 있다.
PRINTL 이 카드를 튜너 취급하여 특수 소환한다.


@CARDSUMMON_57602(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件속성 = 물속성
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,57602) 
	RETURN RESULT
ENDIF


@CARDCAN_57602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57602)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,57602)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件종족 = 바다생물족
	CALLFORM 효과条件レベル_ADV(4,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF 処理中_듀얼리스트 == 듀얼리스트
		RETURN 0
	SIF !GROUPMATCH(묘지送り종류,"破壊","戦闘破壊")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,57602,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 물속성
	효과条件종류 = モン全般
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_57602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57602,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件종족 = 바다생물족
		CALLFORM 효과条件レベル_ADV(4,"以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件ID = 選択カード_ID
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57602)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件속성 = 물속성
		효과条件종류 = モン全般
		효과条件行先플래그 = 제외行き
		효과条件他所 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
효과条件名前 = 화이트 오라 포포이즈
			효과추가종류 = 効追永Ｔ
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF



