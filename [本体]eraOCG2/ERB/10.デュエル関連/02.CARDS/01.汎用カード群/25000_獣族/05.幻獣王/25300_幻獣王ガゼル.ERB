@CARDNAME_25300,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 환상수왕 가젤
	CASE "略称"
RESULTS = 환상수왕 가젤
	CASE "カテゴリ"
RESULTS:0 = 가젤
RESULTS:1 = 환상수
ENDSELECT


@CARD_25300(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1200
ENDSELECT

RETURN 0

