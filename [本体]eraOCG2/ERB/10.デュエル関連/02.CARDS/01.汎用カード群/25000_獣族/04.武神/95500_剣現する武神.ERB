@CARDNAME_95500,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 검현하는 무신
	CASE "略称"
RESULTS = 검현하는 무신
	CASE "カテゴリ"
RESULTS:0 = 무신
ENDSELECT


@CARD_95500(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_95500(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①: 이하의 효과에서 1개를 선택하고 발동할 수 있다.
PRINTL ●자신의 묘지의 「무신」 또는 「무신기」 몬스터 1장을 선택하고 패에 넣는다.
PRINTL ●자신의 제외되어 있는 「무신」 또는 「무신기」 몬스터 1장을 선택하고 묘지로 되돌린다.
CALLFORM CARD_ABLE(1,묘지起動)
PRINTL ②: 자신 필드에 「무신」 몬스터가 존재할 경우,
PRINTL 자신의 묘지에서 「무신」 또는 「무신기」 몬스터 1장을 제외하고 발동할 수 있다.
PRINTL 묘지의 이 카드를 세트한다.


@CARDCAN_95500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,95500)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ:0 = 무신
	효과条件カテゴリ:1 = 무신기
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,95500)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ:0 = 무신
	효과条件カテゴリ:1 = 무신기
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 무신
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_95500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성,2


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,95500)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ:0 = 무신
		효과条件カテゴリ:1 = 무신기
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:0 = 1
		CALL FULLRESET
		효과範囲제외 = 1
		효과条件カテゴリ:0 = 무신
		효과条件カテゴリ:1 = 무신기
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:1 = 1
		
		IF 条件달성:0 == 1 && 条件달성:1 == 1
			CALL YN_CHOICE(듀얼리스트,"●몬스터를 손패に加える","●몬스터를 묘지に戻す")
		ELSEIF 条件달성:0 == 1
			RESULT = 0
		ELSEIF 条件달성:1 == 1
			RESULT = 1
		ENDIF

		IF RESULT == 0
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ:0 = 무신
			효과条件カテゴリ:1 = 무신기
			효과条件종류 = モン全般
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			
		ELSEIF RESULT == 1
			CALL FULLRESET
			효과範囲제외 = 1
			효과条件カテゴリ:0 = 무신
			효과条件カテゴリ:1 = 무신기
			효과条件종류 = モン全般
			효과条件行先플래그 = 묘지行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,95500,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ:0 = 무신
		효과条件カテゴリ:1 = 무신기
		효과条件종류 = モン全般
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
효과条件名前 = 검현하는 무신
			CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"裏側표시")
		ENDIF
	ENDIF
ENDIF
