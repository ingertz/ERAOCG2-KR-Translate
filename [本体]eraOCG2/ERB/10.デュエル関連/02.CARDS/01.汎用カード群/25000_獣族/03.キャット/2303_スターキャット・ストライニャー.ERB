@CARDNAME_2303,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 스타-캣 스트라이냐-
	CASE "略称"
RESULTS = 스트라이냐-
	CASE "カテゴリ"
RESULTS:0 = 캣
ENDSELECT


@CARD_2303(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 220
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_2303(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件종족 = 1
	素材条件カテゴリ = 캣
ELSEIF EX参照 >= 10
	素材条件종족 = 6
	素材条件カテゴリ = 캣
ENDIF

IF EX参照 == 1
	;PRINTL サモンまでは言ってるよ
	CALL FUSCAN(듀얼리스트,소환カード,2303) 
	RETURN RESULT
ENDIF

