@CARDNAME_71134,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 보옥의 인도
	CASE "略称"
RESULTS = 보옥의 인도
	CASE "カテゴリ"
RESULTS:0 = 보옥
ENDSELECT


@CARD_71134(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250401
ENDSELECT

