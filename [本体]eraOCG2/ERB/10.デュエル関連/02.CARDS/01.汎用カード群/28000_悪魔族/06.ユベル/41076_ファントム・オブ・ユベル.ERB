@CARDNAME_41076,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 팬텀 오브 유벨
	CASE "略称"
RESULTS = 환영 유벨
	CASE "カテゴリ"
RESULTS:0 = 유벨
ENDSELECT


@CARD_41076(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN ふたなり
	CASE "連動ID"
		RETURN 1075
ENDSELECT
RETURN 0

@CARDSUMMON_41076(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 유벨
	CALLFORM 素材条件レベル_ADV(10,"以上")
	;素材条件固名前 = 팬텀 オブ 유벨
ELSEIF EX参照 >= 10
	素材条件종족 = 악마족
	素材条件攻撃力 = 1
	素材条件守備力 = 1
	素材条件固名前 = 팬텀 オブ 유벨
ENDIF


IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,41076)
	;RETURN RESULT
	RETURN 0
ENDIF

@CARD_EXPLANATION_41076(종류)
#DIM DYNAMIC 종류

PRINTL 레벨 10 이상의 「유벨」 몬스터 + 공격력과 수비력이 0인 악마족 몬스터
PRINTL 자신의 패 / 필드 / 묘지의 상기 카드를 덱 / 엑스트라 덱으로 되돌렸을 경우에만 특수 소환할 수 있다.
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 몬스터 존의 이 카드는 특수 소환의 소재로 할 수 없으며, 전투로는 파괴되지 않는다.
PRINTL 또한, 이 카드의 전투로 발생하는 자신에게로의 전투 데미지는 0이 된다.
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②: 상대 몬스터의 효과가 발동했을 때 발동할 수 있다.
PRINTL 이 카드를 릴리스하고 그 효과를 무효로 하며,
PRINTL 자신의 패 / 덱 / 필드의 레벨 10의 「유벨」 몬스터 1장을 파괴한다.

@CARDSUMMON_AA_41076


@CARDCAN_41076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,41076)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,41076)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 유벨
	CALLFORM 효과条件レベル_ADV(10,"以上")
	효과条件固名前 = 팬텀 오브 유벨
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件종족 = 악마족
	효과条件攻撃力 = 1
	효과条件守備力 = 1
	효과条件固名前 = 팬텀 오브 유벨
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,41076)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 유벨
	효과条件レベル = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_41076(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC ダメージ量

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41076)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 유벨
		CALLFORM 효과条件レベル_ADV(10,"以上")
		;효과条件固名前 = 팬텀 オブ 유벨
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲필드 = 1
			효과範囲묘지 = 1
			효과条件종족 = 악마족
			효과条件攻撃力 = 1
			효과条件守備力 = 1
			효과条件固名前 = 팬텀 오브 유벨
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41076,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",0,"無効")

		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲필드 = 1
		효과条件カテゴリ = 유벨
		효과条件종류 = モン全般
		효과条件レベル = 10
		효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)	
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"不材")
ENDIF


