#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = DDD 헤드 헌트
	CASE "略称"
RESULTS = DDD 헤드 헌트
	CASE "カテゴリ"
RESULTS:0 = DD
RESULTS:1 = DDD
ENDSELECT


@CARD_92890(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92890(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動 ,フリー誘発)
PRINTL ①: 상대 필드의 앞면 표시 몬스터 1장을 대상으로 하여 이 카드를 발동할 수 있다.
PRINTL 그 몬스터의 컨트롤을 다음 턴의 엔드 페이즈까지 얻는다.
PRINTL 그 몬스터는 공격할 수 없으며, 효과를 발동할 수 없다.
PRINTL 이 효과로 컨트롤을 얻은 몬스터가 EX덱에서 특수 소환된 몬스터일 경우, 그 몬스터는 「DDD」 몬스터로도 취급한다.


@CARDCAN_92890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92890)
SIF RESULT == 1
	RETURN 0

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,92890)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = DDD
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT!= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT!= 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者

#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC セット先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92890)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과추가종류:0 = 遅返却行き
		효과추가종류:1 = 効追無効
		효과추가종류:2 = 効追戦無
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
		IF RESULT == 1
			IF GETVAR(@"%듀얼리스트%_MZ_召種:コントロール先") == 召種EX덱
				CALL CARD_SET_CATEGORY(듀얼리스트,"필드",コントロール先,"DDD")
			ENDIF
		ENDIF
	ENDIF
ENDIF

