@CARDNAME_92662,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－레디니스
	CASE "略称"
RESULTS = RR－레디니스
	CASE "カテゴリ"
RESULTS:0 = RR(레이드 랩터즈)
ENDSELECT


@CARD_92662(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92662(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動 ,フリー誘発)
PRINTL ①: 이번 턴에, 자신 필드의 「RR」 몬스터는 전투로는 파괴되지 않는다.
CALLFORM CARD_ABLE(0,묘지起動 ,フリー誘発)
PRINTL ②: 자신 묘지에 「RR」 몬스터가 존재할 경우, 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 이번 턴에, 자신이 받는 모든 데미지는 0이 된다.


@CARDCAN_92662(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92662)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
		RETURN 1
	ELSEIF ゾーン == "묘지"
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 레이드 랩터즈
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_92662(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 설정レベル 

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92662)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL CARD_RESERV(듀얼리스트,92662,短紋章予約)
		ENDIF
	ELSEIF ゾーン == "묘지"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92662)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",1)
		ENDIF
	ENDIF
ELSEIF 종류 == 短紋章起動
	CALL FULLRESET
	효과条件カテゴリ = 레이드 랩터즈
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦耐")
ENDIF
