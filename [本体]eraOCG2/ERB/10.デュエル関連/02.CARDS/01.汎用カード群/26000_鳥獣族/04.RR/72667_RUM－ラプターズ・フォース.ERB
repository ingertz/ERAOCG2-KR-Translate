@CARDNAME_72667,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RUM－랩터즈 포스
	CASE "略称"
RESULTS = RUM 랩터즈
	CASE "カテゴリ"
RESULTS:0 = RUM
ENDSELECT


@CARD_72667(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72667(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動 ,손패起動,フリー誘発)
PRINTL ①: 자신 묘지의 "RR" 엑시즈 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 그 몬스터를 특수 소환하고, 그보다 1랭크 높은 "RR" 엑시즈 몬스터 1장을,
PRINTL 대상 몬스터 위에 겹쳐 엑시즈 소환 취급하여 엑스트라 덱에서 특수 소환한다.
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL  ②: 자신의 "RR" 몬스터가 전투로 파괴되어 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 그 몬스터를 특수 소환하고, 그보다 1랭크 높은 "RR" 엑시즈 몬스터 1장과, 랭크가 그 2배인 엑시즈 몬스터 1장을,
PRINTL 대상 몬스터 위에 겹쳐 엑시즈 소환 취급하여 엑스트라 덱에서 특수 소환한다.

@CARDCAN_72667(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72667)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 레이드 랩터즈
	효과条件종류 = 엑시즈
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과条件カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"戦闘破壊")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 레이드 랩터즈
	효과条件종류 = 엑시즈
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_72667(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 설정レベル
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72667)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 엑시즈
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			설정レベル = GETVAR(@"%듀얼리스트%_MZ_星:カード소환先") + 1
			CALL FULLRESET
			효과条件レベル = 설정レベル
			효과条件종류 = 엑시즈
			효과条件カテゴリ = 레이드 랩터즈
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カード = RESULT
			;戻る
			IF 選択カード != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
				CALL RESETCONDITION_MAT(1)
				代替素材数 = 1
				重ねてＸ = 1
				素材条件固定 = カード소환先
				TRYCALLFORM XYZSUMMON(듀얼리스트,選択カード)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72667)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 엑시즈
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			설정レベル = GETVAR(@"%듀얼리스트%_MZ_星:カード소환先") * 2
			CALL FULLRESET
			효과条件レベル = 설정レベル
			효과条件종류 = 엑시즈
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カード = RESULT
			;戻る
			IF 選択カード != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
				CALL RESETCONDITION_MAT(1)
				代替素材数 = 1
				重ねてＸ = 1
				素材条件固定 = カード소환先
				TRYCALLFORM XYZSUMMON(듀얼리스트,選択カード)
			ENDIF
		ENDIF
	ENDIF
ENDIF
