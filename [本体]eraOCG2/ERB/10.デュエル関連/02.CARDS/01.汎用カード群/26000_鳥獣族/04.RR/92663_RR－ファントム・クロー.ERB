@CARDNAME_92663,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－팬텀 클로
	CASE "略称"
RESULTS = RR－P 클로
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
RESULTS:1 = 팬텀
ENDSELECT


@CARD_92663(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92663(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 카드는 1턴에 1장밖에 발동할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 몬스터의 효과가 발동했을 때,
PRINTL 자신 필드의 어둠 속성 X 몬스터의 X 소재를 1개 제거하고 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
PRINTL 그 후, 자신 필드에 「팬텀」, 「RR」, 「엑시즈 드래곤」 몬스터가 존재할 경우,
PRINTL 이어서 자신 필드의 어둠 속성 X 몬스터 1장을 고르고,
PRINTL 그 공격력을 1000 올린다.

@CARDCAN_92663(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92663)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	IF ゾーン == "마함"
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	ENDIF
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0	
	SIF 발동済_1(듀얼리스트,92663)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 어둠속성
	효과条件Ｘ素所持 = 999
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92663(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力상승
#DIM DYNAMIC 설정レベル 

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92663)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = 엑시즈
		효과条件속성 = 어둠속성
		효과条件Ｘ素所持 = 999
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		SIF 選択カード == 9999
			RETURN 0
		CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"Ｘ",-1)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")

		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = 엑시즈
		효과条件カテゴリ:0 = 팬텀
		효과条件カテゴリ:1 = 레이드 랩터즈
		효과条件カテゴリ:2 = 엑시즈 드래곤
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件종류 = 엑시즈
			효과条件속성 = 어둠속성
			CALL CARD_GRANT_ADV(듀얼리스트,ゾーン,場所,"一攻",1000)
		ENDIF
	ENDIF
ENDIF