@CARDNAME_26609,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－바니싱 레이니어스
	CASE "略称"
RESULTS = RR 바니싱 L
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
ENDSELECT


@CARD_26609(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1300
	CASE "守備力"
		RETURN 1600
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26609(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時 ,특수소환時)
PRINTL ①：이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 패에서 레벨 4 이하의 "RR" 몬스터 1장을 특수 소환한다.
CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①：1턴에 1번, 자신 필드의 "RR" 몬스터 1장을 릴리스하고,
PRINTL 자신 묘지의 "RUM" 마법 카드 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 카드를 패에 넣는다.


@CARDCAN_26609(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26609)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_DRAW_CHECK
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 레이드 랩터즈
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26609(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 条件レベル

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26609,)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 레이드 랩터즈
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26610,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = RUM
			효과条件종류 = 마법全般
			CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF