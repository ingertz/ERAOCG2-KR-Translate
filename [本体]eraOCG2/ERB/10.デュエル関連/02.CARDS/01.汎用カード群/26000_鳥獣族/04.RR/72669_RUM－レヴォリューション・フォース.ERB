@CARDNAME_72669,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RUM - 레볼루션 포스
	CASE "略称"
RESULTS = RUM 레볼루션
	CASE "カテゴリ"
RESULTS:0 = RUM
ENDSELECT


