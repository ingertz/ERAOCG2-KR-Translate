@CARD_EXPLANATION_72669(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動 ,손패起動,フリー誘発)
PRINTL ①: 발동 턴에 따라 이하의 효과를 발동할 수 있다.
PRINTL ●자신 턴: 자신 필드의 「RR」 엑시즈 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 랭크가 1개 높은 「RR」 몬스터 1장을,
PRINTL 대상 몬스터의 위에 겹쳐 엑시즈 소환 취급으로서 엑스트라 덱에서 특수 소환한다.
PRINTL ●상대 턴: 상대 필드의 엑시즈 소재가 없는 엑시즈 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 엑시즈 몬스터의 컨트롤을 얻는다.
PRINTL 그 후, 랭크가 1개 높은 「RR」 몬스터 1장을,
PRINTL 대상 몬스터의 위에 겹쳐 엑시즈 소환 취급으로서 엑스트라 덱에서 특수 소환한다.


@CARDCAN_72669(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72669)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 레이드 랩터즈
		효과条件종류 = 엑시즈
		효과条件重ねてＸ = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		SIF モンスター数(듀얼리스트) >= 5
			RETURN 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件종류 = 엑시즈
		효과条件Ｘ素所持 = 888
		효과条件重ねてＸ = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ENDIF



@CARDEFFECT_72669(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 설정レベル
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72669)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		IF ターンプレイヤー != 듀얼리스트
			CALL FULLRESET
			효과条件종류 = 엑시즈
			효과条件Ｘ素所持 = 888
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
		ENDIF
		CALL FULLRESET
		SIF ターンプレイヤー == 듀얼리스트
			효과条件カテゴリ = 레이드 랩터즈
		SIF ターンプレイヤー != 듀얼리스트
			효과条件Ｘ素所持 = 888
		효과条件종류 = 엑시즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			설정レベル = GETVAR(@"%듀얼리스트%_MZ_星:選択カード") + 1
			CALL FULLRESET
			효과条件レベル = 설정レベル
			효과条件종류 = 엑시즈
			효과条件カテゴリ = 레이드 랩터즈
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カードB = RESULT
			;戻る
			IF 選択カードB != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カードB}")}(듀얼리스트,選択カードB)
				CALL RESETCONDITION_MAT(1)
				代替素材数 = 1
				重ねてＸ = 1
				素材条件固定 = 選択カード
				TRYCALLFORM XYZSUMMON(듀얼리스트,選択カードB)
			ENDIF
		ENDIF
	ENDIF
ENDIF
