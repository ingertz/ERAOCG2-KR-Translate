@CARDNAME_26605,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR-블룸 벌처
	CASE "略称"
RESULTS = RR 블룸 V
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
ENDSELECT


@CARD_26605(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 300
	CASE "守備力"
		RETURN 1900
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26605(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 1턴에 1번, 어느 한 가지밖에 사용할 수 없으며,
PRINTL 이 효과를 발동하는 턴, 자신은 어둠 속성 몬스터밖에 특수 소환할 수 없다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 비행야수족 몬스터 이외의 앞면 표시 몬스터가 존재하지 않는 경우에 발동할 수 있다.
PRINTL 이 카드와 "RR" 몬스터 1장을 패에서 특수 소환한다.
CALLFORM CARD_ABLE(1,묘지起動)
PRINTL ②: 자신 필드에 몬스터가 존재하지 않는 경우,
PRINTL 자신의 묘지의 레벨 4 이하의 "RR" 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 이 카드와 그 몬스터를 수비 표시로 특수 소환한다.

@CARDCAN_26605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26605)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,26605)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,26605,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	CALLFORM 효과条件종족_ADV(비행야수족,"以外")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 레이드 랩터즈
	효과条件종류 = モン全般
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALLFORM 효과条件속성_ADV(어둠속성,"以外")
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"SS이력")
	SIF RESULT != 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,26605)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,26605,"묘지")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) != 0
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 레이드 랩터즈
	효과条件他所 = 1
	CALL 효과条件レベル_ADV(4, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26605(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26605,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		SS소환可能속성 = 어둠속성
		CALL CARD_SS_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26605,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所,"守備표시")
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 레이드 랩터즈
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
		SS소환可能속성 = 어둠속성
		CALL CARD_SS_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF