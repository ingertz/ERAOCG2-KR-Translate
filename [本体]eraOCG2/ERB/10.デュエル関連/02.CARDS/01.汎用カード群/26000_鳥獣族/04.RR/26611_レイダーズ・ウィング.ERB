@CARDNAME_26611,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 레이더즈 윙
	CASE "略称"
RESULTS = R 윙
	CASE "カテゴリ"
RESULTS:0 = 팬텀
RESULTS:1 = 레이드 랩터즈
ENDSELECT


@CARD_26611(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 2000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26611(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,손패起動 ,묘지起動)
PRINTL ①: 이 카드가 패 / 묘지에 존재할 경우에 발동할 수 있다.
PRINTL 자신 필드의 어둠 속성 X 몬스터의 X 소재를 1개 제거하고, 이 카드를 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는, 필드에서 벗어났을 경우에 제외된다.
CALLFORM CARD_ABLE(0,X素材時효과)
PRINTL ②: 이 카드를 소재로 하여 X 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 자신 필드의 어둠 속성 X 몬스터 1체에 대상 내성을 부여한다.

@CARDCAN_26611(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26611)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,26611)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,26611,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 어둠속성
	효과条件종류 = 엑시즈
	효과条件Ｘ素所持 = 999
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == X素材時효과
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 어둠속성
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26611(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 손패起動 || 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26611,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件속성 = 어둠속성
		효과条件종류 = 엑시즈
		효과条件Ｘ素所持 = 999
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード（CS）,"Ｘ",-1)
			CALL FULLRESET
			효과추가종류 = 2
			CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == X素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,"묘지",場所,26611)
	IF RESULT == 0
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件종류 = 엑시즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"対耐",2,1)
		ENDIF
	ENDIF
ENDIF