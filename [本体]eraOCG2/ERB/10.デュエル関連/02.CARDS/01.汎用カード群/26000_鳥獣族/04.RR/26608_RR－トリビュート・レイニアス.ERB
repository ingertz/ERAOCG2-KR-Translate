@CARDNAME_26608,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－트리뷰트 레이니어스
	CASE "略称"
RESULTS = RR 트리뷰트 L
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
ENDSELECT


@CARD_26608(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 1100
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26608(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,소환時 ,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우 발동할 수 있다. 덱에서 「RR」 카드 1장을 묘지로 보낸다.
CALLFORM CARD_ABLE(2,攻撃後誘発)
PRINTL ②: 이 카드가 전투로 상대 몬스터를 파괴했을 경우 발동할 수 있다.
PRINTL 덱에서 「RUM」 속공마법 카드 1장을 패에 넣는다.
PRINTL 이 효과의 발동 후, 턴 종료시까지 자신은 「RR」 몬스터밖에 특수 소환할 수 없다.

@CARDCAN_26608(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26608)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,26608)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,26608)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 速攻마법
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26608(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26608,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26608,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = RUM
		효과条件종류 = 速攻마법
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		CALL CARD_SS_RESTRICTION(듀얼리스트,"레이드 랩터즈")
	ENDIF
ENDIF