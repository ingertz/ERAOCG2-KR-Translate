@CARDNAME_53602,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－와이즈 스트릭스
	CASE "略称"
RESULTS = RR 와이즈 S
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
ENDSELECT


@CARD_53602(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1400
ENDSELECT

RETURN 0
@CARDSUMMON_53602(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족 = 비행야수족
素材条件속성 = 어둠속성

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,53602) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_53602(종류)
#DIM DYNAMIC 종류

PRINTL 비행야수족 어둠 속성 몬스터 2장
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 L 소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 비행야수족 어둠 속성 레벨 4 몬스터 1장을 효과를 무효로 하고 수비 표시로 특수 소환한다.
PRINTL 이 효과 발동 후, 턴 종료 시까지 자신은 L 몬스터를 EX덱에서 특수 소환할 수 없다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 자신의 "RR" X 몬스터의 효과가 발동했을 경우에 발동한다.
PRINTL 덱에서 "RUM" 마법 카드 1장을 자신 필드에 세트한다.
PRINTL 속공 마법 카드를 세트했을 경우, 그 카드는 세트한 턴에도 발동할 수 있다.

@CARDCAN_53602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,53602)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,53602)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 비행야수족
	효과条件속성 = 어둠속성
	효과条件レベル = 4
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,53602)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 엑시즈
	효과条件カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	SIF 마함数(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_53602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID 


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,53602,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종족 = 비행야수족
		효과条件속성 = 어둠속성
		효과条件レベル = 4
		효과추가종류 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
		EX소환可能종류:0 = 효과モン
		EX소환可能종류:1 = 通常モン
		EX소환可能종류:2 = 의식
		EX소환可能종류:3 = 융합
		EX소환可能종류:5 = 싱크로
		EX소환可能종류:6 = 엑시즈
		EX소환可能종류:7 = 초차원
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,53602,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = RUM
		효과条件종류 = 마법全般
		CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"使化裏側표시")
	ENDIF
ENDIF
