@CARDNAME_92610,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = BF－트윈 섀도우
	CASE "略称"
RESULTS = BF 트윈 섀도우
	CASE "カテゴリ"
RESULTS:0 = 블랙 페더
ENDSELECT


@CARD_92610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92610(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 효과는 1턴에 한 번밖에 사용할 수 없다.
PRINTL 자신 필드에 「BF」 몬스터가 2체 이상 존재할 경우, 이 카드의 발동은 패에서도 할 수 있다.
CALLFORM CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①: 자신의 묘지/제외 상태의,
PRINTL 「BF」 튜너 1체와 튜너 이외의 「BF」 몬스터 1장을 덱 / 엑스트라 덱으로 되돌리고 발동할 수 있다.
PRINTL 그 2체의 레벨의 합계와 같은 레벨을 가지는, 「BF」 싱크로 몬스터 1장을 싱크로 소환 취급으로 엑스트라 덱에서 특수 소환한다.

@CARDCAN_92610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92610)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 블랙 페더
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT < 2
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF 발동済_1(듀얼리스트,92610)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 블랙 페더
	효과条件종류 = モン全般
	효과条件튜너 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 블랙 페더
	효과条件종류 = モン全般
	효과条件튜너 = -1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_92610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC 選択レベル

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92610,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 블랙 페더
		효과条件종류 = モン全般
		효과条件튜너 = 1
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			TRYCALLFORM CARD_{RESULT:1},"レベル"
			選択レベル = RESULT
			CALL FULLRESET
			효과範囲묘지 = 1
			효과範囲제외 = 1
			효과条件カテゴリ = 블랙 페더
			효과条件종류 = モン全般
			효과条件튜너 = -1
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				選択レベル += RESULT
				CALL FULLRESET
				효과範囲EX덱 = 1
				효과条件カテゴリ = 블랙 페더
				효과条件レベル = 選択レベル
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF
