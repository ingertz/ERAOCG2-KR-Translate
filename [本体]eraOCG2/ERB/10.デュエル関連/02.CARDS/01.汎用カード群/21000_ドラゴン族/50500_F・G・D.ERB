@CARDNAME_50500,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 파이브 갓 드래곤
	CASE "略称"
RESULTS = F G D
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_50500(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 12
	CASE "攻撃力"
		RETURN 5000
	CASE "守備力"
		RETURN 5000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_50500(종류)
#DIM DYNAMIC 종류

PRINTL 드래곤족 몬스터 × ５
PRINTL 이 카드는 융합 소환으로밖에 특수 소환할 수 없다.
PRINTL ①: 이 카드는 어둠, 땅, 물, 화염, 바람 속성 몬스터와의 전투 중, 전투 내성을 가진다.
PRINTL ②: 이 카드는 어둠, 땅, 물, 화염, 바람 속성 몬스터의 효과 처리 중에는 몬스터 효과 내성을 얻는다.


@CARDSUMMON_50500(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 5
素材条件종족 = 드래곤족


IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,50500) 
	RETURN RESULT
ENDIF



@CARDCAN_50500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50500)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0

	RETURN 1
ENDIF




@CARDEFFECT_50500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 효과발동불가,10
#DIM DYNAMIC 戦闘대상
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 지속효과
	戦闘대상 = 戦闘場所(듀얼리스트,1)
	IF 戦闘대상 != 5
		SIF 戦闘場所(듀얼리스트) != 場所
			효과발동불가:0 = 1
		SIF !GROUPMATCH(GETVAR(@"%対面者%_MZ_属:戦闘대상"),빛속성,신속성)
			효과발동불가:0 = 1
		IF 효과발동불가:0 == 0
			CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
		ENDIF
	ENDIF
	TRYCALLFORM CARD_{処理中_효과ID}("속성")
	IF !GROUPMATCH(RESULT,빛속성,신속성)
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	ENDIF
ENDIF




