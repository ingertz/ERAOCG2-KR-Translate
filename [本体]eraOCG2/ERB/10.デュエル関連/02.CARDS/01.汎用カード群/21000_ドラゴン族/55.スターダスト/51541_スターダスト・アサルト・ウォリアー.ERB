@CARDNAME_51541,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 스타더스트 어썰트 워리어
	CASE "略称"
RESULTS = SDA 워리어
	CASE "カテゴリ"
RESULTS:0 = 스타더스트
RESULTS:1 = 워리어

ENDSELECT


@CARD_51541(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2100
	CASE "守備力"
		RETURN 1200

	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARDSUMMON_51541(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,51541) 
	RETURN RESULT
ENDIF


