@CARD_1094(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1094(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①: 이 카드가 패 / 묘지에 존재하고, 자신 필드의 몬스터가
PRINTL 릴리스되었을 경우 발동할 수 있다. 이 카드를 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는, 필드에서 벗어났을 경우 제외된다.
CALLFORM CARD_ABLE(2,S素材時효과)
PRINTL ②: 이 카드가 S 소재가 되었을 경우 발동할 수 있다. 자신 필드에
PRINTL 「스타더스트 토큰」(드래곤족 / 빛 / 레벨1 / 공격력0 / 수비력0) 1장을 특수 소환한다.


@CARDCAN_1094(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1094)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,1094)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"손패","묘지")
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,1094,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"リリース")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == S素材時효과
	SIF 발동済_2(듀얼리스트,1094)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,551094,ゾーン)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1094(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択レベル
#DIM DYNAMIC 選択カード_ID
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1094,1)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 2
		CALL CARD_SP_SUMMON(듀얼리스트, ゾーン, 場所)
	ENDIF
ELSEIF 종류 == S素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1094,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_GENERATE(듀얼리스트,551094,)
	ENDIF
ENDIF



