@CARDNAME_50850,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 클리어 윙 싱크로 드래곤
	CASE "略称"
RESULTS = CWS 드래곤
	CASE "カテゴリ"
RESULTS:0 = 싱크로 드래곤
RESULTS:1 = 싱크로
ENDSELECT


@CARD_50850(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_50850(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要レベル数 = 7
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,50850) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_50850(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
CALLFORM CARD_ABLE(10,발동誘発)
PRINTL ①: 1턴에 1번, 이 카드명 이외의 필드의 레벨 5 이상의 몬스터의 효과가 발동했을 때 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴하며, 이 카드의 공격력은 턴 종료 시까지 2000 올린다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②: 자신 필드의 레벨 5 이상의 몬스터는 대상 내성을 가진다.


@CARDCAN_50850(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50850)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	CALL 효과条件レベル_ADV(5, "以上")
	효과条件固名前 = 클리어 윙 싱크로 드래곤
	CALL CARD_CHECK_START_DATA(듀얼리스트,2)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_50850(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM 選択カード


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50850,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一攻",2000)
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL 효과条件レベル_ADV(5, "以上")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
ENDIF