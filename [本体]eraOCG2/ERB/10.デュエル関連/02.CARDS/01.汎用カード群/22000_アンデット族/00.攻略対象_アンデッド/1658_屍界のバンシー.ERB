@CARDNAME_1658,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 시계의 밴시
	CASE "略称"
RESULTS = 시계의 밴시
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_1658(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 언데드족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 200
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1658(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드가 몬스터 존에 존재하는 한,
PRINTL 자신 필드의 「언데드 월드」는 효과의 대상이 되지 않으며, 효과로는 파괴되지 않는다.
CALLFORM CARD_ABLE(1,필드起動,묘지起動,フリー誘発)
PRINTL ②: 자신 / 상대 턴에, 필드 / 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 패 / 덱에서 「언데드 월드」 1장을 고르고 발동한다.
PRINTL

@CARDCAN_1658(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1658)
SIF RESULT == 1
	RETURN 0


IF 종류 == 필드起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,1658)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"필드","묘지")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
효과条件名前 = 언데드 월드
	효과条件うらら解除 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_1658(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1658,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲손패 = 1
효과条件名前 = 언데드 월드
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
	ENDIF

ELSEIF 종류 == 지속효과
	CALL FULLRESET
효과条件名前 = 언데드 월드
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"破耐")
	CALL FULLRESET
효과条件名前 = 언데드 월드
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
ENDIF
