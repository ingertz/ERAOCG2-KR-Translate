@CARDNAME_1263,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 하루우라라
	CASE "略称"
RESULTS = 하루우라라
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


