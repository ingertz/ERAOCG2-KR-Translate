@CARDNAME_92252,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 황금향의 와케로
	CASE "略称"
RESULTS = 와케로
	CASE "カテゴリ"
RESULTS:0 = 황금향
RESULTS:1 = 황금노
ENDSELECT


@CARD_92252(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 언데드족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 雄性
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92252(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 1턴에 1번, 어느 쪽이든 1개만 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①: 이 카드를 몬스터 존에 특수 소환한다.
PRINTL 자신 필드에 「엘드리치」 몬스터가 존재할 경우,
PRINTL 추가로 서로의 묘지의 카드 1장을 선택하여 제외할 수 있다.
CALLFORM CARD_ABLE(1,ターン終了時)
PRINTL ②: 서로의 엔드 페이즈에 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 덱에서 「엘드릭시르」 마법 / 함정 카드 1장을 선택하여 자신 필드에 세트한다.


@CARDCAN_92252(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92252)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,92252)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0

	CALL CARD_SS_CHECK(듀얼리스트,92252,"마함")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_1(듀얼리스트,92252)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 마함数(듀얼리스트) == 5
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 엘드릭실
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK(1)

	RETURN 1
ENDIF

@CARDEFFECT_92252(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92252,1)
	IF RESULT == 0
		CALL FULLRESET		
		CALL CARD_SP_SUMMON(듀얼리스트, "마함", 場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 엘드리치
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			SIF 듀얼리스트 == "자신"
				효과範囲묘지 = 1
			효과範囲상대묘지 = 1
			효과条件行先플래그 = 제외行き
			효과条件任意 = 1
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92252,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = 엘드릭실
		효과条件종류 = 마함全般
		CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"裏側표시")
	ENDIF
ENDIF

