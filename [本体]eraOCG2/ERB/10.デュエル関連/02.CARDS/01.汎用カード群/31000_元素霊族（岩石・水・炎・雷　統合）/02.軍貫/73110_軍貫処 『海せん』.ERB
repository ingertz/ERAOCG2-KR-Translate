@CARDNAME_73110,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 군관처『카이센』
	CASE "略称"
RESULTS = 『카이센』
	CASE "カテゴリ"
RESULTS:0 = 군관
ENDSELECT


@CARD_73110(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73110(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 「『해선』」이 된다.
PRINTL 이 카드명의 ①② 효과는 각각 １턴에 １번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①：자신이 「군관」 몬스터의 소환 / 특수 소환에 성공했을 경우 발동할 수 있다.
PRINTL 덱에서 「군관」 카드 １장을 패에 넣는다.

CALLFORM CARD_ABLE(2,효과後誘発,攻撃後誘発)
PRINTL ②：자신 필드의 「군관」 X 몬스터가 전투 / 효과로 파괴되었을 경우 발동할 수 있다.
PRINTL 상대는 ５００LP를 지불한다. 그 후, 자신은 이하의 효과를 적용할 수 있다.
PRINTL ●패의 「샤리 군관」 １장을 특수 소환하고, 「군관」 X 몬스터 １장을 그 위에 겹쳐 X 소환으로 취급하여 EX덱에서 특수 소환한다.
PRINTL 

@CARDCAN_73110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 효과使用불가,3
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73110)
SIF RESULT == 1
	RETURN 0



IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2	
		RETURN 0

	SIF 발동済_1(듀얼리스트,73110)
		효과使用불가:0 = 1
	CALL FULLRESET
	효과条件カテゴリ = 군관
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		효과使用불가:0 = 1
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 군관
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		효과使用불가:0 = 1


	SIF 발동済_2(듀얼리스트,73110)
		효과使用불가:1 = 1
	CALL FULLRESET
	효과条件カテゴリ = 군관
	효과条件종류 = EXモン全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과破壊")
	SIF RESULT != 1
		효과使用불가:1 = 1

	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 샤리 군관
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		효과使用불가:1 = 1

	SIF 효과使用불가:0 == 1 && 효과使用불가:1 == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_2(듀얼리스트,73110)
		RETURN 0	
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2	
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 샤리 군관
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	TRYCALLFORM CARDNAME_{直前戦闘破壊ID(듀얼리스트)}, "カテゴリ"
	SIF !MATCH(RESULTS, "군관")
		RETURN 0
	TRYCALLFORM CARD_{直前戦闘破壊ID(듀얼리스트)}, "종류"
	SIF !MATCH(RESULT,엑시즈)
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_73110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 효과使用불가,3

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73110)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 효과後誘発

	SIF 발동済_1(듀얼리스트,73110)
		효과使用불가:0 = 1

	SIF 발동済_2(듀얼리스트,73110)
		효과使用불가:1 = 1

	CALL FULLRESET
	효과条件カテゴリ = 군관
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		효과使用불가:0 = 1

	CALL FULLRESET
	효과条件カテゴリ = 군관
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과破壊")
	SIF RESULT != 1
		효과使用불가:1 = 1
	
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 샤리 군관
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		효과使用불가:1 = 1

	IF 효과使用불가:0 == 0 && 효과使用불가:1 == 0
PRINTL 군관 카드를 패에 넣겠습니까?
		CALL YN_CHOICE(듀얼리스트)
	ELSEIF 효과使用불가:1 == 1
		RESULT = はい
	ELSEIF 효과使用불가:0 == 1
		RESULT = いいえ
	ENDIF
	IF RESULT == はい
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73110,1)
		IF RESULT == 0
			CALL FULLRESET
			효과条件カテゴリ = 군관
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ELSEIF RESULT == いいえ
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73110,2)
		IF RESULT == 0
			CALL LIFE_CHANGE(対面者,"減少","コスト",500)
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 샤리 군관
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과条件종류 = 엑시즈
				효과条件カテゴリ = 군관
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
				選択カード = RESULT
				IF 選択カード != 9999
					TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
					代替素材数 = 1
					素材条件カテゴリ = 샤리 군관
					素材条件レベル = 4
					CALL XYZSUMMON(듀얼리스트, 選択カード)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73110,2)
	IF RESULT == 0
		CALL LIFE_CHANGE(対面者,"減少","コスト",500)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 샤리 군관
		효과条件任意 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件종류 = 엑시즈
			효과条件カテゴリ = 군관
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
				代替素材数 = 1
				素材条件カテゴリ = 샤리 군관
				素材条件レベル = 4
				CALL XYZSUMMON(듀얼리스트, 選択カード)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "『바다せん』")
ENDIF
