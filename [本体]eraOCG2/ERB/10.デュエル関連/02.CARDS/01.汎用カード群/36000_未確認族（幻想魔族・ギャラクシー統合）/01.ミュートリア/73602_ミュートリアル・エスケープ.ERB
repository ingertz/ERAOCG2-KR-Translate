@CARDNAME_73602,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 뮤트리얼 에스케이프
	CASE "略称"
RESULTS = 긴급 에스케이프
	CASE "カテゴリ"
RESULTS:0 = 뮤트리아
RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_73602(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 速攻마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73602(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①: 손패 또는 덱에서 레벨 3 이하의 미확인족 몬스터 1장을 특수 소환한다.
PRINTL 이 효과로 특수 소환한 몬스터는 턴 종료시에 덱으로 되돌아간다.
CALLFORM CARD_ABLE(2,제외時)
PRINTL ②: 이 카드명 이외의 「뮤트리아」 카드가 제외되어 있고,
PRINTL 이 카드가 제외되었을 경우에 발동할 수 있다.
PRINTL 덱에서 「퓨전 뮤트리아」 1장을 패에 더한다.

@CARDCAN_73602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73602)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,73602)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件종족 = 미확인족
	효과条件종류 = モン全般
	CALL 효과条件レベル_ADV(3, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 제외時
	SIF 발동済_2(듀얼리스트,73602)
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件カテゴリ = 뮤트리아
	효과条件固名前 = 뮤트리얼 에스케이프
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
효과条件名前 = 융합 뮤트리아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_73602(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73602,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件종족 = 미확인족
		효과추가종류 = 10 + 덱行き
		CALL 효과条件レベル_ADV(3, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)		
	ENDIF
ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73602,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
효과条件名前 = 융합 뮤트리아
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)		
	ENDIF
ENDIF