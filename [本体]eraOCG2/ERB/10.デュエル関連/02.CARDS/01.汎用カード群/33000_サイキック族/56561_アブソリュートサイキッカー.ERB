@CARDNAME_56561,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 앱솔루트 사이킥커
	CASE "略称"
RESULTS = AB 사이킥커
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_56561(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 11
	CASE "攻撃力"
		RETURN 3400
	CASE "守備力"
		RETURN 2200
	CASE "추가日"
		RETURN 250430
ENDSELECT

RETURN 0

@CARD_EXPLANATION_56561(종류)
#DIM DYNAMIC 종류

PRINTL 사이킥족 S 몬스터 + S 몬스터
PRINTL 이 카드명의 ③의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,EX소환時)
PRINTL ①: 이 카드가 융합 소환했을 경우, 2000 LP를 지불하고 발동할 수 있다.
PRINTL 상대 필드의 앞면 표시 카드를 전부 제외한다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②: 이 카드는 몬스터 존에 존재하는 한, 효과로는 파괴되지 않는다.
CALLFORM CARD_ABLE(3,묘지起動)
PRINTL ③: 이 카드를 묘지에서 제외하고 발동할 수 있다.
PRINTL EX덱에서 사이킥족 레벨 10 융합 몬스터 1장을 융합 소환으로 취급하여 특수 소환한다.


@CARDSUMMON_56561(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件종족 = 사이킥족
	素材条件종류 = 싱크로
ELSEIF EX参照 >= 10
	素材条件종류 = 싱크로
ENDIF

IF EX参照 == 1
	;PRINTL サモンまでは言ってるよ
	CALL FUSCAN(듀얼리스트,소환カード,56561) 
	RETURN RESULT
ENDIF

@CARDCAN_56561(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 使用済み
#DIM DYNAMIC 효과使用불가,3

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56561)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF GETVAR(@"ライフ_%듀얼리스트%") < 2000
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF 발동済_3(듀얼리스트,56561)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종족 = 사이킥족
	효과条件종류 = 융합
	효과条件レベル = 10
	효과条件소환可 = 1
	효과条件特不無視 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_56561(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56561)
	IF RESULT == 0
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",2000)

		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場全体",場所,"제외")
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56561,3)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		
		CALL FULLRESET
		효과条件종족 = 사이킥족
		효과条件종류 = 융합
		효과条件レベル = 10
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		IF RESULT != 9999
			CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",RESULT)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")
ENDIF

