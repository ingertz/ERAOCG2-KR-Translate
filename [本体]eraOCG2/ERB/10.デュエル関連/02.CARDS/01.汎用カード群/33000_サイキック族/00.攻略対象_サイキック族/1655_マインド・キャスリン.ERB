@CARDNAME_1655,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 마인드 캐스린
	CASE "略称"
RESULTS = 엠 캐스린
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_1655(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 2600
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1655(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 ＋ 튜너 이외의 몬스터 1체 이상
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 상대 필드의 앞면 표시 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터와 이 카드의 컨트롤을 바꾼다.
CALLFORM CARD_ABLE(2,戦破壊→묘지)
PRINTL ②: 이 카드가 필드에서 묘지로 보내졌을 경우,
PRINTL 자신 및 상대 필드의 앞면 표시 몬스터를 1체씩 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터 2체의 컨트롤을 바꾼다.


@CARDSUMMON_1655(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,1655) 
	RETURN RESULT
ENDIF


@CARDCAN_1655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1655)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,1655)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5 && モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	SIF 발동済_2(듀얼리스트,1655)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5 && モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1655(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 상대選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1655,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 10
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所,1655)
		選択カード = RESULT
		IF 選択カード != 9999
			IF モンスター数(듀얼리스트) >= 5
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,場所)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,選択カード)
			ELSEIF モンスター数(対面者) >= 5
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,選択カード)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,場所)
			ELSE
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,選択カード)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1655,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 10
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所,1655)
		選択カード = RESULT
		CALL FULLRESET
		효과条件표시 = 10
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所,1655)
		상대選択カード = RESULT
		IF 選択カード != 9999 && 상대選択カード != 9999
			IF モンスター数(듀얼리스트) >= 5
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,選択カード)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,상대選択カード)
			ELSEIF モンスター数(対面者) >= 5
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,상대選択カード)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,選択カード)
			ELSE
				CALL FULLRESET
				CALL CARD_GET_CONTROL(듀얼리스트,상대選択カード)
				CALL FULLRESET
				CALL CARD_GET_CONTROL(対面者,選択カード)
			ENDIF
		ENDIF
	ENDIF
ENDIF



