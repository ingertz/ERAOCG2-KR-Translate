@CARDNAME_11650,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 정적의 사이코 걸
	CASE "略称"
RESULTS = 정적 사이코 걸
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_11650(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 300
	CASE "성별"
		RETURN 牝性
	CASE "튜너"
		RETURN 1
	CASE "추가日"
		RETURN 250430
ENDSELECT
RETURN 0

@CARD_EXPLANATION_11650(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,소환時,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 자신의 손패를 1장 선택하여 제외하고, "정적의 사이코 걸"을 제외한 레벨 4 이하 사이킥족 몬스터 1장을 덱에서 특수 소환한다.
PRINTL 이 턴, 자신은 사이킥족 몬스터밖에 EX덱에서 특수 소환할 수 없다.
PRINTL ②: 자신 필드의 사이킥족 몬스터의 횟수까지,
CALLFORM CARD_ABLE(2,필드起動)
PRINTL 필드의 앞면 표시 몬스터를 대상으로 발동할 수 있다.
PRINTL 그 몬스터의 레벨을 턴 종료 시까지 1 올린다.
PRINTL その몬스터의 レベルをターン終了時まで１つ上げる。


@CARDSUMMON_AA_11650


@CARDCAN_11650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,11650)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,11650)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 손패数(듀얼리스트) < 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 사이킥족
	효과条件固名前 = 정적의 사이코 걸
	CALLFORM 효과条件レベル_ADV(4,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,11650)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1	
ENDIF

@CARDEFFECT_11650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択횟수
#DIM DYNAMIC 選択カード,5

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11650)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종족 = 사이킥족
			효과条件固名前 = 정적의 사이코 걸
			CALLFORM 효과条件レベル_ADV(4,"以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			EX소환可能종족 = 사이킥족
			CALL CARD_EX_RESTRICTION(듀얼리스트)
		ENDIF
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11650,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 사이킥족
		효과条件표시 = 表側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		選択횟수 = RESULT

		FOR カウンタ,0,選択횟수
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲상대필드 = 1
			효과条件종류 = レベル持ち
			효과条件표시 = 表側표시
			CALL CARD_GRANT_ADV(듀얼리스트,ゾーン,場所,"一星",1,1)
		NEXT
	ENDIF
ENDIF
