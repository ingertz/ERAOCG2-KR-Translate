@CARDSUMMON_56560(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件종족 = 사이킥족
	素材条件종류 = 싱크로
ELSEIF EX参照 >= 10
	素材条件종족 = 사이킥족
ENDIF

IF EX参照 == 1
	;PRINTL サモンまでは言ってるよ
	CALL FUSCAN(듀얼리스트,소환カード,56560) 
	RETURN RESULT
ENDIF

@CARDCAN_56560(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 使用済み
#DIM DYNAMIC 효과使用불가,3

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56560)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_56560(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 条件달성,3
#DIM DYNAMIC 選択肢표시
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 破壊カード


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 攻撃後強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56560)
	IF RESULT == 0
		CALLFORM CARD_{直前戦闘破壊ID(듀얼리스트,1)}("攻撃力")
		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",RESULT)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")
ENDIF

