@CARDNAME_56560,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 얼티밋 사이킥커-
	CASE "略称"
RESULTS = UL 사이킥커-
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_56560(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 1700
	CASE "추가日"
		RETURN 250430
ENDSELECT

RETURN 0

