@CARDNAME_33102,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = PSY 프레임기어 알파
	CASE "略称"
RESULTS = PSY 알파
	CASE "カテゴリ"
RESULTS:0 = PSY 프레임
ENDSELECT


@CARD_33102(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 0
	CASE "소환불가"
		RETURN 1
	CASE "튜너"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33102(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 일반 소환할 수 없다.
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ①: 자신 필드에 몬스터가 존재하지 않고,
PRINTL 상대가 몬스터를 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 패의 이 카드와 자신의 패 / 덱 / 묘지의 「PSY 프레임 드라이버」 1장을 고르고 특수 소환하고,
PRINTL 덱에서 「PSY 프레임기어 알파」 이외의 「PSY 프레임」 카드 1장을 패에 넣는다.
PRINTL 이 효과로 특수 소환한 몬스터는 전부 엔드 페이즈에 제외된다.

@CARDCAN_33102(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33102)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33102,"손패")
	SIF RESULT == 1
		RETURN 0
	IF モンスター数(듀얼리스트) != 0
		CALL FULLRESET
		효과範囲필드 = 1
효과条件名前 = PSY 프레임기어 알파
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		SIF モンスター数(듀얼리스트) >= 4
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = PSY 프레임 드라이버
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般",1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33102(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성


IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33102)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 効追終除
		CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = PSY 프레임 드라이버
		효과추가종류 = 効追終除
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = PSY 프레임
			효과条件固名前 = PSY 프레임기어 알파
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF

