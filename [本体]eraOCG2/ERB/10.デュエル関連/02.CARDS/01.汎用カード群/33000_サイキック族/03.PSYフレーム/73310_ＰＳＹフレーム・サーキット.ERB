@CARDNAME_73310,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = PSY프레임 서킷
	CASE "略称"
RESULTS = PF 서킷
	CASE "カテゴリ"
RESULTS:0 = PSY프레임
ENDSELECT


@CARD_73310(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73310(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 「PSY프레임 서킷」이 된다.
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ①: 자신 필드에 「PSY프레임」 몬스터가 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 자신 필드의 몬스터를 싱크로 소재로서 싱크로 소환한다.
CALLFORM CARD_ABLE(10,攻撃時誘発)
PRINTL ②: 1턴에 1번, 자신의 「PSY프레임」 몬스터가 상대 몬스터와 전투를 실행할 경우,
PRINTL 패 / 덱에서 「PSY프레임」 몬스터 1장을 묘지로 보내고 발동할 수 있다.
PRINTL 그 전투를 실행하는 자신 몬스터의 공격력은 턴 종료시까지,
PRINTL 묘지로 보낸 몬스터의 공격력과 수비력 중, 높은 쪽의 수치만큼 올라간다.

@CARDCAN_73310(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73310)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0

	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 싱크로
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = PSY 프레임
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) == 5
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트)},"カテゴリ"
	SIF !MATCH(RESULTS,"PSY 프레임")
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = PSY 프레임
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_73310(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 攻撃力修正

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73310)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73310)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 싱크로
		효과条件소환可 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード= RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
			CALL SYNSUMMON(듀얼리스트, 選択カード)
		ENDIF
	ENDIF

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73310,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ = PSY 프레임
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			TRYCALLFORM CARD_{選択カード_ID},"攻撃力"
			攻撃力修正 = RESULT
			TRYCALLFORM CARD_{選択カード_ID},"守備力"
			SIF RESULT > 攻撃力修正
				攻撃力修正 = RESULT
			戦闘カード = 戦闘場所(듀얼리스트)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",戦闘カード,"一攻",攻撃力修正)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "PSY 프레임 サ-キット")
ENDIF
