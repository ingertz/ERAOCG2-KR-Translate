@CARDNAME_33106,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = PSY 프레임기어 엡실론
	CASE "略称"
RESULTS = PFG 엡실론
	CASE "カテゴリ"
RESULTS:0 = PSY 프레임
ENDSELECT


@CARD_33106(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 0
	CASE "소환불가"
		RETURN 1
	CASE "튜너"
		RETURN 1
	CASE "レア度"
		RETURN CMN
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33106(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 통상 소환할 수 없다.
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ①: 자신 필드에 몬스터가 존재하지 않고, 상대의 함정 카드의 효과가 발동했을 때 발동할 수 있다.
PRINTL 패의 이 카드와 자신 패 / 덱 / 묘지의 "PSY 프레임 드라이버" 1장을 선택하여 특수 소환하고,
PRINTL 그 발동을 무효로 하고 파괴한다.
PRINTL 이 효과로 특수 소환한 몬스터는 전부 엔드 페이즈에 제외된다.

@CARDCAN_33106(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33106)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33106,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33100,"")
	SIF RESULT == 1
		RETURN 0
	IF モンスター数(듀얼리스트) != 0
		CALL FULLRESET
		효과範囲필드 = 1
효과条件名前 = PSY 프레임기어 알파
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		SIF モンスター数(듀얼리스트) >= 4
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = PSY 프레임 드라이버
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 함정全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33106(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33106)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 効追終除
		CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = PSY 프레임 드라이버
		효과추가종류 = 効追終除
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF

