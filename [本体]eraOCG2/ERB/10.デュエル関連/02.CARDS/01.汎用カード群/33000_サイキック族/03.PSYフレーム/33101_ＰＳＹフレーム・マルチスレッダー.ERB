@CARDNAME_33101,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = PSY 프레임 멀티 스레더
	CASE "略称"
RESULTS = PF 멀티 스레
	CASE "カテゴリ"
RESULTS:0 = PSY 프레임
RESULTS:1 = PSY 프레임 드라이버
ENDSELECT


@CARD_33101(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_33101(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 "PSY 프레임 드라이버"이다.
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,身代효과)
PRINTL ①: 자신 필드의 "PSY 프레임" 카드가 전투 효과로 파괴될 경우,
PRINTL 대신에 손패 필드의 이 카드를 릴리스할 수 있다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 자신 필드에 "PSY 프레임" 튜너가 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 이 카드를 묘지에서 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는, 필드에서 벗어났을 경우에 제외된다.

@CARDCAN_33101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,33101)
SIF RESULT == 1
	RETURN 0

IF 종류 == 身代효과
	SIF !GROUPMATCH(ゾーン,"손패","필드")
		RETURN 0
	TRYCALLFORM CARDNAME_{破壊대상ID}("カテゴリ")
	SIF !MATCH(RESULTS,"PSY 프레임")
		RETURN 0
	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 발동済_2(듀얼리스트,33101)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,33101,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = PSY 프레임
	효과条件튜너 = 1
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"특수소환")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_33101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성


IF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33101)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,33101,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = PSY 프레임 멀티 스레더
		효과추가종류 = 効追除処
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

