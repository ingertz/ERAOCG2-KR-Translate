@CARDNAME_56556,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = ＰＳＹ 프레임로드 Ζ
	CASE "略称"
RESULTS = ＰＦＬ Ζ
	CASE "カテゴリ"
RESULTS:0 = ＰＳＹ 프레임
ENDSELECT


@CARD_56556(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN RARE
ENDSELECT

RETURN 0

@CARD_EXPLANATION_56556(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
CALLFORM CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ①: 서로의 턴에 1번, 상대 필드의 특수 소환된 앞면 공격 표시 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터와 필드의 이 카드를 제외한다.
PRINTL 다음 자신의 스탠바이 페이즈에, 자신은 제외 상태인 「ＰＳＹ 프레임로드 Ζ」를 특수 소환하고,
PRINTL 상대는 자신의 제외 상태의 몬스터 1장을 특수 소환한다.
CALLFORM CARD_ABLE(0,묘지起動)
PRINTL ②: 이 카드가 묘지에 존재할 경우,
PRINTL 이 카드 이외의 자신의 묘지의 「ＰＳＹ 프레임」 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 이 카드를 EX덱으로 되돌리고, 대상의 카드를 패에 넣는다.


@CARDSUMMON_56556(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 7
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,56556) 
	RETURN RESULT
ENDIF


@CARDCAN_56556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56556)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 攻撃표시
	효과条件召種 = 召種全특수
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = PSY 프레임
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン開始時予約
	RETURN 1

ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_56556(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56556,10)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		CALL FULLRESET
		효과条件표시 = 攻撃표시
		효과条件召種 = 召種全특수
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"제외")
		CALL CARD_RESERV(듀얼리스트,56556,スタン予約)
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56556)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = PSY 프레임
		효과条件他所 = 1
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
효과条件名前 = ＰＳＹ 프레임로드 Ζ
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == ターン開始時予約
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과範囲제외 = 1
효과条件名前 = ＰＳＹ 프레임로드 Ζ
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		
		CALL FULLRESET
		효과範囲제외 = 1
		CALL CARD_SP_SUMMON_ADV(対面者,ゾーン,場所)
	ELSE
		CALL CARD_RESERV(듀얼리스트,56556)
	ENDIF

ELSEIF 종류 == ターン終了時予約
	CALL CARD_RESERV(듀얼리스트,56556,スタン予約)
ENDIF



