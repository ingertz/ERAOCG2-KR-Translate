@CARDSUMMON_56550(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 11
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,56550) 
	RETURN RESULT
ENDIF


@CARDCAN_56550(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56550)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") > GETVAR(@"ライフ_%対面者%")
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_召種:場所") != 召種EX덱
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF 듀얼리스트 != "자신"
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件他所 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == バトル開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,56550)
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_56550(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56550,10)
	IF RESULT == 0
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",1000)
		CALL FULLRESET
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所,56550)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_DISPOSE(듀얼리스트,"필드",選択カード,1)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"제외")
		ENDIF
	ENDIF

ELSEIF 종류 == バトル開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56550,2)
	IF RESULT == 0
		IF GETVAR(@"ライフ_%듀얼리스트%") > GETVAR(@"ライフ_%対面者%")
			攻撃力修正 = GETVAR(@"ライフ_%듀얼리스트%") - GETVAR(@"ライフ_%対面者%")
		ELSEIF　GETVAR(@"ライフ_%対面者%") > GETVAR(@"ライフ_%듀얼리스트%")
			攻撃力修正 = GETVAR(@"ライフ_%対面者%") - GETVAR(@"ライフ_%듀얼리스트%")
		ENDIF
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",攻撃力修正)
	ENDIF
ENDIF



