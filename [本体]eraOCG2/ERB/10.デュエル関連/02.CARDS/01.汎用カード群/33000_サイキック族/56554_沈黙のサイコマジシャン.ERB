@CARDNAME_56554,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 침묵의 사이코 마술사
	CASE "略称"
RESULTS = 침묵 사이코 마지
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_56554(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 雄性
	CASE "튜너"
		RETURN 2
	CASE "추가日"
		RETURN 250430
ENDSELECT

RETURN 0

@CARD_EXPLANATION_56554(종류)
