@CARDNAME_73333,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = JAM：P 체크!
	CASE "略称"
RESULTS = JAM：P 체크!
	CASE "カテゴリ"
RESULTS:0 = JAM：P
ENDSELECT


@CARD_73333(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73333(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 1의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL 1: 자신 필드에 사이킥족 일반 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 상대 필드의 마법 / 함정 카드를 2장까지 선택하고 파괴한다.
PRINTL 그 후, 자신 필드 / 묘지에 "CAN：D"가 존재할 경우,
PRINTL 덱에서 "JAM：P 체크!" 이외의 "JAM：P" 마법 / 함정 카드를 1장 필드에 세트한다.

@CARDCAN_73333(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73333)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73333)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	;最低でもモンスター1枚あればOK
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이킥족
	효과条件종류 = 通常モン
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK(1)
	RETURN 1
ENDIF

@CARDEFFECT_73333(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 묘지送カード_ID

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73333,1)
	IF RESULT == 0
		;通常마법汎用발동後묘지送り処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件複数 = 98
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함単体",場所,"손패")
		SIF 마함数(듀얼리스트) >= 5
			RETURN 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲필드 = 1
효과条件名前 = CAN：D
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT != 1
			CALL FULLRESET
			효과条件カテゴリ = JAM：P
			효과条件종류 = 마함全般
			효과条件固名前 = JAM：P 체크!
			CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"裏側표시")
		ENDIF
	ENDIF
ENDIF
