@CARDNAME_73331,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = JAM：P 시작!
	CASE "略称"
RESULTS = JAM：P 시작!
	CASE "カテゴリ"
RESULTS:0 = JAM：P
RESULTS:1 = 융합
ENDSELECT


@CARD_73331(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73331(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①：자신의 손패 필드 묘지에서,
PRINTL 사이킥족의 융합 몬스터 카드에 의해 정해진 융합 소재 몬스터를 덱으로 되돌리고,
PRINTL 그 융합 몬스터 1장을 EX덱에서 융합 소환한다.
PRINTL 이 카드의 효과 처리 시에 자신의 필드 묘지에 「CAN：D」가 존재하고 있었을 경우,
PRINTL 또한 자신은 2000 라이프 포인트를 회복하고, 묘지의 「CAN：D」 몬스터 1장을 패에 추가한다.


@CARDCAN_73331(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73331)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73331)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL RESETAREA_MAT
	素材範囲손패 = 1
	素材範囲필드 = 1
	素材範囲묘지 = 1
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 융합
	효과条件종족 = 사이킥족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73331(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73331,1)
	IF RESULT == 0
		;通常마법汎用발동後묘지送り処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲묘지 = 1
효과条件名前 = CAN：D
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 = 1
		CALL RESETAREA_MAT
		素材範囲손패 = 1
		素材範囲필드 = 1
		素材範囲묘지 = 1
		CALL FULLRESET
		효과条件종류 = 융합
		효과条件종족 = 사이킥족
		효과条件소환可 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
			CALL FUSSUMMON(듀얼리스트, 選択カード,덱行き)
			IF 条件달성 == 1
				CALL LIFE_CHANGE(듀얼리스트,"増加","コスト",2000)
				CALL FULLRESET
				효과条件종류 = モン全般
				효과条件カテゴリ = CAN：D
				CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF
