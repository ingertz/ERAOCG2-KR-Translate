@CARDNAME_1619,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 알루어 퀸
	CASE "略称"
RESULTS = 매혹의 여왕
	CASE "カテゴリ"
RESULTS:0 = 매혹의 여왕
RESULTS:1 = Lv
ENDSELECT


@CARD_1619(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1619(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 상대 필드 위의 레벨 3 이하의 몬스터 1장을 선택하여, 장착 카드로 이 카드에 장착한다.
PRINTL ②: 이 카드가 파괴될 경우,
PRINTL 대신에 이 카드의 효과로 장착한 카드를 묘지로 보낼 수 있다.
CALLFORM CARD_ABLE(0,ターン開始時)
PRINTL ③: 자신 스탠바이 페이즈에 발동할 수 있다.
PRINTL 이 카드는 성장한다.

@CARDCAN_1619(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1619)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,1619)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 골든 알뤼르 퀸
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1 && ターンプレイヤー != 듀얼리스트
		RETURN 0
	
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALLFORM 효과条件レベル_ADV(3, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_1619(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1619,1)
	IF RESULT == 0
		CALL FULLRESET
		CALLFORM 효과条件レベル_ADV(3, "以下")
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_EQUIP_MONSTER(듀얼리스트,場所,"상대필드",選択カード,(4))
		ENDIF
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1619)
	IF RESULT == 0
		CALL CARD_RECONSTRUCT(듀얼리스트,"필드",場所,11619)
	ENDIF
ENDIF


