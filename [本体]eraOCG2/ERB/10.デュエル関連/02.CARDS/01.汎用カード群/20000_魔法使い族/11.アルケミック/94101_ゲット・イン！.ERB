@CARD_EXPLANATION_94101(종류)
#DIM DYNAMIC 종류
CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL 이 카드는 자신 필드에 L 몬스터가 존재하지 않는 경우에 발동할 수 있다.
PRINTL ①: 자신 필드의 몬스터 1장과, 상대 필드의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드들을 덱으로 되돌리고, EX덱에서 '아카식' L 몬스터 1장을 특수 소환한다.


@CARDCAN_94101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,94101)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,94101)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF モンスター数(対面者) == 0 && 마함数(対面者) == 0 
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 링크
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_94101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,94101,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_DISPOSE(듀얼리스트,"필드",選択カード,2)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"덱")
			CALL FULLRESET
			효과範囲EX덱 = 1
			효과条件カテゴリ = 아카식
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)	
		ENDIF		
	ENDIF
ENDIF
