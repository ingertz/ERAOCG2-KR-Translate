@CARDNAME_92052,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 알터가이스트 프로토콜
	CASE "略称"
RESULTS = 오지 프로토콜
	CASE "カテゴリ"
RESULTS:0 = 알터가이스트
ENDSELECT


@CARD_92052(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속함정
ENDSELECT

RETURN 0
@CARD_EXPLANATION_92052(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드가 마법 & 함정 존에 존재하는 한,
PRINTL 자신의 "알터가이스트" 효과에 대해 상대는 카드의 효과를 발동할 수 없다.
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②: 상대가 몬스터의 효과를 발동했을 때,
PRINTL 이 카드 이외의 자신 필드의 앞면 표시의 "알터가이스트" 카드 1장을 묘지로 보내고 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.

@CARDCAN_92052(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC EX条件달성


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92052)
SIF RESULT == 1
	RETURN 0



IF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0

	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,92052)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 얼터가이스트
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92052(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 素材数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 설정종족
#DIM DYNAMIC 選択효과
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92052)
	IF RESULT == 0
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_CAT_SOGAI_INFO(듀얼리스트,"얼터가이스트")
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92052,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 얼터가이스트
		효과条件他所 = 1	
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF
