@CARDNAME_23555,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 주바바 반쵸-가가가 코트
	CASE "略称"
RESULTS = 주바바 반쵸
	CASE "カテゴリ"
RESULTS:0 = 주바바
RESULTS:1 = 가가가
RESULTS:2 = 오노마토
ENDSELECT


@CARD_23555(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_23555(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 "주바바 반쵸-GC" 이외의 "오노마토" 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 패의 이 카드를 특수 소환한다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 자신 묘지의 "오노마토" 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 특수 소환한다.
PRINTL 이 효과의 발동 후, 턴 종료 시까지 자신은 엑시즈 몬스터밖에 엑스트라 덱에서 특수 소환할 수 없다.
PRINTL 

@CARDCAN_23555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23555)
SIF RESULT == 1
	RETURN 0
IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,23555)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,23555,"손패")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 오노마토
	효과条件固名前 = 주바바 반쵸-가가가 코트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,23555)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 오노마토
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23555,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23555,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 오노마토
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		EX소환可能종류 = 엑시즈
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF


