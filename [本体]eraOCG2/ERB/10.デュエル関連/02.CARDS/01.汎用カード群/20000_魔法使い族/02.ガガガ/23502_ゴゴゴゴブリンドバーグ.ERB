@CARDNAME_23502,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 고고고고블린도버그
	CASE "略称"
RESULTS = 고고고고블린도
	CASE "カテゴリ"
RESULTS:0 = 고고고
RESULTS:1 = 고블린
RESULTS:2 = 오노마토
ENDSELECT


@CARD_23502(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250426
ENDSELECT

RETURN 0

@CARD_EXPLANATION_23502(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,소환時)
PRINTL ①: 이 카드가 소환했을 때에 발동할 수 있다.
PRINTL 자신의 패 / 덱 / 묘지에서 전사족 이외의 「고고고」 몬스터 1장을 특수 소환한다.
PRINTL 이 카드가 공격 표시일 경우, 또한, 이 카드는 수비 표시가 된다.
PRINTL 이 효과의 발동 후, 턴 종료시까지 자신은 엑시즈 몬스터밖에 엑스트라 덱에서 특수 소환할 수 없다.
CALLFORM CARD_ABLE(2,X素材時효과)
PRINTL ②: 이 카드가 엑시즈 소환의 소재가 되었을 경우에 발동할 수 있다.
PRINTL 덱에서 「도도도」 몬스터 1장을 패에 넣는다.


@CARDCAN_23502(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23502)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	SIF 발동済_1(듀얼리스트,23502)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲묘지 = 1
	CALL 효과条件종족_ADV(전사족,"以外")
	효과条件カテゴリ = 고고고
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == X素材時효과
	SIF 발동済_2(듀얼리스트,23502)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 도도도
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23502(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23502,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 고고고
		CALL 효과条件종족_ADV(전사족,"以外")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",2,1)
		EX소환可能종류 = 엑시즈
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == X素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23502,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 도도도
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

