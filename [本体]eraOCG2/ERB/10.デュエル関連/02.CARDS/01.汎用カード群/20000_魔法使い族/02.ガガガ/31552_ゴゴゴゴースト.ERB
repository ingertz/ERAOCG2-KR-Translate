@CARDNAME_31552,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 고고고고-스트
	CASE "略称"
RESULTS = 고고고고-스트
	CASE "カテゴリ"
RESULTS:0 = 오노마토
RESULTS:1 = 고고고
ENDSELECT


@CARD_31552(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 언데드족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1900
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_31552(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,특수소환時)
PRINTL ①: 이 카드가 특수 소환에 성공했을 때, 자신 묘지의 「고고고」 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 수비 표시로 특수 소환한다.
PRINTL 

@CARDCAN_31552(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,31552)
SIF RESULT == 1
	RETURN 0


IF 종류 == 특수소환時
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 고고고
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_31552(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,31552)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 고고고
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
