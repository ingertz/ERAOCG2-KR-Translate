@CARDNAME_23554,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 주바바바나이트
	CASE "略称"
RESULTS = 주바바바나이트
	CASE "カテゴリ"
RESULTS:0 = 주바바
RESULTS:1 = 오노마트
ENDSELECT


@CARD_23554(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 900
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250426
ENDSELECT

RETURN 0

@CARD_EXPLANATION_23554(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,소환時,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 "주바바바나이트" 이외의 "주바바" 몬스터 1장을 패에 넣을 수 있다.
PRINTL 이 카드의 레벨은, 이 효과로 패에 넣은 몬스터와 같아진다.
PRINTL 패에 넣지 않았을 경우, 상대 필드의 레벨 4 이하의 수비 표시 몬스터 1장을 파괴할 수 있다.
CALLFORM CARD_ABLE(2,X素材時효과)
PRINTL ②: 이 카드가 엑시즈 소환의 소재로 사용되었을 경우에 발동할 수 있다.
PRINTL 덱에서 "가가가" 몬스터 1장을 패에 넣는다.


@CARDCAN_23554(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23554)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,23554)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 주바바
	효과条件固名前 = 주바바바나이트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == X素材時효과
	SIF 발동済_2(듀얼리스트,23554)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 가가가
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23554(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23554,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 주바바
		효과条件固名前 = 주바바바나이트
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			TRYCALLFORM CARD_{選択カード_ID},"レベル"
			選択カード = RESULT
			CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"永星",選択カード,1)
		ELSE
			CALL FULLRESET
			효과条件표시 = 守備표시
			CALLFORM 효과条件レベル_ADV(4,"以下")
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")
		ENDIF
	ENDIF
ELSEIF 종류 == X素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23554,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 가가가
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF


