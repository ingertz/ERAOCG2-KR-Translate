#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 대지의 기사 가이아 나이트
	CASE "略称"
RESULTS = 가이아 나이트
	CASE "カテゴリ"
RESULTS:0 = 기사 가이아
ENDSELECT


@CARD_51503(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 800
	CASE "分割"
		RETURN 通常モン
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

