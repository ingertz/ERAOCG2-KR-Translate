@CARDNAME_72345,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = U.A. 플래그십 딜
	CASE "略称"
RESULTS = 플래그십 딜
	CASE "カテゴリ"
RESULTS:0 = 울트라 애슬리트
ENDSELECT


@CARD_72345(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250618
ENDSELECT
RETURN 0

@CARD_EXPLANATION_72345(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 1턴에 1번만 사용할 수 있다.
CALLFORM CARD_ABLE(1,손패起動,마함起動)
PRINTL ①: 덱에서 "U.A." 몬스터 1장을, 효과를 무효로 하고 특수 소환한다.
PRINTL (이 효과로 특수 소환한 몬스터는 특수 소환 소재로 할 수 없다.)
PRINTL 그 후, 자신은 그 몬스터의 레벨 × 300의 LP를 잃는다.
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②: 묘지의 이 카드를 제외하고, 자신 묘지의 "U.A." 몬스터 3장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 덱으로 되돌리고, 자신은 덱에서 1장 드로우한다.


@CARDCAN_72345(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72345)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
    SIF 발동済_1(듀얼리스트,72345)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
효과条件カテゴリ = 울트라 애슬리트
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
    SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 발동済_2(듀얼리스트,72345)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지= 1
효과条件カテゴリ = 울트라 애슬리트
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 2
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1

ENDIF

@CARDEFFECT_72345(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC ダメージ量

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72345,1)
	IF RESULT == 0
	    CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	    CALL FULLRESET
		효과範囲덱 = 1
효과条件カテゴリ = 울트라 애슬리트
		효과条件종류 = モン全般
		효과추가종류:0 = 効追無効
		효과추가종류:1 = 効追永材
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
		    CALLFORM CARD_{RESULT:1},"レベル"
		    CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",RESULT*300)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72345)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件종류 = モン全般
효과条件カテゴリ = 울트라 애슬리트
		효과条件複数 = 3
		효과条件行先플래그 = 2
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,1)
		ENDIF
	ENDIF
ENDIF


