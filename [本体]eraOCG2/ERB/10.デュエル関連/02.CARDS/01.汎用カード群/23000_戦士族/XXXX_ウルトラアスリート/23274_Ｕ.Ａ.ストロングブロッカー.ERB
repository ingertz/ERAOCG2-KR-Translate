@CARDNAME_23274,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = U.A. 스트롱 블로커
	CASE "略称"
RESULTS = 스트롱 블로커
	CASE "カテゴリ"
RESULTS:0 = 울트라 애슬리트
ENDSELECT


@CARD_23274(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 효과モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 2700
	;牝性/雄性があるよ
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250618
ENDSELECT
RETURN 0

@CARD_EXPLANATION_23274(종류)
#DIM DYNAMIC 종류


PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드명 이외의 자신 필드의 「U.A.」 몬스터 1장을 패로 되돌리고, 이 카드를 패에서 특수 소환한다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 상대가 몬스터를 특수 소환했을 경우에 발동할 수 있다.
PRINTL 그 몬스터의 효과를 무효로 하고, 수비 표시로 한다.


@CARDCAN_23274(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23274)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF ゾーン != "손패"
		RETURN 0
	SIF 발동済_1(듀얼리스트,23274)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,23274,"손패",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件固名前 = U.A. 스트롱 블로커
효과条件カテゴリ = 울트라 애슬리트
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
    SIF ゾーン != "필드"
        RETURN 0
    SIF 발동済_2(듀얼리스트,23274)
        RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"특수소환",1)
	SIF RESULT != 1
		RETURN 0

	SIF FINAL_SUMMON_TARGET(듀얼리스트,1) == 5 
		RETURN 0

	RETURN 1


ENDIF

@CARDEFFECT_23274(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 효과先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23274,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
효과条件固名前 = U.A. 스트롱 블로커
효과条件カテゴリ = 울트라 애슬리트
		효과条件行先플래그 = 손패行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 효과後誘発
    CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23274,2)
	IF RESULT == 0
	    효과先 = FINAL_SUMMON_TARGET(듀얼리스트,1)
	    CALL FULLRESET
		효과추가종류 = 5
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",효과先,"표시변경")
	ENDIF


ENDIF
