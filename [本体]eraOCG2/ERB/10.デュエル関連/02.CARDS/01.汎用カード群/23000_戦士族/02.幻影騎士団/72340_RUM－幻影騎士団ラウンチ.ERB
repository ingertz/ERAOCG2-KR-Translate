@CARD_EXPLANATION_72340(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,마함起動 ,손패起動 ,フリー誘発)
PRINTL ①: 자신 / 상대의 메인 페이즈에, 자신 필드의 X 소재가 없는 어둠 속성 X 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 자신의 몬스터보다 랭크가 1개 높은 어둠 속성 X 몬스터 1장을, 대상 몬스터 위에 겹쳐서 X 소환으로 취급하여 EX덱에서 특수 소환한다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 자신 메인 페이즈에 묘지의 이 카드를 제외하고, 자신 필드의 어둠 속성 X 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터의 X 소재를 1개 늘린다.



@CARDCAN_72340(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72340)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 어둠속성
	효과条件종류 = 엑시즈
	효과条件Ｘ素所持 = 888
	효과条件RUM = 72380
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,72340)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件속성 = 어둠속성
	효과条件종류 = 엑시즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_72340(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 설정レベル

#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードB
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72340)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		;通常마법汎用処理
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件종류 = 엑시즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			설정レベル = GETVAR(@"%듀얼리스트%_MZ_星:選択カード") + 1
			CALL FULLRESET
			효과条件レベル = 설정レベル
			효과条件속성 = 어둠속성
			효과条件종류 = 엑시즈
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
			選択カードB = RESULT
			;戻る
			IF 選択カードB != 9999
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カードB}")}(듀얼리스트,選択カードB)
				CALL RESETCONDITION_MAT(1)
				代替素材数 = 1
				重ねてＸ = 1
				素材条件固定 = 選択カード
				TRYCALLFORM XYZSUMMON(듀얼리스트,選択カードB)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72340,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件종류 = 엑시즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		SIF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"Ｘ",1)
	ENDIF
ENDIF



@CARDRUM_72340(듀얼리스트,場所,カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カードID
#DIM DYNAMIC 설정カードID
#DIM DYNAMIC 素材レベル
VARSET RESULT

素材レベル = GETVAR(@"%듀얼리스트%_MZ_星:場所") + 1

FOR カウンタ,0,15
	설정カードID =  GETVAR(@"%듀얼리스트%_EX덱:カウンタ")
	SIF 설정カードID == -1 
		CONTINUE
	TRYCALLFORM CARD_{설정カードID}, "종류"
	SIF RESULT != 엑시즈
		CONTINUE
	TRYCALLFORM CARD_{설정カードID}, "속성"
	SIF RESULT != 어둠속성
		CONTINUE		
	TRYCALLFORM CARD_{설정カードID}, "レベル"
	SIF RESULT != 素材レベル
		CONTINUE

	VARSET RESULT
	RETURN 1
NEXT
	
VARSET RESULT
RETURN 0