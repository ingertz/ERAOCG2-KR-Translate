@CARDCAN_1621(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1621)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과 || 종류 == 常在無効
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1621(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 減少値
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과

	CALL FULLRESET
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件종족:2 = 비늘야수족
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦無",0,1621)

	CALL FULLRESET
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件종족:2 = 비늘야수족
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"戦無",0,1621)

ELSEIF 종류 == 常在無効
	CALL FULLRESET
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件종족:2 = 비늘야수족
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無効",0,1621)

	CALL FULLRESET
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件종족:2 = 비늘야수족
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効",0,1621)
ENDIF



