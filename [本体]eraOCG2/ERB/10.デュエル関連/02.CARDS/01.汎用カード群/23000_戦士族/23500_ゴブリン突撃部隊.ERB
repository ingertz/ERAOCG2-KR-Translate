@CARDCAN_23500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23500)
SIF RESULT == 1
	RETURN 0
IF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_戦:場所") == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23500(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 攻撃後強制誘発
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",2,1)
ENDIF

