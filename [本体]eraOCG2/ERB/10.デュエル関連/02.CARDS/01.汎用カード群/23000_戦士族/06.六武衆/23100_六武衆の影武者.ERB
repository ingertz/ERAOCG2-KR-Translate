@CARDNAME_23100,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 여섯무사의 그림자 무사
	CASE "略称"
RESULTS = 여섯무사의그림자
	CASE "カテゴリ"
RESULTS:0 = 여섯무사
ENDSELECT


@CARD_23100(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 400
	CASE "守備力"
		RETURN 1800
	CASE "튜너"
		RETURN 1
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_23100(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드 이외의 자신 필드의 "여섯무사" 몬스터는 대상 내성을 얻는다.
PRINTL 

@CARDCAN_23100(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23100)
SIF RESULT == 1
	RETURN 0


IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23100(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 여섯무사
	효과条件他所 = 1
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
ENDIF


