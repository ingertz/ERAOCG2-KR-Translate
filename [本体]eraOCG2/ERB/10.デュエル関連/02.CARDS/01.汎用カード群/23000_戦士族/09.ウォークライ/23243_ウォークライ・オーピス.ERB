@CARDNAME_23243,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 워크라이 오피스
	CASE "略称"
RESULTS = 워크라이 오피스
	CASE "カテゴリ"
RESULTS:0 = 워크라이
ENDSELECT


@CARD_23243(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2100
	CASE "守備力"
		RETURN 2100
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_23243(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,효과소환)
PRINTL ①: 자신 필드의 몬스터가 존재하지 않는 경우 또는 전사족 몬스터뿐인 경우,
PRINTL 이 카드는 릴리스 없이 소환할 수 있다.
CALLFORM CARD_ABLE(2,攻撃時誘発)
PRINTL ②: 자신의 전사족 땅 속성 몬스터가 전투를 실행할 경우에 발동할 수 있다.
PRINTL 덱에서 「워크라이 오피스」 이외의 「워크라이」 카드 1장을 묘지로 보낸다.
PRINTL 그 후, 자신 필드의 모든 「워크라이」 몬스터의 공격력은 200 올린다.


@CARDCAN_23243(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23243)
SIF RESULT == 1
	RETURN 0
IF 종류 == 효과소환
	SIF モンスター数(듀얼리스트) == 0
		RETURN 1

	CALL FULLRESET
	효과範囲필드 = 1
	CALLFORM 효과条件종족_ADV(전사족, "以外")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 0
		RETURN 0

	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	SIF 발동済_2(듀얼리스트,23243)
		RETURN 0	
	SIF ゾーン != "필드"
		RETURN 0
	CALLFORM ATTACK_CARD(듀얼리스트,"종족")
	SIF !MATCH(RESULT,전사족)
		RETURN 0
	CALLFORM ATTACK_CARD(듀얼리스트,"속성")
	SIF !MATCH(RESULT,땅속성)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 워크라이
	효과条件固名前 = 워크라이 오피스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23243(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
IF 종류 == 효과소환
	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23243,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 워크라이
		효과条件固名前 = 워크라이 오피스
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)

		CALL FULLRESET
		효과条件カテゴリ = 워크라이
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"永攻",200)

	ENDIF
ENDIF


