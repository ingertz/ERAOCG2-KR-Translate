VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 종말의 기사
	CASE "略称"
RESULTS = 종말의 기사
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_23000(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1200
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

