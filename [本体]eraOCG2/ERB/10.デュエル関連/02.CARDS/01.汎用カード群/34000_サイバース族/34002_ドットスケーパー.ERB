@CARDCAN_34002(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34002)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_1(듀얼리스트,34002)
		RETURN 0
	SIF 발동済_D1(듀얼리스트,34002)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,34002,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
효과条件名前 = 도트 스케이퍼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 제외時
	SIF 발동済_1(듀얼리스트,34002)
		RETURN 0
	;SIF 발동済_D2(듀얼리스트,34002)
	;	RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,34002,"제외")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
효과条件名前 = 도트 스케이퍼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34002(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード


IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34002,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 도트 스케이퍼
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT == 1
			CALL CARD_EFFECT_D1USE(듀얼리스트,34002)
	ENDIF

ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34002,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
효과条件名前 = 도트 스케이퍼
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
