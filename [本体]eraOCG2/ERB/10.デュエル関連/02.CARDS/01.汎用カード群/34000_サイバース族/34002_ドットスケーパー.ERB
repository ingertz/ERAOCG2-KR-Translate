@CARDNAME_34002,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 도트 스케이퍼
	CASE "略称"
RESULTS = 도트 스케이퍼
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_34002(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 2100
ENDSELECT

RETURN 0

