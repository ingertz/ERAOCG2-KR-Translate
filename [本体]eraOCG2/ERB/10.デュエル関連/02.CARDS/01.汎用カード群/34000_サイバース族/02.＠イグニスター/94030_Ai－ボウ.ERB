@CARDNAME_94030,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = Ai－보우
	CASE "略称"
RESULTS = Ai－보우
	CASE "カテゴリ"
RESULTS:0 = Ai
ENDSELECT


@CARD_94030(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0


@CARD_EXPLANATION_94030(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,フリー誘発,발동誘発)
PRINTL ①: 자신 필드의 원래 공격력이 2300인 사이버스족 몬스터의 수만큼,
PRINTL 상대 필드의 앞면 표시 카드의 효과를 턴 종료시까지 무효로 한다.
;審議中
PRINTL 상대의 일반 마법, 속공 마법, 일반 함정에 체인하여 발동했을 경우, 그 카드의 발동을 포함하여 무효로 한다.
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②: 이 카드를 묘지에서 제외하고 발동할 수 있다.
PRINTL 자신의 제외되어 있는 사이버스족 몬스터 1장을 특수 소환한다.



@CARDCAN_94030(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,94030)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,94030)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件元固攻撃力 = 2300
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 10
	효과条件除去種 = 無効
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,94030)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
;審議中
ELSEIF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,94030)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件元固攻撃力 = 2300
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류:0 = 通常마법
	효과条件종류:1 = 速攻마법
	효과条件종류:2 = 通常함정
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,94030)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件종족 = 사이버스족
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_94030(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과횟수
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,94030,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件元固攻撃力 = 2300
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		효과횟수 = RESULT
		CALL FULLRESET
		효과条件표시 = 10
		효과条件複数 = 100 - 효과횟수
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"無効")
	ENDIF
;審議中
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,94030,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件元固攻撃力 = 2300
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		효과횟수 = RESULT - 1
		CALL FULLRESET
		효과条件표시 = 10
		효과条件複数 = 100 - 효과횟수
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"無効")
		
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",0,"묘지")
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,94030,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件종족 = 사이버스족
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
