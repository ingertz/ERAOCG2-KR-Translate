#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 5

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,1633) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_1633(종류)
#DIM DYNAMIC 종류

PRINTL 레벨 5 몬스터 2개
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 이 카드의 X 소재를 1개 제거하고, 자신 또는 상대의 묘지의 레벨 5 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 자신 또는 상대의 필드에 특수 소환한다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 몬스터 존에 존재하는 상태에서,
PRINTL 자신 필드의 레벨 5 몬스터가 필드에서 벗어났을 경우에 발동할 수 있다.
PRINTL 덱에서 레벨 5 몬스터 1장을 고르고, 패에 넣거나 특수 소환한다.


@CARDCAN_1633(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1633)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,1633)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5 && モンスター数(対面者) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件レベル = 5
	효과条件종류 = レベル持ち
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,1633)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件レベル = 5
	효과条件종류 = レベル持ち
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"필드離れ")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件レベル = 5
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_1633(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択肢


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1633,1,X消費1)
	IF RESULT == 0
		IF モンスター数(듀얼리스트) < 5 && モンスター数(対面者) < 5
PRINTL 자신 필드에 특수 소환하시겠습니까?
			CALL YN_CHOICE(듀얼리스트)
			選択肢 = RESULT
		ELSEIF モンスター数(듀얼리스트) < 5
			選択肢 = はい
		ELSE
			選択肢 = いいえ
		ENDIF

		
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件レベル = 5
		효과条件종류 = レベル持ち
		IF 選択肢 == はい
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ELSE
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"",0,1)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1633,2)
	IF RESULT == 0
		IF モンスター数(듀얼리스트) != 5
PRINTL 특수 소환하시겠습니까?
			CALL YN_CHOICE(듀얼리스트)
			IF RESULT == 0
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件レベル = 5
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ELSEIF RESULT == 1
				CALL FULLRESET
				효과条件レベル = 5
				CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
			ENDIF
		ELSE
			CALL FULLRESET
			효과条件レベル = 5
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF