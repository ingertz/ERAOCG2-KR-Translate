@CARDNAME_1633,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 소황녀 퀸쿼리
	CASE "略称"
RESULTS = 퀸쿼리
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_1633(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 2800
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_1633(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
