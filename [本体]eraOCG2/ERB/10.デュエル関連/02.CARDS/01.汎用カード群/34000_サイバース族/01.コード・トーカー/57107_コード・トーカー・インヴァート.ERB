@CARDNAME_57107,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 코드 토커 인버트
	CASE "略称"
RESULTS = 코드 인버트
	CASE "カテゴリ"
RESULTS:0 = 코드 토커
ENDSELECT


@CARD_57107(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1300
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0
@CARDSUMMON_57107(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족 = 사이버스족

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,57107) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_57107(종류)
#DIM DYNAMIC 종류

PRINTL 사이버스족 몬스터 2체
PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 링크 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 패에서 사이버스족 몬스터 1장을 자신 필드에 특수 소환한다.


@CARDCAN_57107(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57107)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,57107)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종족 = 사이버스족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_57107(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択レベル
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 속성セット
#DIM DYNAMIC 속성セットビット

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57107,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종족 = 사이버스족
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
