@CARDNAME_34101,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 코드 제너레이터
	CASE "略称"
RESULTS = 코드 젠
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_34101(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1300
	CASE "守備力"
		RETURN 500
ENDSELECT

RETURN 0

@CARD_EXPLANATION_34101(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 사이버스족 몬스터가 존재할 경우,
PRINTL 엑스트라 덱의 「코드 토커」 몬스터 1장을 공개하고 발동할 수 있다.
PRINTL 패의 이 카드를 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드가 몬스터 존에 앞면 표시로 존재하는 한, 자신은 사이버스족 몬스터밖에 특수 소환할 수 없다.

CALLFORM CARD_ABLE(2,L素材時효과)
PRINTL ②: 이 카드가 링크 소재로서 묘지로 보내져,
PRINTL 자신 필드에 「코드 토커」 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 덱에서 공격력 1200 이하의 사이버스족 몬스터 1장을 묘지로 보낸다.
PRINTL 자신 필드에 링크 3 이상의 몬스터가 존재할 경우, 묘지로 보내지 않고 패에 넣을 수도 있다.

@CARDCAN_34101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34101)
SIF RESULT == 1
	RETURN 0
IF 종류 == 손패起動

	SIF 발동済_1(듀얼리스트,34101)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,34101,"손패")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 코드 토커
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이버스족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == L素材時효과
	SIF 발동済_2(듀얼리스트,34101)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 코드 토커
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 사이버스족
	효과条件攻撃力 = 1200
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_汎数:場所") == 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34101(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34101,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 코드 토커
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL FULLRESET
			효과추가종류 = 効追汎数
			CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_TMP_SS_RST(듀얼리스트,"종족",사이버스족)
ELSEIF 종류 == L素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34101,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = 링크
		CALL 효과条件レベル_ADV(3, "以上")
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			PRINTFORML 덱에서 攻撃力１２００이하의 사이버스족モンスター１体を…
			CALL YN_CHOICE(듀얼리스트,"손패に加える","묘지へ送る")
		ELSE
			RESULT = いいえ
		ENDIF
		IF RESULT == はい
			CALL FULLRESET
			효과条件종족 = 사이버스족
			효과条件攻撃力 = 1200
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ELSEIF RESULT ==いいえ
			CALL FULLRESET
			효과条件종족 = 사이버스족
			효과条件攻撃力 = 1200
			CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
