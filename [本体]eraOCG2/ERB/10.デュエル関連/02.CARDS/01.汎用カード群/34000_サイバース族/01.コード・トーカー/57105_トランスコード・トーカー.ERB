@CARDNAME_57105,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 트랜스코드 토커
	CASE "略称"
RESULTS = 트랜스코드
	CASE "カテゴリ"
RESULTS:0 = 코드 토커
ENDSELECT


@CARD_57105(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2300
	CASE "守備力"
		RETURN 1300
ENDSELECT

RETURN 0
@CARDSUMMON_57105(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 3
素材条件종류 = 효과全般

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,57105) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_57105(종류)
#DIM DYNAMIC 종류

PRINTL 효과 몬스터 2체
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,攻守変動,지속효과)
PRINTL ①: 자신 필드에 이 카드 이외의 링크 몬스터가 존재할 경우,
PRINTL 자신 필드의 링크 몬스터의 공격력은 500 올리고, 대상 내성을 얻는다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 자신 메인 페이즈에 발동할 수 있다.
PRINTL "트랜스코드 토커"를 제외한, 자신 묘지의 링크 3 이하의 사이버스족 링크 몬스터 1장을 자신 필드에 공격 표시로 특수 소환한다.
PRINTL (이 효과를 발동하는 턴, 자신은 사이버스족 몬스터밖에 특수 소환할 수 없다).


@CARDCAN_57105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57105)
SIF RESULT == 1
	RETURN 0


IF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 링크
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,57105)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	CALL 효과条件종족_ADV(사이버스족, "以外")
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"SS이력")
	SIF RESULT != 0
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件固名前 = 트랜스코드 토커
	효과条件종류 = 링크
	CALL 효과条件レベル_ADV(3, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_57105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択レベル
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 속성セット
#DIM DYNAMIC 속성セットビット

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻守変動
	CALL FULLRESET
	효과条件종류 = 링크
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500)
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件종류 = 링크
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐",3)
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57105,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件固名前 = 트랜스코드 토커
		효과条件종류 = 링크
		효과条件종족 = 사이버스족
		CALL 효과条件レベル_ADV(3,"以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"攻撃표시")
		SS소환可能종족 = 사이버스족
		CALL CARD_SS_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF
