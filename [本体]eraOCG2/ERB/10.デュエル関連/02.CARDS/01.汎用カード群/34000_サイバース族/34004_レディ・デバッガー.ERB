@CARDNAME_34004,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 레이디 디버거
	CASE "略称"
RESULTS = L 디버거
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_34004(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1700
	CASE "守備力"
		RETURN 1400
ENDSELECT

RETURN 0

@CARD_EXPLANATION_34004(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,소환時 ,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 레벨 3 이하의 사이버스족 몬스터 1장을 패에 넣는다.


@CARDCAN_34004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34004)
SIF RESULT == 1
	RETURN 0


IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,34004)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 사이버스족
	CALL 효과条件レベル_ADV(3, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード


IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34004,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 사이버스족
		CALL 효과条件レベル_ADV(3, "以下")
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
