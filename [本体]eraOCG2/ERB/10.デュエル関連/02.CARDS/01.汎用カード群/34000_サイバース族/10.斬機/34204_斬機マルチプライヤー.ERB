@CARDNAME_34204,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 참기 멀티플라이어
	CASE "略称"
RESULTS = 참기 멀티
	CASE "カテゴリ"
RESULTS:0 = 참기
ENDSELECT


@CARD_34204(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

