@CARDCAN_93420(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 最大링크値
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93420)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,93420)
		RETURN 0

	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT >= 3 
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 참기
	효과条件종류 = モン全般
	효과条件소환可 = 1	
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 3
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_93420(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード2
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択링크値
#DIM DYNAMIC 最大링크値
#DIM DYNAMIC 소환カード
#DIM DYNAMIC モンスター数,5
#DIM DYNAMIC SS可能数
#DIM DYNAMIC 소환カードID

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93420,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		FOR カウンタ,0,3
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 참기
			효과条件종류 = モン全般
			효과추가종류 = 効追無効
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		NEXT
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 참기
		효과条件종류:0 = 싱크로
		효과条件종류:1 = 엑시즈
		효과条件소환可 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트, "EX덱",ゾーン,場所)	
		소환カード = RESULT
		IF 소환カード != 9999
			소환カードID = RESULT:99
			CALLFORM CARD_{소환カードID}, "종류"
			IF RESULT == 싱크로
				TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
				CALL SYNSUMMON(듀얼리스트, 소환カード)	
			ELSEIF RESULT == 엑시즈
				TRYCALLFORM CARDSUMMON_{소환カードID}(듀얼리스트,소환カード)
				CALL XYZSUMMON(듀얼리스트, 소환カード)
			ENDIF
		ENDIF
	ENDIF
ENDIF
