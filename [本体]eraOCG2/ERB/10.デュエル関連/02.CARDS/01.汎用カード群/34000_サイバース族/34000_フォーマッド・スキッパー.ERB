@CARDNAME_34000,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 포맷 스키퍼
	CASE "略称"
RESULTS = 스키퍼
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_34000(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARD_EXPLANATION_34000(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,필드起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL EX덱의 링크 몬스터 1장을 상대에게 보여준다.
PRINTL 이 카드는 보여준 몬스터와 같은 카테고리명 종족 속성의 소재 정보를 얻는다.
CALL CARD_ABLE(2,L素材時효과)
PRINTL ②: 이 카드가 링크 소재로서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 덱에서 레벨 5 이상의 사이버스족 몬스터 1장을 패에 넣는다.


@CARDCAN_34000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34000)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動

	SIF 발동済_1(듀얼리스트,34000)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	RETURN 1
ELSEIF 종류 == L素材時효과
	SIF 발동済_2(듀얼리스트,34000)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 사이버스족
	CALL 효과条件レベル_ADV(5, "以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34000,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 링크
		CALL CARD_CHOOSE_BASIC(듀얼리스트, "EX덱",ゾーン,場所)	
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_EX덱:選択カード")}, "カテゴリ"
			CALL CARD_SET_CATEGORY(듀얼리스트,"필드",場所,RESULTS)
			TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_EX덱:選択カード")}, "종족"
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永種",RESULT,1)
			TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_EX덱:選択カード")}, "속성"
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永属",RESULT,1)
		ENDIF
	ENDIF

ELSEIF 종류 == L素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34000,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종족 = 사이버스족
		CALL 효과条件レベル_ADV(5, "以上")
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
