@CARDNAME_1212,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 마린세스 마블드 록
	CASE "略称"
RESULTS = M 록
	CASE "カテゴリ"
RESULTS:0 = 마린세스
ENDSELECT


@CARD_1212(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
ENDSELECT


RETURN 0


