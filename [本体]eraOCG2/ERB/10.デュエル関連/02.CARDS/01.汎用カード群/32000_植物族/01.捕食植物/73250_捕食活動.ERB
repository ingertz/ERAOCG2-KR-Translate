@CARDNAME_73250,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 프레데터 프랙티스
	CASE "略称"
RESULTS = 포식 활동
	CASE "カテゴリ"
RESULTS:0 = 프레데터 플랜츠
ENDSELECT


@CARD_73250(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73250(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 카드는 1턴에 1번밖에 발동할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①: 패에서 "프레데터 플랜츠" 몬스터 1장을 특수 소환한다.
PRINTL 그 후, 덱에서 "프레데터 프랙티스" 이외의 "프레데터" 카드 1장을 패에 넣는다.
PRINTL 이 카드를 발동한 후, 턴 종료시까지 자신은 융합 몬스터밖에 EX덱에서 특수 소환할 수 없다.

@CARDCAN_73250(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73250)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73250)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 프레데터 플랜츠
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 프레데터 플랜츠
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73250(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73250,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 프레데터 플랜츠
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = 프레데터 플랜츠
			효과条件固名前 = 프레데터 프랙티스
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
		EX소환可能종류 = 융합
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF