@CARDNAME_32401,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 프레데터 플랜츠 달링 코브라
	CASE "略称"
RESULTS = 포식 D 코브라
	CASE "カテゴリ"
RESULTS:0 = 프레데터 플랜츠
ENDSELECT


@CARD_32401(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1500
ENDSELECT

RETURN 0

