CALLFORM CARD_ABLE(20,특수소환時)
PRINTL ①: 이 카드가 특수 소환에 성공했을 때 발동할 수 있다.
PRINTL 덱에서 '융합' 마법 카드 1장을 패에 넣는다.
PRINTL 

@CARDCAN_32401(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,32401)
SIF RESULT == 1
	RETURN 0

IF 종류 == 특수소환時
	SIF 발동済_D1(듀얼리스트,32401)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = 마법全般
	효과条件カテゴリ = 융합
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_32401(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32401,20)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 마법全般
		효과条件カテゴリ = 융합
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF

