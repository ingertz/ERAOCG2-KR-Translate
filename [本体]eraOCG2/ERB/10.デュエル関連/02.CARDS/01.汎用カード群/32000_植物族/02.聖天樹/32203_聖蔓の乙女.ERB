@CARDNAME_32203,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 생바인 메이든
	CASE "略称"
RESULTS = 성만의 을녀
	CASE "カテゴリ"
RESULTS:0 = 생아발론
RESULTS:1 = 생바인
ENDSELECT




@CARD_32203(参照先)
;名前専用
#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_32203(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: EX덱에서 특수 소환된 식물족 몬스터가 자신 필드에 존재하고,
PRINTL 마법 / 함정 / 몬스터의 효과를 상대가 발동했을 때 발동할 수 있다.
PRINTL 자신의 EX덱에서 특수 소환된 식물족 몬스터는 전부 다음 페이즈까지 대상 내성을 얻는다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②: 이 카드가 몬스터 존에 존재하는 한,
PRINTL 자신 필드의 "생아발론" 마법 / 함정 카드는 상대 효과의 대상이 되지 않는다.

@CARDCAN_32203(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,32203)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,32203)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,32203,"손패")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 식물족
	효과条件召種= 召種EX덱
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_32203(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32203,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 식물족
		효과条件召種= 召種EX덱
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐",5)
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 생아발론
	효과条件종류 = 마함全般
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
ENDIF
