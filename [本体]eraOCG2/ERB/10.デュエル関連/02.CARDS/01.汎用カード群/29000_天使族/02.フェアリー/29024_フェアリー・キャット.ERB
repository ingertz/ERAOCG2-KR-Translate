@CARDNAME_29024,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS
;캣ちゃんモチ-フかつ、ギラグとのタッグマッチ戦をモチ-フに
;RUM취득はこっちに이동
SELECTCASE 参照先
	CASE "名前"
RESULTS = 페어리 캣
	CASE "略称"
RESULTS = F 캣
	CASE "カテゴリ"
RESULTS:0 = 페어리
RESULTS:1 = 캣
ENDSELECT


@CARD_29024(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 1600
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29024(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드가 필드에 존재하는 한, 상대 몬스터는 표시 형식을 변경할 수 없다.
CALL CARD_ABLE(1,손패起動)
PRINTL ②: 자신 필드에 레벨 3 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.
CALL CARD_ABLE(2,X素材時효과)
PRINTL ③: 이 카드가 엑시즈 소재로 사용되었을 경우에 발동할 수 있다,
PRINTL 덱에서 「RUM」 카드 1장을 덱의 위에 놓는다.

@CARDSUMMON_AA_29024


@CARDCAN_29024(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과使用불가,3

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29024)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,29024)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,29024,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件レベル = 3
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == X素材時효과
	SIF 발동済_2(듀얼리스트,29024)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = RUM
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF
@CARDEFFECT_29024(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 효과使用불가,3

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29024,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"表固")
ELSEIF 종류 == X素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29024,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = RUM
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_ADD_DECK_SPC(듀얼리스트,"덱",選択カード)
		ENDIF
	ENDIF
ENDIF


