;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 페어리 치어 걸
	CASE "略称"
RESULTS = F 치어 걸
	CASE "カテゴリ"
RESULTS:0 = 페어리
ENDSELECT


@CARD_54560(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1900
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
@CARDSUMMON_54560(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 4
素材条件종족 = 천사족

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,54560) 
	RETURN RESULT
ENDIF



