@CARDNAME_11230,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 트릭스터- 노블 엔젤
	CASE "略称"
RESULTS = 노블 엔젤
	CASE "カテゴリ"
RESULTS:0 = 트릭스터-
ENDSELECT


@CARD_11230(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
@CARDSUMMON_11230(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件カテゴリ = 트릭스터

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,11230) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_11230(종류)
#DIM DYNAMIC 종류

PRINTL 「트릭스터-」 몬스터 2장
PRINTL 이 카드명의 ①②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 L소환했을 경우에 발동할 수 있다.
PRINTL 덱에서 「푸른눈물」 카드 1장을 패에 더한다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 자신의 필드 또는 묘지에 융합 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 자신의 묘지의 「트릭스터-」 몬스터 1장을 특수 소환한다.
CALLFORM CARD_ABLE(3,효과後誘発)
PRINTL ③: 자신 또는 상대가 효과 데미지를 받았을 경우에 발동한다.
PRINTL 필드의 앞면 표시 카드 1장을 파괴한다.


@CARDCAN_11230(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,11230)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,11230)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 푸른눈물
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,11230)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件종류 = 융합
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 트릭스터
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_3(듀얼리스트,11230)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL CARD_CHECK_FIN_BIT(듀얼리스트,"被弾種",3,2)
	SIF RESULT != 1
		RETURN 0
	IF 듀얼리스트 == "상대"
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		효과条件표시 = 10
		효과条件除去種 = 묘지
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_11230(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正



IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11230,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 푸른눈물
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11230,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 트릭스터
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11230,3)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 10
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両全場単体",場所,"묘지")
	ENDIF
ENDIF