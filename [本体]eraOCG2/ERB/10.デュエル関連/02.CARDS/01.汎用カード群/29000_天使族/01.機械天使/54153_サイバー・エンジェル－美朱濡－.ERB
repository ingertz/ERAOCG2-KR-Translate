@CARDNAME_54653,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 사이버 엔젤－비슈루－
	CASE "略称"
RESULTS = CA-비슈루-
	CASE "カテゴリ"
RESULTS:0 = 사이버 엔젤
ENDSELECT


@CARD_54653(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 의식
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
@CARDSUMMON_54653(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要レベル数 = 10

IF EX参照 == 1
	CALL RITCAN(듀얼리스트,소환カード,54653) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_54653(종류)
#DIM DYNAMIC 종류

PRINTL 「기계천사의 의식」에 의해 강림. 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 의식 소환에 성공했을 경우에 발동할 수 있다.
PRINTL EX덱에서 특수 소환된 상대 필드의 몬스터를 모두 파괴하고,
PRINTL 파괴한 몬스터의 수× 1000 데미지를 상대에게 준다.
PRINTL 이 턴, 이 카드는 1번의 배틀 페이즈 중에 2회 공격할 수 있다.

CALL CARD_ABLE(10,발동誘発)
PRINTL ②: 1턴에 1번, 상대가 효과를 발동했을 때, 자신의 묘지에서 의식 몬스터 1장을 덱으로 되돌리고 발동할 수 있다.
PRINTL 이 턴, 자신 필드의 카드는 효과로 파괴되지 않는다.


@CARDCAN_54653(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,54653)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 의식
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_54653(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 손패選択
#DIM 選択カード
#DIM 選択カード_ID
#DIM 소환カード
#DIM DYNAMIC 攻撃力修正



IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,54653)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 3
		효과条件召種 = 召種EX덱
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"묘지",1000)
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦",2)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,54653,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 의식
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐",1)
			CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"破耐",1)
		ENDIF
	ENDIF
	RETURN 0
ENDIF

