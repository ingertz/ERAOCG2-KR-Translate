@CARDNAME_72926,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 성점술의 의식
	CASE "略称"
RESULTS = 성점술의 의식
	CASE "カテゴリ"
RESULTS:0 = 점술희
RESULTS:1 = 의식마법
ENDSELECT


@CARD_72926(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72926(종류)
#DIM DYNAMIC 종류


PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL 「성점술희 타로트레이」의 강림에 필요.
CALL CARD_ABLE(0,마함起動 ,손패起動)
PRINTL ①: 자신의 패 / 필드에서, 레벨의 합계가 9 이상이 되도록 몬스터를 릴리스하고,
PRINTL 패에서 「성점술희 타로트레이」를 의식 소환한다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신 메인 페이즈에 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 덱에서 「점술희」 몬스터 1장을 패에 넣는다.
;PRINTL こ의 효과はこの카드가 묘지へ送られたターンには발동할 수 없다。

@CARDCAN_72926(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성,5

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72926)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL RESETAREA_MAT
	素材範囲필드 = 1
	素材範囲손패 = 1
	CALL FULLRESET
	효과範囲손패 = 1
효과条件名前 = 성점술희 타로트레이
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
    SIF 발동済_2(듀얼리스트,72926)
    	RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲덱 = 1
	효과条件カテゴリ = 점술희
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_72926(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード



IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72926)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
효과条件名前 = 성점술희 타로트레이
		효과条件소환可 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL RESETAREA_MAT
			CALL CARD_SELECT(듀얼리스트,選択カード)
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_%参照先（CS）%:{選択カード（CS）}")}(듀얼리스트,選択カード（CS）)
			素材範囲필드 = 1
			素材範囲손패 = 1
			TRYCALLFORM RITSUMMON(듀얼리스트,選択カード（CS）,参照先（CS）)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72926,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 점술희
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF