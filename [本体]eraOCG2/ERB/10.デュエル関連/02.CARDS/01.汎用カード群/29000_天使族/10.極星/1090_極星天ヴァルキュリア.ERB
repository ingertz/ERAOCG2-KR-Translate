@CARDCAN_1090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1090)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 ||  종류 == 특수소환時
	CALL CARD_SS_CHECK(듀얼리스트,101090,"")
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード



IF 종류 == 소환時 ||  종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1090)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)	
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,101090,"守備표시",,2)
		ENDIF
	ENDIF
ENDIF




