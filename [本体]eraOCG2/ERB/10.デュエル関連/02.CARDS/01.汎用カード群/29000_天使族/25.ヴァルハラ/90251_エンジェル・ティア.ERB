@CARDNAME_92961,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 엔젤 티어
	CASE "略称"
RESULTS = A 티어
	CASE "カテゴリ"
RESULTS:0 = 발할라
RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_92961(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92961(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 카드는 1턴에 1장밖에 발동할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,フリー誘発)
PRINTL ①: 자신 묘지에서 천사족 몬스터 4장을 제외하고,
PRINTL 자신 묘지의 천사족 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 특수 소환한다.
PRINTL 자신 필드 또는 묘지에 "신의 거성－발할라"가 존재하는 상태에서,
PRINTL 이 카드를 상대 몬스터의 직접 공격 선언시에 발동했을 경우,
PRINTL 또한 공격 대상을 대상 몬스터에게 옮겨서 데미지 계산을 실행한다.


@CARDCAN_92961(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 효과先
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92961)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,92961)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 천사족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 천사족
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 5
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92961(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성,2
#DIMS DYNAMIC 選択,10

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92961,1)
	IF RESULT == 0
		;通常마법汎用処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 천사족
		효과条件複数 = 4
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)	
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족 = 천사족
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF GETVAR(@"%듀얼리스트%_필드:カード소환先") > 0
				CALL FULLRESET
				효과範囲마함 = 1
				효과範囲묘지 = 1
효과条件名前 = 신의 거성－발할라
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT == 1
					IF ターンプレイヤー == 対面者 && 戦闘場所(듀얼리스트) == 5
						CALL COVER_ATTACK(듀얼리스트,カード소환先)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
