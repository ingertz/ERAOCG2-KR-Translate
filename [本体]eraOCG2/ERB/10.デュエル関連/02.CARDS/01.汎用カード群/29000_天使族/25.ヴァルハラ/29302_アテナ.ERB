@CARDNAME_29302,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 아테나
	CASE "略称"
RESULTS = 아테나
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_29302(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 800
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29302(종류)
#DIM DYNAMIC 종류


CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①: 1턴에 1번, 「아테나」 이외의 자신 필드의 앞면 표시 천사족 몬스터 1장을 묘지로 보내고,
PRINTL 「아테나」 이외의 자신 묘지의 천사족 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 천사족 몬스터를 특수 소환한다.
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ②: 이 카드가 이미 몬스터 존에 존재하는 상태에서,
PRINTL 필드에 다른 천사족 몬스터가 일반 소환 / 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 상대에게 600 데미지를 준다.

@CARDSUMMON_AA_29302


@CARDCAN_29302(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29302)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 천사족
	효과条件표시 = 表側표시
	효과条件固名前 = 아테나
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 천사족
	효과条件固名前 = 아테나
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_最召場所") == 場所
		RETURN 0
	CALL FULLRESET
	효과条件종족 = 천사족
	효과条件他所 = 1
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般",2)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29302(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC リリース数
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29302,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 천사족
		효과条件표시 = 表側표시
		효과条件固名前 = 아테나
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족 = 천사족
			효과条件固名前 = 아테나
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29302,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",600,1)
	ENDIF
ENDIF
