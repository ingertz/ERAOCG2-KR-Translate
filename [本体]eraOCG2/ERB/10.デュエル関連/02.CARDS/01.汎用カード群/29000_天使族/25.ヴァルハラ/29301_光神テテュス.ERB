@CARDNAME_29301,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 광신 테티스
	CASE "略称"
RESULTS = 테티스
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_29301(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29301(종류)
#DIM DYNAMIC 종류


CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ①: 자신이 천사족 몬스터를 드로우했을 경우, 그 카드를 상대에게 보여 발동할 수 있다.
PRINTL 이 카드가 필드에 앞면 표시로 존재할 경우, 자신은 덱에서 1장 드로우한다.


@CARDSUMMON_AA_29301


@CARDCAN_29301(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29301)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종족 = 천사족
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"Ｃドロー")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29301(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC リリース数
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29301)
	IF RESULT == 0
		SIF ゾーン != "필드"
			RETURN 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ENDIF
