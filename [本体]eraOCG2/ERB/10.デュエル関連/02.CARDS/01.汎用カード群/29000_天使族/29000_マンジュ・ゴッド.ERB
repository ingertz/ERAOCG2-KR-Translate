@CARD_29000(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1000
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29000(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時)
PRINTL ①: 이 카드가 소환되었을 때 발동할 수 있다.
PRINTL 덱에서 의식 몬스터 1장 또는 의식 마법 카드 1장을 패에 추가한다.


@CARDSUMMON_AA_29000


@CARDCAN_29000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29000)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件スーパーカテゴリ = 의식마법
	효과条件종류 = 의식
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29000)
	IF RESULT == 0
		CALL FULLRESET
		효과条件スーパーカテゴリ = 의식마법
		효과条件종류 = 의식
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF



