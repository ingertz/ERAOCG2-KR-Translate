	CASE "名前"
RESULTS = TG-클로즈
	CASE "略称"
RESULTS = TG 클로즈
	CASE "カテゴリ"
RESULTS:0 = 테크 지너스
ENDSELECT


@CARD_92415(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

