@CARD_EXPLANATION_92415(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①②의 효과는 1턴에 1번, 어느 한 가지밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 자신 필드에 "TG" 엑스 몬스터가 존재하고, 상대 몬스터의 효과 / 마법 / 함정 카드가 발동했을 때 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
PRINTL ②: 이 카드가 묘지에 존재하는 상태로, 싱크로 몬스터가 제외되었을 경우에 발동할 수 있다. 이 카드를 자신 필드에 세트한다.
PRINTL 이 효과로 세트한 이 카드는 필드에서 벗어났을 경우에 제외된다.
PRINTL 



@CARDCAN_92415(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92415)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,92415)
		RETURN 0
	SIF 발동済_2(듀얼리스트,92415)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0	

	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 테크지너스
	효과条件종류 = EXモン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,92415)
		RETURN 0
	SIF 발동済_2(듀얼리스트,92415)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0

	CALL FULLRESET
	효과条件カテゴリ = 테크지너스
	효과条件종류 = 싱크로
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과除送")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_92415(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92415,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92415,2)
	IF RESULT == 0
		CALL FULLRESET
효과条件名前 = TG-클로즈
		효과추가종류 = 500
		CALL CARD_SET_BASIC(듀얼리스트,"묘지",ゾーン,場所,"裏側표시")
	ENDIF
ENDIF
