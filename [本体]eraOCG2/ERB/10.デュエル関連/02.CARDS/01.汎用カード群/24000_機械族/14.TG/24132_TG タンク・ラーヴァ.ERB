@CARDNAME_24132,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = TG 탱크 라바
	CASE "略称"
RESULTS = TG 라바
	CASE "カテゴリ"
RESULTS:0 = 텍 지너스
ENDSELECT


@CARD_24132(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RESULT:1 = 기계족 
		RETURN 곤충족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "튜너"
		RETURN 2
ENDSELECT

RETURN 0

@CARD_EXPLANATION_24132(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL ①: 자신 필드의 이 카드를 싱크로 소재로 할 경우, 이 카드를 튜너 이외의 몬스터로 취급할 수 있다.
CALLFORM CARD_ABLE(2,S素材時효과)
PRINTL ②: 필드에 「TG」 싱크로 몬스터가 존재하고, 이 카드가 싱크로 소재로서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 자신 필드에 「TG 토큰」(기계족 땅 속성 레벨 1 공격력 / 수비력 0) 1장을 공격 표시로 특수 소환한다.

@CARDCAN_24132(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24132)
SIF RESULT == 1
	RETURN 0

IF 종류 == S素材時효과
	SIF 발동済_2(듀얼리스트,24132)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 테크지너스
	효과条件종류 = 싱크로
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_24132(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == S素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24132,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_GENERATE(듀얼리스트,124132,"攻撃표시")
	ENDIF
ENDIF

