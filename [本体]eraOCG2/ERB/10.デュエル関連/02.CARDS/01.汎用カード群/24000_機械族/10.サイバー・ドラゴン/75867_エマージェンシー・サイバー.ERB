@CARDNAME_75867,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 긴급 사이버
	CASE "略称"
RESULTS = E 사이버
	CASE "カテゴリ"
RESULTS:0 = 사이버
ENDSELECT


@CARD_75867(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_75867(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTL ①: 덱에서 빛 속성 "사이버" 몬스터 1장을 패에 추가한다.
CALLFORM CARD_ABLE(0,효과後誘発)
PRINTL ②: 패의 이 카드가 파괴되었을 경우에 발동할 수 있다.
;PRINTL 패를 １장捨て、묘지の「エマ-ジェンシ- 사이버」１장을 손패に加える。
PRINTL 묘지의 "긴급 사이버" 1장을 패에 추가한다.


@CARDCAN_75867(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,75867)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 사이버
	효과条件속성 = 빛속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
효과条件名前 = 긴급 사이버
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과破壊")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_75867(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75867,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,完全묘지行き)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 사이버
		효과条件속성 = 빛속성
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75867,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 긴급 사이버
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
