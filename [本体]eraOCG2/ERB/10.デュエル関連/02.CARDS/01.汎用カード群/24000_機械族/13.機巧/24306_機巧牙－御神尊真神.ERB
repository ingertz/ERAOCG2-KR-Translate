@CARDNAME_24306,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 기교아-오신존진신
	CASE "略称"
RESULTS = 오신존진신
	CASE "カテゴリ"
RESULTS:0 = 기교
ENDSELECT


@CARD_24306(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2150
	CASE "守備力"
		RETURN 2150
ENDSELECT

RETURN 0

@CARD_EXPLANATION_24306(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①②③의 효과는 각각 1턴에 한 번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①: 제외되어 있는 자신의 카드가 6장 이상일 경우에 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
CALLFORM CARD_ABLE(2,소환時, 특수소환時)
PRINTL ②: 이 카드가 소환 / 특수 소환에 성공했을 경우, 패에서 몬스터 1장을 버리고 발동할 수 있다.
PRINTL 덱에서 「기교아-오신존진신」 이외의, 공격력과 수비력의 수치가 같은 몬스터 1장을 패에 넣는다.
CALLFORM CARD_ABLE(3,戦破壊→묘지)
PRINTL ③: 이 카드가 파괴되었을 경우에 발동할 수 있다.
PRINTL 제외되어 있는 자신의 카드 6장까지를 고르고 덱으로 되돌린다.


@CARDCAN_24306(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24306)
SIF RESULT == 1
	RETURN 0
IF 종류 == 손패起動
	SIF ゾーン != "손패"
		RETURN 0
	SIF 발동済_1(듀얼리스트,24306)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 6
		RETURN 0
	RETURN 1
ELSEIF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_2(듀얼리스트,24306)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件同攻守 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF 발동済_3(듀얼리스트,24306)
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_24306(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択종족



IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24306,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24306,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件固名前 = 기교아-오신존진신
			효과条件종류 = モン全般
			효과条件同攻守 = 1
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24306,3)
	IF RESULT == 0
		CALL FULLRESET
		효과条件行先플래그 = 덱行き
		효과条件複数 = 94
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"제외",ゾーン,場所)
	ENDIF
ENDIF