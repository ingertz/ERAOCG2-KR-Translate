@CARDNAME_27106,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 우주괴수 간길
	CASE "略称"
RESULTS = 하늘짐승
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_27106(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 비늘야수족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_27106(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(0,손패起動)
PRINTL ①: 필드의 악성 카운터가 놓여 있는 몬스터 1장을 릴리스하고 발동할 수 있다.
PRINTL 패에서 이 카드를 특수 소환한다.
CALL CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ②: 서로의 턴에 1번, 상대 필드 위의 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 몬스터에 악성 카운터 1개를 놓는다.
PRINTL

@CARDCAN_27106(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,27106)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,27106,"손패",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	SIF モンスター数(듀얼리스트) < 5
		효과範囲상대필드 = 1
	효과条件悪Ｃ所持 = 999
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_27106(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,27106)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		SIF モンスター数(듀얼리스트) < 5
			효과範囲상대필드 = 1
		효과条件悪Ｃ所持 = 999
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_EROTICS(RESULT:1)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORMW %CALLNAME:조교대상%는突然宇宙より現れた獣に飲み込まれた。
				PRINTFORML 初め、宇宙獣の身体の下から聞こえていた %CALLNAME:조교대상%の苦悶の声は、
				PRINTFORMW 数瞬後には牝の本能に支配された嬌声へと変わっていった。
				PRINTFORMW しばらくして宇宙獣が立ち去ると、そこには両穴から정액を垂れ流した %CALLNAME:조교대상%が白目を向いて腹を見せ、身体を痙攣させていた……
				CALL EROTIC_MOVE("両穴凌辱",조교대상,-1,500)
			ENDIF
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,27106,10)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
		IF RESULT != 9999
			選択カード = RESULT
			CALL CARD_SELECT(듀얼리스트,選択カード)
			CALL CARD_EROTICS_FIELD(대상者（CS）,参照先（CS）,選択カード（CS）)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORML %CALLNAME:조교대상%는宇宙獣より放たれた謎の액체を浴びてしまった。
				CALL EROTIC_MOVE("ぶっかけ",조교대상,-1,100)
			ENDIF
			CALL SET_FIELD_INFO(対面者,"필드",選択カード,"悪Ｃ",1)
		ENDIF
	ENDIF
ENDIF
