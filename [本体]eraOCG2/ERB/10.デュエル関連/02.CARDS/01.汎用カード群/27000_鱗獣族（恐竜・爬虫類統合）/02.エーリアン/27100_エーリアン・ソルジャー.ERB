@CARDNAME_27100,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 에일리언 솔저
	CASE "略称"
RESULTS = A 솔저
	CASE "カテゴリ"
RESULTS:0 = 에일리언
ENDSELECT


@CARD_27100(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 通常モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 비늘야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1900
	CASE "守備力"
		RETURN 800
	CASE "성별"
		RETURN 雄性
ENDSELECT

