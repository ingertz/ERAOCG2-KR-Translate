@CARDNAME_93050,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 비틀트루퍼 플로트 스팅
	CASE "略称"
RESULTS = 비틀트루퍼플로트
	CASE "カテゴリ"
RESULTS:0 = 비틀트루퍼
ENDSELECT


@CARD_93050(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93050(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 ①②의 효과는 1턴에 1번, 어느 한쪽만 사용할 수 있다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 자신 필드에 「비틀트루퍼」 몬스터가 존재하고, 상대가 몬스터의 효과를 발동했을 때에 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
CALLFORM CARD_ABLE(1,ターン終了時)
PRINTL ②: 이 카드가 묘지에 존재하고, 자신 필드에 공격력 3000 이상의 곤충족 몬스터가 존재할 경우,
PRINTL 자신 엔드 페이즈에 자신 묘지에서 곤충족 몬스터 1장을 제외하고 발동할 수 있다.
PRINTL 이 카드를 자신 필드에 세트한다.
PRINTL 



@CARDCAN_93050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93050)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,93050)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0	
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 비틀트루퍼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時
	SIF 발동済_1(듀얼리스트,93050)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件以攻撃力 = 3000
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 곤충족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_93050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93050,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93050,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 곤충족
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_DISPOSE(듀얼리스트,"묘지",場所,4,1)
		ENDIF
	ENDIF
ENDIF
