@CARDNAME_30200,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 레조넌스 인섹트
	CASE "略称"
RESULTS = 공진충
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_30200(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 700
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_30200(종류)
#DIM DYNAMIC 종류

SIF 종류 == 필드→묘지
	SETCOLOR C_YELLOW
CALLFORM CARD_ABLE(0,戦破壊→묘지)
PRINTL ①: 이 카드가 필드에서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 덱에서 레벨 5 이상의 곤충족 몬스터 1장을 패에 넣는다.
RESETCOLOR
CALLFORM CARD_ABLE(0,제외時)
PRINTL ②: 이 카드가 제외되었을 경우에 발동할 수 있다.
PRINTL 덱에서 「공진충」 이외의 곤충족 몬스터 1장을 묘지로 보낸다.
PRINTL
PRINTL 


@CARDCAN_30200(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,30200)
SIF RESULT == 1
	RETURN 0
IF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 곤충족
	CALL 효과条件レベル_ADV(5, "以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 제외時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 곤충족
	효과条件固名前 = 레조넌스 인섹트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_30200(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM サーチカード 


IF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30200)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 곤충족
		CALL 효과条件レベル_ADV(5, "以上")
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30200)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종족 = 곤충족
		효과条件固名前 = 레조넌스 인섹트
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
