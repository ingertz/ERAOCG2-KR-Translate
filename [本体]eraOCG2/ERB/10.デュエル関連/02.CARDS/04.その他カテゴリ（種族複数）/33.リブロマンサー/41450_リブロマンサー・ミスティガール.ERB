@CARD_41450(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 의식
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 2100	
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
