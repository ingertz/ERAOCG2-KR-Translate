@CARDCAN_2222(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2222)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || (종류 == フリー誘発 && ゾーン == "손패")
	SIF ゾーン != "손패"
		RETURN 0
	SIF 발동済_1(듀얼리스트,2222)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,2222,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
효과条件カテゴリ = 뱅퀴시 소울
	효과条件종류 = モン全般
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || (종류 == フリー誘発 && ゾーン == "필드")
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,2222)
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과条件속성 = 어둠속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件속성 = 땅속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 ++
	ENDIF

	CALL FULLRESET
	효과範囲덱 = 1
효과条件カテゴリ = 뱅퀴시 소울
	효과条件종류 = モン全般
	CALLFORM 효과条件종족_ADV(사이킥족,"以外")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件속성 = 화염속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 ++
	ENDIF
	SIF 条件달성 == 0
		RETURN 0

	RETURN 1
ENDIF


@CARDEFFECT_2222(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 条件달성A
#DIM DYNAMIC 条件달성B

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%




IF 종류 == 손패起動 || (종류 == フリー誘発 && ゾーン == "손패")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2222,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
효과条件カテゴリ = 뱅퀴시 소울
		효과条件종류 = モン全般
		효과条件他所 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動 || (종류 == フリー誘発 && ゾーン == "필드")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2222,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件속성 = 땅속성
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件달성A ++
		ENDIF

		CALL FULLRESET
		효과範囲덱 = 1
효과条件カテゴリ = 뱅퀴시 소울
		효과条件종류 = モン全般
		CALLFORM 효과条件종족_ADV(사이킥족,"以外")
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件속성 = 화염속성
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件달성B ++
		ENDIF

		CALL FULLRESET
		효과条件속성 = 어둠속성
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"손패",ゾーン,場所)
		選択カード_ID = RESULT:99
		選択カード = RESULT
		IF 選択カード != 9999
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件속성:0 = 신속성
			SIF 条件달성A
				효과条件속성:0 = 땅속성
			SIF 条件달성B
				효과条件속성:1 = 화염속성
			효과条件固ID = 選択カード_ID
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"손패",ゾーン,場所)
			選択カード_ID = RESULT:99
			TRYCALLFORM CARD_{選択カード_ID},"속성"
			IF MATCH(RESULT,화염속성)
				CALL CARDEFFECT_2222_2(듀얼리스트,종류,ゾーン,場所)
			ELSEIF MATCH(RESULT,땅속성)
				CALL CARDEFFECT_2222_1(듀얼리스트,종류,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF




@CARDEFFECT_2222_1(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力판정
#DIM DYNAMIC 설정場所

VARSET 설정場所, -1

攻撃力판정 = 999999

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

FOR カウンタ,0,5
	SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
		CONTINUE
	SIF GETVAR(@"%対面者%_MZ_表:カウンタ") == 3
		CONTINUE
	IF 攻撃力판정 > GETVAR(@"%対面者%_MZ_攻:カウンタ")
		攻撃力판정 = GETVAR(@"%対面者%_MZ_攻:カウンタ")
		설정場所 = カウンタ
	ENDIF
NEXT
IF 설정場所 >= 0
	CALL FULLRESET
	효과추가종류 = 5
	CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",설정場所,"強奪")
ENDIF
RETURN



@CARDEFFECT_2222_2(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC めくり数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL FULLRESET
효과範囲덱 = 1
효과条件カテゴリ = 뱅퀴시 소울
효과条件종류 = モン全般
CALLFORM 효과条件종족_ADV(사이킥족,"以外")
CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

RETURN

