@CARDNAME_1436,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 뇌화의 공주 사안
	CASE "略称"
RESULTS = 공주자노메
	CASE "カテゴリ"
RESULTS:0 = 뇌화
RESULTS:2 = 가설치
ENDSELECT


@CARD_1436(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

