CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 이 카드를 패에서 버리고 발동할 수 있다.
PRINTL 패에서 곤충족 식물족 비늘야수족 몬스터 1장을 특수 소환한다.
CALLFORM CARD_ABLE(2,제외時)
PRINTL ②: 이 카드가 제외되었을 경우에 발동한다.
PRINTL 이 턴의 엔드 페이즈에,
PRINTL 자신 필드의 곤충족 식물족 비늘야수족 몬스터의 종족 종류의 수만큼 자신은 드로한다.
PRINTL 


@CARDSUMMON_AA_1436


@CARDCAN_1436(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1436)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,1436)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종족:0 = 곤충족
	효과条件종족:1 = 식물족
	효과条件종족:2 = 비늘야수족
	효과条件종류 = モン全般
	효과条件他所 = 1
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 제외時
	SIF 발동済_2(듀얼리스트,1436)
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF
@CARDEFFECT_1436(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC ドロー枚数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1436,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종족:0 = 곤충족
		효과条件종족:1 = 식물족
		효과条件종족:2 = 비늘야수족
		효과条件종류 = モン全般
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1436,2)
	IF RESULT == 0

		CALL CARD_RESERV(듀얼리스트,1436)
	ENDIF
ELSEIF 종류 == ターン終了時予約
	ドロー枚数 = 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 곤충족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		ドロー枚数 += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 식물족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		ドロー枚数 += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 비늘야수족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		ドロー枚数 += 1

	SIF ドロー枚数 > 0
		PRINTW 異界のジャノメちゃんから仕送りが届いた！
	CALL CARDDRAW_BASIC(듀얼리스트,ドロー枚数)
ENDIF
