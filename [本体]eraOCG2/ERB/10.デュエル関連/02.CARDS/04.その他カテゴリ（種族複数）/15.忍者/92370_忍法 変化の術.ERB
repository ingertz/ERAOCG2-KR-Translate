@CARDNAME_92370,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 인법 변화의 술
	CASE "略称"
RESULTS = 인법 변화의 술
	CASE "カテゴリ"
RESULTS:0 = 인법
ENDSELECT


@CARD_92370(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92370(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①：자신 필드의 앞면 표시의 「닌자」 몬스터 1장을 릴리스하여 이 카드를 발동할 수 있다.
PRINTL 릴리스한 몬스터의 원래 레벨 +3 이하의 레벨을 가지는,
PRINTL 야수족, 비행야수족, 곤충족 몬스터 1장을, 자신의 손패, 덱, 묘지에서 특수 소환한다.


@CARDCAN_92370(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92370)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 닌자
	효과条件종류 = レベル持ち
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0


	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件종족:2 = 곤충족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_92370(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC ニンジャ
#DIM DYNAMIC ジツ・レベル
#DIM DYNAMIC セット先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92370)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 닌자
		효과条件종류 = レベル持ち
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			ニンジャ = RESULT:1

			TRYCALLFORM CARD_{ニンジャ},"レベル"
			ジツ・レベル = RESULT + 3
			;PRINTFORMW ジツ・レベルは{ジツ・レベル}
			;参考：マテリアリゼ-ション
			セット先 = 場所
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件종족:0 = 야수족
			효과条件종족:1 = 비행야수족
			효과条件종족:2 = 곤충족
;			효과추가종류 = 50 + セット先
			CALL 효과条件レベル_ADV(ジツ・レベル,"以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
