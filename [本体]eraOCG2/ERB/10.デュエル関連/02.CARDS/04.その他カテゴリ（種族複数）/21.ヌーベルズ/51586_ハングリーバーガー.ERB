@CARDNAME_51586,参照先
;ココで지정카드의 名前、略称を返す予定
;#FUNCTIONS
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 헝그리 버거
	CASE "略称"
RESULTS = 헝그리 버거
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_51586(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 의식
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족 
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 1850
	CASE "分割"
		RETURN 효과なし
ENDSELECT
RETURN 0

