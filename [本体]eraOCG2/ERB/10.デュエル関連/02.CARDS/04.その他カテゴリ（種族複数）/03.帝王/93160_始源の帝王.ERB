@CARDNAME_93160,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 시원의 제왕
	CASE "略称"
RESULTS = 시원의 제왕
	CASE "カテゴリ"
RESULTS:0 = 제왕
ENDSELECT


@CARD_93160(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 雄性
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93160(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①: 이 카드는 발동 후, 몬스터 존에 특수 소환한다.
CALLFORM CARD_ABLE(0,필드起動,フリー誘発)
PRINTL ②: 이 카드가 몬스터 존에 존재할 경우,
PRINTL 자신 / 상대의 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드를 릴리스하고, 패에서 「제왕」 몬스터 1장을 어드밴스 소환한다.
PRINTL 그 때, 이 카드는 2장 분의 릴리스로 할 수 있다.


@CARDCAN_93160(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93160)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,93160,"마함")
	SIF RESULT == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || (종류 == フリー誘発 && ゾーン == "필드")
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 제
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_93160(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93160)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "마함", 場所)
	ENDIF

ELSEIF 종류 == 필드起動 || (종류 == フリー誘発 && ゾーン == "필드")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93160)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 제
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT != 9999
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL CARD_SUMMON_SHORI(듀얼리스트,RESULT,"攻撃표시",1)
		ENDIF
	ENDIF
ENDIF

