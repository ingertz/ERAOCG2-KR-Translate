@CARDNAME_93161,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 진원의 제왕
	CASE "略称"
RESULTS = 진원의 제왕
	CASE "カテゴリ"
RESULTS:0 = 제왕
ENDSELECT


@CARD_93161(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_93161(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 한 번만 사용할 수 있다.
CALLFORM CARD_ABLE(10,마함起動,フリー誘発)
PRINTL ①: 1턴에 한 번, 자신의 묘지에서 제외된 상태의 "제왕" 마법 / 함정 카드 2장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 덱으로 되돌리고 셔플하며, 자신은 1장 드로우한다.
CALLFORM CARD_ABLE(2,묘지起動,フリー誘発)
PRINTL ②: 이 카드가 묘지에 존재할 경우,
PRINTL 이 카드 이외의 자신의 묘지의 "제왕" 마법 / 함정 카드 1장을 제외하고 발동할 수 있다.
PRINTL 이 카드를 몬스터 존에 특수 소환한다.


@CARDCAN_93161(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93161)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
	RETURN 1

ELSEIF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲제외 = 1
	효과条件カテゴリ = 제왕
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 발동済_2(듀얼리스트,93161)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,93161,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 제왕
	효과条件종류 = 마함全般
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_93161(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 2
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93161)
	IF RESULT == 0
	ENDIF

ELSEIF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93161,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 제왕
		효과条件종류 = 마함全般
		효과条件複数 = 2
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL SHUFFLE(듀얼리스트)
			CALL CARDDRAW_BASIC(듀얼리스트,1)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93161,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 제왕
		효과条件종류 = 마함全般
		효과条件他所 = 1
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"묘지",場所)
		ENDIF
	ENDIF
ENDIF

