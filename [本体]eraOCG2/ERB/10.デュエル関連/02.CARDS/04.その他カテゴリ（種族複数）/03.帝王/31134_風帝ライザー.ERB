@CARDNAME_31134,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 바람 제왕 라이저
	CASE "略称"
RESULTS = 바람제왕 라이저
	CASE "カテゴリ"
RESULTS:0 = 제
ENDSELECT


@CARD_31134(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
ENDSELECT
RETURN 0

