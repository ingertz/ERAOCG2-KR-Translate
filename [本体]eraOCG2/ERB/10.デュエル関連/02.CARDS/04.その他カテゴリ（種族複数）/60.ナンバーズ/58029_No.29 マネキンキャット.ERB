@CARDNAME_58029,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = No.29 마네킹 캣
	CASE "略称"
RESULTS = #29 마네킹 캣
	CASE "カテゴリ"
RESULTS:0 = 넘버즈
RESULTS:1 = 캣
ENDSELECT


@CARD_58029(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 900
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

