@CARDCAN_58029(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58029)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	;상대モン５以上はダメ
	SIF モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発 
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,58029)
		RETURN 0
	SIF モンスター数(対面者) < 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	;ss時誘発、サッカ-から
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"특수소환",2)
	SIF RESULT != 1
		RETURN 0
	RETURN 1	
ENDIF

@CARDEFFECT_58029(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 속성条件
#DIM DYNAMIC 종족条件
#DIM DYNAMIC 条件달성,2


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58029,10,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件종류 = モン全般
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"",0,1)
	ENDIF

ELSEIF 종류 == 효과後誘発 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58029,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			속성条件 = GETVAR(@"%対面者%_MZ_属:選択カード")
			종족条件 = GETVAR(@"%対面者%_MZ_種:選択カード")

			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件종족 = 종족条件
			효과条件スーパー속성 = 속성条件
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF	
ENDIF