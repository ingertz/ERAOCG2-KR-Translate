@CARDNAME_91802,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 체인지! 제트 아이언호
	CASE "略称"
RESULTS = 변형 아이언호
	CASE "カテゴリ"
RESULTS:0 = 로빈 패밀리
RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_91802(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91802(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,フリー誘発)
PRINTL ①：자신의 패 / 덱/필드에서 「이차원 에스퍼 스타 로빈」, 「야수전사 퓨먼」, 「봉왕수 가일루다」와 자신 필드의 「철거인 아이언 해머」를 각각 1체씩 묘지로 보내고 발동할 수 있다.
PRINTL 패 / 덱에서 「이차원 제트 아이언호」 1장을 소환 조건을 무시하고 특수 소환한다.
PRINTL ②：묘지의 이 카드를 제외하고, 자신 필드의 레벨/랭크 10의 빛 속성 몬스터 1장을 대상으로 하여 발동할 수 있다.
CALLFORM CARD_ABLE(2,묘지起動 ,フリー誘発)
PRINTL 그 몬스터의 공격력 / 수비력은 턴 종료시까지 배가 된다.
PRINTL その몬스터의 攻撃力 守備力はターン終了時まで倍になる。


@CARDCAN_91802(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91802)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
		SIF 발동済_1(듀얼리스트,91802)
			RETURN 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件名前 = 이차원 에스퍼 스타 로빈
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件名前 = 야수 전사 퓨먼
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件名前 = 봉황수 가루다
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件名前 = 쇠 거인 아이언 해머
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件名前 = 이차원 제트 아이언호
		효과条件소환可 = 1
		효과条件特不無視 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSEIF ゾーン == "묘지"
		SIF 발동済_2(듀얼리스트,91802)
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件レベル = 10
		효과条件속성 = 빛속성
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_91802(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 설정レベル 

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91802,1)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과条件名前 = 이차원 에스퍼 스타 로빈
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲손패 = 1
				효과範囲덱 = 1
				효과範囲필드 = 1
				효과範囲마함 = 1
				효과条件名前 = 야수 전사 퓨먼
				CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과範囲손패 = 1
					효과範囲덱 = 1
					효과範囲필드 = 1
					효과範囲마함 = 1
					효과条件名前 = 봉황수 가루다
					CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
					IF RESULT == 1
						CALL FULLRESET
						효과範囲필드 = 1
						효과範囲마함 = 1
						효과条件名前 = 쇠 거인 아이언 해머
						CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
						IF RESULT == 1
							CALL FULLRESET
							효과範囲손패 = 1
							효과範囲덱 = 1
							효과条件特不無視 = 1
							효과条件名前 = 이차원 제트 아이언호
							CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ELSEIF ゾーン == "묘지"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91802,2)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件レベル = 10
			효과条件속성 = 빛속성
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"一攻",GETVAR(@"%듀얼리스트%_MZ_攻:選択カード"))
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"一守",GETVAR(@"%듀얼리스트%_MZ_守:選択カード"))
			ENDIF
		ENDIF
	ENDIF
ENDIF
