@CARDNAME_26802,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 봉황수 가일-다
	CASE "略称"
RESULTS = 가일-다
	CASE "カテゴリ"
RESULTS:0 = 로빈 패밀리
ENDSELECT


@CARD_26802(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 1200
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26802(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時 ,특수소환時)
PRINTL ①: 자신의 필드 묘지에 「이차원 에스퍼 스타 로빈」이 존재하고,
PRINTL 이 카드가 소환 / 특수 소환했을 경우에 발동할 수 있다.
PRINTL 자신은 1장 드로우한다.
CALLFORM CARD_ABLE(0,攻撃時強制誘発)
PRINTL ②: 이 카드가 몬스터에게 공격할 경우, 이 카드의 공격력은 300 오른다.


@CARDCAN_26802(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26802)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲묘지 = 1
	효과条件名前 = 이차원 에스퍼 스타 로빈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,26802)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26802(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 戦闘カード


IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26802)
	IF RESULT == 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ELSEIF 종류 == 攻撃時強制誘発
	戦闘カード = 戦闘場所(듀얼리스트)
	CALL SET_FIELD_INFO(듀얼리스트,"필드",戦闘カード,"一時",300)
ENDIF
