@CARDNAME_24804,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 이차원 제트 아이언호
	CASE "略称"
RESULTS = 아이언호
	CASE "カテゴリ"
RESULTS:0 = 로빈 패밀리
ENDSELECT


@CARD_24804(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 4000
	CASE "守備力"
		RETURN 0
	CASE "소환불가"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_24804(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 일반 소환할 수 없다.
CALLFORM CARD_ABLE(0,손패起動)
PRINTL ①: 「이차원 에스퍼 스타 로빈」, 「야수 전사 퓨먼」, 「봉황수 가루다」, 「쇠 거인 아이언 해머」를
PRINTL 자신의 손패, 필드에서 1체씩 묘지로 보내 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
CALLFORM CARD_ABLE(0,필드起動 ,フリー誘発)
PRINTL ②: 자신, 상대 턴에 이 카드를 릴리스하고, 자신의 묘지의 「이차원 에스퍼 스타 로빈」, 「야수 전사 퓨먼」
PRINTL 「봉황수 가루다」, 「쇠 거인 아이언 해머」를 1체씩 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 특수 소환한다.


@CARDCAN_24804(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24804)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,24804,"손패",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 이차원 에스퍼 스타 로빈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 야수 전사 퓨먼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 봉황수 가루다
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 쇠 거인 아이언 해머
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 이차원 에스퍼 스타 로빈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 야수 전사 퓨먼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 봉황수 가루다
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 쇠 거인 아이언 해머
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 3
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_24804(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 소환済レベル
#DIM DYNAMIC 묘지送りレベル


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24804)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件名前 = 이차원 에스퍼 스타 로빈
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과条件名前 = 야수 전사 퓨먼
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲손패 = 1
				효과範囲필드 = 1
				효과範囲마함 = 1
				효과条件名前 = 봉황수 가루다
				CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					CALL FULLRESET
					효과範囲손패 = 1
					효과範囲필드 = 1
					효과範囲마함 = 1
					효과条件名前 = 쇠 거인 아이언 해머
					CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
					IF RESULT == 1
						CALL FULLRESET
						CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)	
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24804)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 이차원 에스퍼 스타 로빈
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 야수 전사 퓨먼
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 봉황수 가루다
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 쇠 거인 아이언 해머
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
