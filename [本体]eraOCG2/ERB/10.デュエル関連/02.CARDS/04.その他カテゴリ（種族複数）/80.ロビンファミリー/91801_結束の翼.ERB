@CARDNAME_91801,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 결속의 날개
	CASE "略称"
RESULTS = 결속의 날개
	CASE "カテゴリ"
RESULTS:0 = 로빈 패밀리
RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_91801(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91801(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 카드는 1턴에 1장만 발동할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,フリー誘発)
PRINTL ①: 자신의 필드 / 묘지에 「이차원 에스퍼 스타 로빈」이 존재할 경우에 발동할 수 있다.
PRINTL 패 / 덱 / 묘지/제외 상태에서 「봉황수 가일-다」 1장을 특수 소환한다.
CALLFORM CARD_ABLE(0,묘지起動 ,フリー誘発)
PRINTL ②: 묘지의 이 카드를 제외하고, 자신 필드의 공격 표시 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터는 이 턴에, 전투 내성을 얻는다.


@CARDCAN_91801(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91801)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
		SIF 발동済_1(듀얼리스트,91801)
			RETURN 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件名前 = 봉황수 가루다
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲묘지 = 1
		효과条件名前 = 이차원 에스퍼 스타 로빈
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSEIF ゾーン == "묘지"
		SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件표시 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_91801(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 설정レベル 

IF 종류 == 마함起動 || 종류 == 묘지起動 || 종류 == フリー誘発
	IF ゾーン == "마함"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91801,1)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과範囲제외 = 1
			효과条件名前 = 봉황수 가루다
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ELSEIF ゾーン == "묘지"
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91801)
		IF RESULT == 0
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			CALL FULLRESET
			효과条件표시 = 1
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			選択カード = RESULT
			IF 選択カード != 9999
				CALL STATCHANGE_EFFECT(듀얼리스트,"自身",選択カード,"戦阻",1)
			ENDIF
		ENDIF
	ENDIF
ENDIF
