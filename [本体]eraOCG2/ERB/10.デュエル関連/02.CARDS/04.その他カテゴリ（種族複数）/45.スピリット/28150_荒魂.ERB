@CARDCAN_28150(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,28150)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件固名前 = 황혼
	효과条件カテゴリ = 스피릿
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_28150(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 戦闘カード


IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,28150)
	IF RESULT == 0
		CALL FULLRESET
		효과条件固名前 = 황혼
		효과条件カテゴリ = 스피릿
		효과条件종류 = モン全般
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン終了時強制
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,손패行き)
ENDIF

