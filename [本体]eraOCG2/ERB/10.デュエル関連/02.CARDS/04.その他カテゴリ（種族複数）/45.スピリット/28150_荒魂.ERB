@CARDNAME_28150,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 황혼
	CASE "略称"
RESULTS = 황혼
	CASE "カテゴリ"
RESULTS:0 = 스피릿
ENDSELECT

@CARD_28150(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 1800
	CASE "특수불가"
		RETURN 1
ENDSELECT

RETURN 0

