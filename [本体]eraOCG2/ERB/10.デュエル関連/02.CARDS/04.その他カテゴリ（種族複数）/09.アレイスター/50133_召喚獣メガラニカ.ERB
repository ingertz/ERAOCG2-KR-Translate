@CARDSUMMON_50133(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
素材条件名前 = 소환사 알레이스터
ELSEIF EX参照 >= 10
	素材条件속성 = 땅속성
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,50133) 
	RETURN RESULT
ENDIF

@CARDCAN_50133(듀얼리스트,종류,ゾーン,場所)
