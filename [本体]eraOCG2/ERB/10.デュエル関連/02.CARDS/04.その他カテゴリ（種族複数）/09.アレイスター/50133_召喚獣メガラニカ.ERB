@CARDNAME_50133,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 소환수 메갈라니아
	CASE "略称"
RESULTS = 메갈라니아
	CASE "カテゴリ"
RESULTS:0 = 소환수
ENDSELECT


@CARD_50133(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3300
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

