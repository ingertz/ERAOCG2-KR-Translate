@CARDNAME_2192,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = K9-66b호 랜턴
	CASE "略称"
RESULTS = K9 랜턴
	CASE "カテゴリ"
RESULTS:0 = K9
ENDSELECT


@CARD_2192(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 1900
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250322
ENDSELECT
RETURN 0

@CARD_EXPLANATION_2192(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②③ 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,효과소환)
PRINTL ①: 상대 손패가 2장 이상일 경우, 이 카드는 릴리스 없이 소환할 수 있다.
CALLFORM CARD_ABLE(2,손패起動)
PRINTL ②: 이 카드가 패에 존재할 경우,
PRINTL 「K9-66b호 랜턴」 이외의 자신 묘지의 레벨 5의 「K9」 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터와 이 카드를 특수 소환한다.
CALLFORM CARD_ABLE(3,필드起動)
PRINTL ③: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 덱에서 「K9」 마법 / 함정 카드 1장을 패에 넣는다.

@CARDSUMMON_AA_2192


@CARDCAN_2192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2192)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과소환
	CALL FULLRESET
	효과範囲상대손패 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 손패起動
	SIF 발동済_2(듀얼리스트,2192)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,2192,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件固ID = 2192
	효과条件レベル = 5
	효과条件カテゴリ = K9
	효과条件종류 = レベル持ち
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_3(듀얼리스트,2192)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = K9
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_2192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード


IF 종류 == 효과소환
	RETURN 1
ELSEIF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2192,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件固ID = 2192
		효과条件レベル = 5
		효과条件カテゴリ = K9
		효과条件종류 = レベル持ち
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2192,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = K9
		효과条件종류 = 마함全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF



