@CARDNAME_73890,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 명해부－메멘토랜
	CASE "略称"
RESULTS = 명해부
	CASE "カテゴリ"
RESULTS:0 = 메멘토
ENDSELECT


@CARD_73890(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN フィールド
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73890(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 「명해부」가 된다.
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 자신의 「메멘토」 몬스터가 전투를 실행할 경우,
PRINTL 상대는 데미지 스텝 종료 시까지 카드 효과를 발동할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ②: 자신의 패 / 필드의 몬스터가 전투 또는 효과로 파괴되었을 경우에 발동할 수 있다.
PRINTL 파괴된 몬스터보다 레벨이 낮은 「메멘토」 몬스터 1장을, 자신의 패 / 묘지에서 특수 소환한다.
CALLFORM CARD_ABLE(0,ターン終了時)
PRINTL ③: 자신의 엔드 페이즈에, 자신의 묘지의 「메멘토」 마법 / 함정 카드 1장을 대상으로 발동할 수 있다.
PRINTL 그 카드를 자신 필드에 세트한다.

@CARDCAN_73890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 破壊レベル

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73890)
SIF RESULT == 1
	RETURN 0


IF 종류 == 효과後誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,73890)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0

	FOR カウンタ,2,13
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件レベル = カウンタ
		CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
		SIF RESULT == 1
			破壊レベル = カウンタ - 1
	NEXT
	SIF 破壊レベル < 1
		RETURN 0
	;その辺に메멘토
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 메멘토
	효과条件固名前:0 = 명해합룡－메멘트럴 텍트리카
	효과条件固名前:1 = 명해합룡－메멘트럴 텍트리카♀
	CALL 효과条件レベル_ADV(破壊レベル,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,73890)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

;汎用処理
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 마함全般
	효과条件カテゴリ = 메멘토
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所,73890)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 破壊レベル

IF 종류 == 효과後誘発
	FOR カウンタ,2,13
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件レベル = カウンタ
		CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
		SIF RESULT == 1
			破壊レベル = カウンタ - 1
	NEXT
	
	IF 듀얼리스트 == "자신"
		SETCOLOR C_YELLOW
		PRINTFORML 특수소환できるのは{破壊レベル}以下です
		RESETCOLOR
	ENDIF
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73890,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 메멘토
		;リカちゃんは대상外
		효과条件固名前 = 명해합룡－메멘트럴 텍트리카
		CALL 효과条件レベル_ADV(破壊レベル,"以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73890)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73890)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 마함全般
		효과条件カテゴリ = 메멘토
		CALL CARD_SET_BASIC(듀얼리스트,"묘지",ゾーン,場所,"裏側표시")
	ENDIF

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "冥骸府")
	CALL FULLRESET
	효과条件종류 = モン全般
	효과条件カテゴリ = 메멘토
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻",3)
ENDIF
