@CARDNAME_1330,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 전뇌계희－냥냥
	CASE "略称"
RESULTS = 전뇌냥냥
	CASE "カテゴリ"
RESULTS:0 = 전뇌계
ENDSELECT


@CARD_1330(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1330(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①: 이 카드가 묘지에 존재하고, 자신 필드에 레벨 3 몬스터가 일반 소환 / 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는 튜너로 취급하며, 필드에서 벗어났을 경우 제외된다.
PRINTL 이 턴, 자신은 레벨 또는 랭크가 3 이상인 몬스터밖에 특수 소환할 수 없다.
CALLFORM CARD_ABLE(2,제외時)
PRINTL ②: 이 카드가 제외되었을 경우, 이 카드 이외의 제외되어 있는 자신 카드 1장을 대상으로 발동할 수 있다.
PRINTL 그 카드를 덱으로 되돌린다.



@CARDCAN_1330(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1330)
SIF RESULT == 1
	RETURN 0
IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,1330)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,1330,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件レベル = 3
	효과条件종류 = レベル持ち
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 제외時
	SIF 발동済_2(듀얼리스트,1330)
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1330(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード
IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1330,1)
	IF RESULT == 0
		CALL RESETCONDITION		
		효과추가종류:0 = 2
		효과추가종류:1 = 7
		CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)

		CALLFORM SS소환可能レベル_ADV(3, "以上")
		CALL CARD_SS_RESTRICTION(듀얼리스트)
	ENDIF

ELSEIF 종류 == 제외時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1330,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件行先플래그 = 2
		효과条件他所 = 1
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"제외",ゾーン,場所)
	ENDIF
ENDIF
