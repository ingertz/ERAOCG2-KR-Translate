@CARDNAME_41331,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 전뇌계수－와시와시
	CASE "略称"
RESULTS = 주주
	CASE "カテゴリ"
RESULTS:0 = 전뇌계
ENDSELECT


@CARD_41331(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1700
ENDSELECT

RETURN 0

@CARD_EXPLANATION_41331(종류)
#DIM DYNAMIC 종류


PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 자신의 묘지에 레벨이 3의 배수인 몬스터 2장 이상 존재하는 한,
PRINTL 필드의 이 카드는 전투 / 효과 파괴 내성을 가진다.
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②: 레벨이 3의 배수인 몬스터 2장을 자신의 묘지에서 제외하고,
PRINTL 상대 필드의 카드 1장을 대상으로 발동할 수 있다.
PRINTL 그 카드를 묘지로 보낸다.


@CARDSUMMON_41331(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,41331) 
	RETURN RESULT
ENDIF


@CARDCAN_41331(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,41331)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = レベル持ち
	효과条件レベル:0 = 3
	효과条件レベル:1 = 6
	효과条件レベル:2 = 9
	효과条件レベル:3 = 12
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,41331)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = レベル持ち
	효과条件レベル:0 = 3
	효과条件レベル:1 = 6
	효과条件レベル:2 = 9
	효과条件レベル:3 = 12
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_41331(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC レベル数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41331,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = レベル持ち
		효과条件レベル:0 = 3
		효과条件レベル:1 = 6
		효과条件レベル:2 = 9
		효과条件レベル:3 = 12
		효과条件複数 = 2
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"リリース")
		ENDIF
	ENDIF
ENDIF



