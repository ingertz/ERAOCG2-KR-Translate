@CARDNAME_41330,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 전뇌계여우－센센
	CASE "略称"
RESULTS = 셴셴
	CASE "カテゴリ"
RESULTS:0 = 전뇌계
ENDSELECT


@CARD_41330(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 2400
ENDSELECT

RETURN 0

@CARD_EXPLANATION_41330(종류)
#DIM DYNAMIC 종류


PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 필드에서 묘지로 보내지는 카드는 묘지로는 가지 않고 제외된다.
CALLFORM CARD_ABLE(2,攻撃時誘発)
PRINTL ②: 자신 몬스터의 공격 선언시에 발동할 수 있다.
PRINTL 제외되어 있는 자신 몬스터 1장을 고르고 묘지로 되돌린다.
CALLFORM CARD_ABLE(3,묘지起動)
PRINTL ③: 이 카드명 이외의, 3의 배수인 레벨을 가지는 몬스터 2장을 자신 묘지에서 제외하고 발동할 수 있다.
PRINTL 이 카드를 묘지에서 특수 소환한다.
;PRINTL こ의 효과はこの카드가 묘지へ送られたターンには발동할 수 없다。


@CARDSUMMON_41330(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 9
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,41330) 
	RETURN RESULT
ENDIF


@CARDCAN_41330(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,41330)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,41330)
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) == 5
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_3(듀얼리스트,41330)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,41330,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = レベル持ち
	효과条件固名前 = 전뇌계여우－센센
	효과条件レベル:0 = 3
	효과条件レベル:1 = 6
	효과条件レベル:2 = 9
	효과条件レベル:3 = 12
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_41330(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC レベル数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"땅제외置換",3,2)

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41330,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般			
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"제외",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41330,3)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = レベル持ち
		효과条件固名前 = 전뇌계여우－센센
		효과条件レベル:0 = 3
		효과条件レベル:1 = 6
		효과条件レベル:2 = 9
		효과条件レベル:3 = 12
		효과条件複数 = 2
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
효과条件名前 = 전뇌계여우－선선
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF



