@CARDNAME_92551,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 네무렐리아 루브
	CASE "略称"
		RESULTS = 루브
	CASE "カテゴリ"
		RESULTS:0 = 네무렐리아
ENDSELECT


@CARD_92551(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250530
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92551(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의  ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：패 / 덱에서 "네무렐리아" 몬스터나 야수족 / 레벨 10 몬스터 1장을 수비 표시로 특수 소환한다。
PRINTL 이 효과로 특수 소환한 몬스터는 엔드 페이즈에 패로 되돌아간다。
CALL CARD_ABLE(2,묘지起動,フリー誘発)
PRINTL ②：이 카드가 묘지에 존재할 경우、상대 필드의 앞면 표시 몬스터 １장을 대상으로 하여 발동할 수 있다。
PRINTL 그 몬스터의 공격력 / 수비력은 턴 종료시까지、자신의 제외 상태인 뒷면 카드의 수× 100 내린다。
PRINTL 이 효과의 발동 후、이 카드를 제외한다。

@CARDCAN_92551(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92551)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	SIF 발동済_1(듀얼리스트,92551)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件スーパーカテゴリ = 네무렐리아
	효과条件종족 = 야수족
	효과条件レベル = 10
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	SIF 발동済_2(듀얼리스트,92551)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	효과条件除去種 = 一攻DOWN
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_92551(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正

IF 종류 == 마함起動 || (종류 == フリー誘発 && ゾーン == "마함")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92551,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件スーパーカテゴリ = 네무렐리아
		효과条件종족 = 야수족
		효과条件レベル = 10
		효과추가종류 = 効追終手
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 묘지起動 || (종류 == フリー誘発 && ゾーン == "묘지")
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92551,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		攻撃力修正 = RESULT * 100
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"一攻DOWN",攻撃力修正)

		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
