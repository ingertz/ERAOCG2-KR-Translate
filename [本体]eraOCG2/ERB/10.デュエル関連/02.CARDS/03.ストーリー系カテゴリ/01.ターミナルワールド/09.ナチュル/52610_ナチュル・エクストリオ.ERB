@CARDSUMMON_52610(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件名前 = 나츄르 비스트
ELSEIF EX参照 >= 10
	素材条件名前 = 나츄르 팔키온
ENDIF
IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,52610) 
	RETURN RESULT
ENDIF

@CARDCAN_52610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 現山札数(듀얼리스트) < 1
		RETURN 0
	SIF 묘지数(듀얼리스트) < 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_52610(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52610)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT ==1
			CALL CARD_DECK_DISPOSE(듀얼리스트,"덱",0)
			IF RESULT ==1
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	RETURN 0
ENDIF