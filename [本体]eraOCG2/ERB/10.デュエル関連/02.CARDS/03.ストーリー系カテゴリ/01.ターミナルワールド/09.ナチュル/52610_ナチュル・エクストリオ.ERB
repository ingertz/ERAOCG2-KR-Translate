@CARDNAME_52610,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 나츄르 엑스트리오
	CASE "略称"
RESULTS = 나츄르엑스트리오
	CASE "カテゴリ"
RESULTS:0 = 나츄르
ENDSELECT


@CARD_52610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 야수족 
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 1950
	CASE "특수불가"
		RETURN 1

ENDSELECT
RETURN 0

