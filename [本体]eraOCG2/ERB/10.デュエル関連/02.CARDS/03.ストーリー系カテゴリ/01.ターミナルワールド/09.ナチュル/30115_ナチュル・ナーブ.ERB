@CARDNAME_30115,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 나츄르 너브
	CASE "略称"
RESULTS = Nc 너브
	CASE "カテゴリ"
RESULTS:0 = 나츄르
ENDSELECT


@CARD_30115(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 200
	CASE "守備力"
		RETURN 300
	CASE "튜너"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_30115(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 자신 필드에 이 카드 이외의 「나츄르」 몬스터가 존재하는 상태에서,
PRINTL 상대가 마법 / 함정 카드를 발동했을 때, 이 카드를 릴리스하고 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
CALLFORM CARD_ABLE(0,戦破壊→묘지)
PRINTL ②: 이 카드가 필드에서 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 덱에서 「나츄르」 함정 카드를 1장 묘지로 보낸다.



@CARDCAN_30115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,30115)
SIF RESULT == 1
	RETURN 0


IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,30115)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 나츄르
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 戦破壊→묘지
	SIF !GROUPMATCH(묘지送り종류,"リリース","破壊","戦闘破壊")
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 나츄르
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_30115(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30115,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")		
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30115)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 나츄르
		효과条件종류 = 함정全般
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
