@CARDNAME_30110,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 나츄르 몰 크리켓
	CASE "略称"
RESULTS = 엔씨 크리켓
	CASE "カテゴリ"
RESULTS:0 = 나츄르
ENDSELECT


@CARD_30110(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARD_EXPLANATION_30110(종류)
#DIM DYNAMIC 종류


;方針としては나츄르一匹一匹の출력を上げることで、クリケットとカメリアのパワーを少し下げる。

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動 ,フリー誘発)
PRINTL ①: 자신 상대의 메인 페이즈에, 이 카드를 릴리스하고 발동할 수 있다.
PRINTL 덱에서 「나츄르」 몬스터 1장을 특수 소환한다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 묘지에 존재하는 상태에서, 상대가 EX덱에서 몬스터를 특수 소환했을 경우,
PRINTL 또는 자신이 EX덱에서 「나츄르」 몬스터를 특수 소환했을 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.


@CARDCAN_30110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,30110)
SIF RESULT == 1
	RETURN 0


IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,30110)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 나츄르
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,30110)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,30110,"묘지")
	SIF RESULT == 1
		RETURN 0

	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX특수소환",1)
	SIF RESULT == 1
		RETURN 1
	CALL FULLRESET
	효과条件カテゴリ = 나츄르
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX특수소환")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_30110(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30110,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 나츄르
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)	
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30110,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
