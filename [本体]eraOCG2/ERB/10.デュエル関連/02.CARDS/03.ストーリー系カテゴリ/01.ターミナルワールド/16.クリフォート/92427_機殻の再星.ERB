@CARDNAME_92427,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 리크리포트
	CASE "略称"
RESULTS = 기각의 재성
	CASE "カテゴリ"
RESULTS:0 = 크리포트
ENDSELECT


@CARD_92427(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속함정
ENDSELECT

RETURN 0
@CARD_EXPLANATION_92427(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환強制誘発)
PRINTL ①: 레벨 4 이하의 몬스터가 소환된 경우에 발동한다.
PRINTL 그 몬스터의 효과를 턴 종료시까지 무효로 한다.
CALLFORM CARD_ABLE(0,常在無効)
PRINTL ②: 특수소환된 레벨 5 이상의 몬스터의 효과는 무효화되며,
PRINTL 그 몬스터는 필드에서 벗어난 경우에 제외된다.
PRINTL ③: 필드에 「기각의 재성」 이외의 「크리포트」 카드가 존재하지 않는 경우, 이 카드의 효과는 무효화된다.


@CARDCAN_92427(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC EX条件달성


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92427)
SIF RESULT == 1
	RETURN 0



IF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 常在無効
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 클리포트
	효과条件固名前 = 리크리포트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 소환強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 클리포트
	효과条件固名前 = 리크리포트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 最終소환타입 != "通常"
		RETURN 0
	CALLFORM CARDNAME_{最終소환ID}("カテゴリ") 
	IF !MATCH(RESULTS, "클리포트")
		SIF GETVAR(@"%最終소환決者%_MZ_星:最終소환場所") > 4
			RETURN 0
	ELSE
		SIF GETVAR(@"%最終소환決者%_MZ_条達:最終소환場所") == 0
			RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_92427(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 素材数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 설정종족
#DIM DYNAMIC 選択효과
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 효과先
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92427)
	IF RESULT == 0
	ENDIF

ELSEIF 종류 == 常在無効
	CALL FULLRESET
	효과条件召種 = 召種全특수
	CALL 효과条件レベル_ADV(5, "以上")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無効")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"除処")

	CALL FULLRESET
;	효과元カードID = 92427
	효과条件召種 = 召種全특수
	CALL 효과条件レベル_ADV(5, "以上")
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効",0,92427)
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"除処",0,92427)
ELSEIF 종류 == 소환強制誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92427)
	IF RESULT == 0
		효과先 = 最終소환場所
		IF 효과先 != 5
			IF 最終소환決者 == 듀얼리스트
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(対面者,"自動処理",효과先,"無効")
			ELSE
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",효과先,"無効")
			ENDIF
		ENDIF
	ENDIF
ENDIF
