@CARD_EXPLANATION_90050(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드에 "인벨즈" 몬스터가 존재할 경우, 이 카드의 발동은 패에서도 할 수 있다.
CALLFORM CARD_ABLE(0,마함起動,손패起動,フリー誘発)
PRINTL ①: 덱에서 "벨즈" 몬스터 1장을 묘지로 보내고,
PRINTL 자신 필드의 "벨즈" 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터의 공격력은 턴 종료 시까지 800 증가한다.
PRINTL "인벨즈" 몬스터를 묘지로 보내 이 카드를 발동했을 경우,
PRINTL 또한 덱에서 레벨 5 이상의 "벨즈" 몬스터 1장을 선택하여 패에 넣을 수 있다.

@CARDCAN_90050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,90050)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	IF ゾーン == "손패"
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 인벨즈
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 벨즈
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 벨즈
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_90050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,90050)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과条件カテゴリ = 벨즈
		효과条件종류 = モン全般
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"덱",ゾーン,場所)
		選択カード = RESULT	
		IF 選択カード != 9999
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_덱:選択カード")}, "カテゴリ"
			SIF !MATCH(RESULTS,"인벨즈")
				条件달성 ++
			CALL CARD_DISPOSE(듀얼리스트,"덱",選択カード)
		ENDIF

		CALL FULLRESET
		효과条件カテゴリ = 벨즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT

		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"一攻",800)
		ENDIF

		IF 条件달성 == 1
			CALL FULLRESET
			효과条件カテゴリ = 벨즈
			효과条件종류 = モン全般
			효과条件任意 = 1
			CALL 효과条件レベル_ADV(5, "以上")
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF