@CARDNAME_55601,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 젬나이트 지르코니아
	CASE "略称"
RESULTS = 젬나이트 지르코
	CASE "カテゴリ"
RESULTS:0 = 젬나이트
ENDSELECT


@CARD_55601(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족 
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 2500
	CASE "分割"
		RETURN 효과なし
	CASE "성별"
		RETURN 雄性
ENDSELECT
RETURN 0

