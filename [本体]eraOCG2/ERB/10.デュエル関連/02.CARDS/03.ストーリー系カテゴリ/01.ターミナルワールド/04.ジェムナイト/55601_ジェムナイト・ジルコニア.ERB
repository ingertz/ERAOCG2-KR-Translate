@CARDSUMMON_55601(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 젬나이트
ELSEIF EX参照 >= 10
	素材条件종족 = 원소정령족
ENDIF
IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,55601) 
	RETURN RESULT
ENDIF

