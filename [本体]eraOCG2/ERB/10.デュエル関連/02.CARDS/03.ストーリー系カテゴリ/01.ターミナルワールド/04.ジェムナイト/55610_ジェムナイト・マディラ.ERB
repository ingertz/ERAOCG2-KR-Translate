@CARDNAME_55610,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 젬나이트 마디라
	CASE "略称"
RESULTS = 젬나이트 마디라
	CASE "カテゴリ"
RESULTS:0 = 젬나이트
ENDSELECT


@CARD_55610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족 
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 1950
ENDSELECT
RETURN 0

