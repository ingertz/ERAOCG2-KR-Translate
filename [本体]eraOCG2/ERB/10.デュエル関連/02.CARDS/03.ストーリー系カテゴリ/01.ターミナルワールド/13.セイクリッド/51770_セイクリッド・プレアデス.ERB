@CARDNAME_51770,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 세이크리드 프레아데스
	CASE "略称"
RESULTS = 프레아데스
	CASE "カテゴリ"
RESULTS:0 = 세이크리드
ENDSELECT


@CARD_51770(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 1500
ENDSELECT

RETURN 0
@CARDSUMMON_51770(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 5
지정素材名前 = 세이크리드 カドケウス

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,51770) 
	RETURN RESULT
ENDIF



