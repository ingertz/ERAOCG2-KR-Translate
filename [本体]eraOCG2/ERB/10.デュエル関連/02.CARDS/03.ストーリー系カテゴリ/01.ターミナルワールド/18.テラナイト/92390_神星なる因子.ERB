@CARDNAME_92391,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 신성인자
	CASE "略称"
RESULTS = 신성인자
	CASE "カテゴリ"
RESULTS:0 = 텔라나이트
ENDSELECT


@CARD_92391(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92391(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드는 룰 상 "텔라나이트" 카드로 취급한다.
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ①: 상대 카드의 효과가 발동했을 때,
PRINTL 자신 필드의 앞면 표시 "텔라나이트" 몬스터 1장을 릴리스하고 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
PRINTL 그 후, 자신은 덱에서 1장 드로우한다.

@CARDCAN_92391(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92391)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 테라나이트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ENDIF


@CARDEFFECT_92391(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92391)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件표시 = 表側표시
		효과条件カテゴリ = 테라나이트
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"破壊")
			CALL CARDDRAW_BASIC(듀얼리스트,1)
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF

