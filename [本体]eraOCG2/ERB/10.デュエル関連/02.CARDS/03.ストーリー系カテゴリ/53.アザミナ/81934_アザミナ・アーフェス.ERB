@CARDNAME_81934,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 아자미나 아-페스
	CASE "略称"
RESULTS = 계화 아-페스
	CASE "カテゴリ"
RESULTS:0 = 아자미나
RESULTS:1 = 하얀 숲
ENDSELECT


@CARD_81934(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_81934(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명은 룰 상 「하얀 숲」 카드로서도 취급한다.
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：자신 필드 묘지의, 악마족 미확인족 마법사족의 융합 싱크로 몬스터의 수까지 이하의 효과를 반복할 수 있다.
PRINTL ●상대 필드의 카드 1장을 패로 되돌린다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：「아자미나 아-페스」가 몬스터의 효과로 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 묘지에서 「아자미나 아-페스」 1장을 자신 필드에 세트한다.



@CARDCAN_81934(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,81934)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,81934)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件종족:0 = 마법사족
	효과条件종족:1 = 악마족
    효과条件종족:2 = 미확인족
	효과条件종류:0 = 융합
	효과条件종류:1 = 싱크로
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,81934)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과条件名前 = 아자미나 아-페스
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"モン효과墓送")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_81934(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 대상枚数


IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,81934,1)
	;カード발동確認
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲묘지 = 1
		효과条件종족:0 = 마법사족
		효과条件종족:1 = 악마족
 	    효과条件종족:2 = 미확인족
		효과条件종류:0 = 융합
		효과条件종류:1 = 싱크로
		효과条件CNT = 1
		효과条件限界値 = 10
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		대상枚数 = RESULT

		CALL FULLRESET
		효과条件複数 = 대상枚数
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"손패")
	ENDIF
ELSEIF 종류 == 효과後誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,81934,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件名前 = 아자미나 아-페스
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL CARD_DISPOSE(듀얼리스트,"묘지",選択カード,마함行き,1)
		ENDIF
	ENDIF
ENDIF
