@CARDNAME_50616,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 심연의 신수 디스 파텔
	CASE "略称"
RESULTS = 디스 파텔
	CASE "カテゴリ"
RESULTS:0 = 비-스테드
ENDSELECT


@CARD_50616(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3500
	CASE "守備力"
		RETURN 3500
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN ULR
ENDSELECT

RETURN 0

