@CARDNAME_55560,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 스프라이트 엘프
	CASE "略称"
RESULTS = 스프라이트 엘프
	CASE "カテゴリ"
RESULTS:0 = 스프라이트
ENDSELECT


@CARD_55560(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1400
ENDSELECT

RETURN 0
@CARDSUMMON_55560(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件レベル = 2

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,55560) 
	RETURN RESULT
ENDIF

