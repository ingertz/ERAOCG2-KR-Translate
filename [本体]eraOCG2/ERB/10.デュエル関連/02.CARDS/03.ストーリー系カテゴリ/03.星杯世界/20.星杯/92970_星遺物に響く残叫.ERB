@CARDNAME_92970,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 성유물에 울리는 잔향
	CASE "略称"
RESULTS = 성유물울리는잔향
	CASE "カテゴリ"
RESULTS:0 = 성유물
ENDSELECT


@CARD_92970(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92970(종류)
#DIM DYNAMIC 종류
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ①: 자신 필드에 2체 이상의 링크 몬스터가 존재하고,
PRINTL 상대가 몬스터의 효과, 마법 / 함정 카드를 발동했을 때 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
PRINTL 
@CARDCAN_92970(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92970)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0	
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 링크
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,0,"",-1)
	SIF RESULT <= 1
		RETURN 0		
	RETURN 1
ENDIF


@CARDEFFECT_92970(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92970)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		RETURN 1
	ENDIF
	RETURN 0
ENDIF
