@CARDCAN_129680(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,129680)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,129680)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == 맥시멈条件
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_필드:1") != 129681 && GETVAR(@"%듀얼리스트%_필드:3") != 129682
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_129680(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,129680,1)
	IF RESULT == 0
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
				CONTINUE
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:カウンタ")}("カテゴリ")
			SIF !MATCH(RESULTS,"일루미네이션 스타즈")
				CONTINUE
			CALL FULLRESET
			CALL CARD_DISPOSE(듀얼리스트,"필드",カウンタ)
			選択数++
		NEXT
		REPEAT 選択数
			SIF モンスター数(対面者) >= 5
				BREAK
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,129683,"守備표시",1)
		REND
	ENDIF
ELSEIF 종류 == 맥시멈条件
	IF GETVAR(@"%듀얼리스트%_필드:1") == 129681 && GETVAR(@"%듀얼리스트%_필드:3") == 129682
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	ENDIF
ENDIF



