@CARDNAME_4652,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 과묵한 래빗 딜러 안나
	CASE "略称"
RESULTS = 래빗 D 안나
	CASE "カテゴリ"
RESULTS:0 = 래빗 걸
		RESULTS:1 = 래빗시티
		RESULTS:2 = 밀리언 라이브!
		; RESULTS:2 = THE IDOLM@STER
		; RESULTS:4 = MILLION LIVE!
		RESULTS:4 = 모치즈키 안나
ENDSELECT

@CARD_4652(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 200
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 펜듈럼
	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4652(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("P_MAGIC")
PRINTL 이 카드명의 ①의 P 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 패 / EX덱에서 레벨 8의 "래빗 걸" P 몬스터 1장을 특수 소환한다.
CALL CARD_ABLE(0,지속효과)
PRINTL ②: 자신 / 상대의 마법 & 함정 존에 이 카드명 이외의 "래빗 걸" P 몬스터 카드가 존재할 경우,
PRINTL 자신 필드의 "래빗 걸" / "바니 폴리스" 몬스터 카드 전부는 일시적 파괴 내성을 얻는다.
CALL TEXT_DECORATION("P_MAGIC_2")
PRINTL 이 카드명의 몬스터 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(2,ターン開始時)
PRINTL ①: 자신 / 상대 스탠바이 페이즈에 발동할 수 있다.
PRINTL 패에서 이 카드 이외의 "래빗 걸" / "바니 폴리스" 카드 1장을 덱으로 되돌리고, 패의 이 카드를 마법 & 함정 존에 두고,
PRINTL 덱에서 이 효과로 덱으로 되돌린 카드명 이외의 레벨 8의 "래빗 걸" P 몬스터 카드 1장을 상대의 마법 & 함정 존에 놓는다.


@CARDCAN_4652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 持ち主판정 ;0で자신,1で상대

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4652)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,4652)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲EX덱 = 1
	효과条件종류 = モン全般
	효과条件分割 = 펜듈럼
효과条件カテゴリ = 래빗 걸
	효과条件レベル = 8
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") > 1
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件종류 = モン全般
	효과条件分割 = 펜듈럼
효과条件カテゴリ = 래빗 걸
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ:0 = 래빗 걸
	효과条件カテゴリ:1 = 바니 폴리스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_2(듀얼리스트,4652)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件他所 = 1
	효과条件カテゴリ:0 = 래빗 걸
	효과条件カテゴリ:1 = 바니 폴리스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件分割 = 펜듈럼
효과条件カテゴリ = 래빗 걸
	효과条件レベル = 8
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	SIF 마함数(対面者) >= 5
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4652(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カードゾーン
#DIM DYNAMIC 選択カード場所
#DIM DYNAMIC 持ち主판정 ;0で자신,1で상대

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4652)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4652,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲EX덱 = 1
효과条件カテゴリ = 래빗 걸
		효과条件分割 = 펜듈럼
		효과条件レベル = 8
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ:0 = 래빗 걸
	효과条件カテゴリ:1 = 바니 폴리스
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"一破")
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"一破")
ELSEIF 종류 == ターン開始時
	SIF GETVAR(@"%듀얼리스트%_손패:場所") != 4652
		RETURN
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4652,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件他所 = 1
		효과条件行先플래그 = 덱行き
		효과条件カテゴリ:0 = 래빗 걸
		효과条件カテゴリ:1 = 바니 폴리스
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		選択カードID = RESULT:1
		IF RESULT == 1
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
			CALL FULLRESET
			효과範囲덱 = 1
효과条件カテゴリ = 래빗 걸
			효과条件固ID = 選択カードID
			효과条件レベル = 8
			CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시",2)
		ENDIF
	ENDIF
ENDIF