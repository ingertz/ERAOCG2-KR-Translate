@CARDNAME_79806,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 음마계대본 「강제 오디션」
	CASE "略称"
RESULTS = 「강제심사회」
	CASE "カテゴリ"
RESULTS:0 = 마계대본
RESULTS:1 = 음마계대본
RESULTS:2 = E.R.A
ENDSELECT


@CARD_79806(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
	CASE "추가日"
		RETURN 250522
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79806(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번만 사용할 수 있다.
CALLFORM CARD_ABLE(1,마함起動,손패起動,フリー誘発)
PRINTL ①：자신 필드에 「음마계극단」 카드가 존재할 경우에,
PRINTL 상대 필드의 암컷 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터의 컨트롤을 엔드 페이즈까지 얻는다.
CALLFORM CARD_ABLE(2,破壊→묘지)
PRINTL ②：필드에 「음마계극단」 카드가 존재하고,
PRINTL 이 카드가 상대 효과로 파괴되었을 경우에 발동할 수 있다.
PRINTL 상대 덱 / EX덱에서 암컷 몬스터 1장을 자신 필드에 특수 소환한다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79806(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79806)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,79806)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 음마계극단
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 破壊→묘지
	SIF 발동済_2(듀얼리스트,79806)
		RETURN 0
	SIF 処理中_듀얼리스트 == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件カテゴリ = 음마계극단
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대덱 = 1
	효과範囲상대EX덱 = 1
	효과条件성별 = 牝性
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79806(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 조교대상
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79806,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별 = 牝性
		효과추가종류 = 5
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
		IF RESULT == 1
			選択カード = GETVAR(@"%듀얼리스트%_필드:コントロール先")
			CALL CARD_EROTICS(選択カード)
			IF RESULT != -1 && 듀얼리스트 == "자신"
				조교대상 = RESULT
				SETCOLOR C_YELLOW
				PRINTFORMW %CALLNAME:조교대상%는 음마계극단의 오디션에 참가당했다！
				PRINTFORMW 그대로 데뷔 작품을 촬영당했다…
				PRINTL
				RESETCOLOR
				CALL EROTIC_MOVE("晒しもの",조교대상,-1,150)
				CALL EROTIC_MOVE("前穴凌辱",조교대상,MASTER,500)
			ENDIF
			SIF GETVARS(@"%듀얼리스트%_フィールド名") == "음마계 스튜디오"
				CALL INMAKAIFILM_GENERATE(듀얼리스트,79806,1,選択カード)
		ENDIF
	ENDIF
ELSEIF 종류 == 破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79806,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대덱 = 1
		효과範囲상대EX덱 = 1
		효과条件성별 = 牝性
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード = RESULT:1
			CALL CARD_EROTICS(選択カード)
			IF RESULT != -1 && 듀얼리스트 == "자신"
				조교대상 = RESULT
				SETCOLOR C_YELLOW
				PRINTFORMW %CALLNAME:조교대상%는 음마계극단의 오디션에 참가당했다！
				PRINTFORMW 그대로 데뷔 작품을 촬영당했다…
				PRINTL
				RESETCOLOR
				CALL EROTIC_MOVE("晒しもの",조교대상,-1,150)
				CALL EROTIC_MOVE("前穴凌辱",조교대상,MASTER,500)
			ENDIF
			SIF GETVARS(@"%듀얼리스트%_フィールド名") == "음마계 스튜디오"
				CALL INMAKAIFILM_GENERATE(듀얼리스트,79806,2,選択カード)
		ENDIF
	ENDIF
ENDIF

;台本の①의 효과で生成されるフィルムの설정
@INMAKAIFILM_79806_1(女優ID:0,女優ID:1,女優ID:2,女優ID:3,女優ID:4)
#DIM DYNAMIC 女優ID, 5 ;대상の牝몬스터의 カードID
#DIM DYNAMIC 女優대상, 5 ;えっち用のID：csv無しなら-1
#DIMS DYNAMIC 女優名前, 5
#DIM DYNAMIC 女優人数 ;女優IDが存在する総人数
#DIM DYNAMIC シチュ종류 ;표시される画像のシチュエ-ション지정したい場合「シチュ종류 = シチュ_파이즈리」のようにする。特に지정しない(ランダムでいい)場合シチュ종류 = 0。지정できるシチュは@IMAGE_HAMEDORIを参照。
#DIMS DYNAMIC タイトル
#DIM DYNAMIC カウンタ
VARSET RESULT
VARSET RESULTS

FOR カウンタ,0,5
	IF 女優ID:カウンタ <= 0
		女優人数 = カウンタ
		BREAK
	ENDIF
	CALL INMAKAIFILM_GETNAME(女優ID:カウンタ)
	女優대상:カウンタ = RESULT
	女優名前:カウンタ = %RESULTS%
NEXT

;生成されるフィルムは【%女優名前%】『%タイトル%』となる
SELECTCASE RAND:4
	CASE 0
		IF 女優대상 != -1
			タイトル = %TOSTR_CUPSIZE(女優대상)%컵 신인 여배우  %女優名前%、음마계필름 데뷔！
		ELSE
			タイトル = 신인여배우 %女優名前%、음마계필름 데뷔！
		ENDIF
	CASE 1
		タイトル = 새로운 저를、물들여주세요… debut
		シチュ종류 = シチュ_삽입
	CASE 2
		IF 女優대상 != -1
			タイトル = %女優名前%、%TOSTR_CUPSIZE(女優대상)%컵、신인이 아니게된 몸과 재능、해금。
		ELSE
			タイトル = %女優名前%、신인이 아니게된 몸과 재능、해금。
		ENDIF
	;半分おふざけ
	CASE 3
		TRYCALLFORM CARD_{女優ID}, "종류"
		SELECTCASE RESULT 
			CASE 의식
				タイトル = 제 몸을 마칠게요… 신인 %女優名前%、금단의 의식소환
			CASE 융합
				タイトル = 저와 당신으로 초융합 신인 %女優名前%、debut
			CASE 싱크로
				タイトル = 신인 %女優名前%、자지로 튜닝되어 절정 싱크로 소환
			CASE 엑시즈
				タイトル = 신인 아가씨 FILE No.{RAND:99 + 1} %女優名前%
			CASE 링크
				タイトル = 쾌락회로 서킷 콤바인！연속 링크로 마구가버리는 신인
			CASE 초차원
				タイトル = 차원을 넘어서 전격 데뷔！ 신인 %女優名前%
			CASEELSE
				タイトル = 신인 그 %女優名前%가 음마계필름 에 첫 소환！
		ENDSELECT
ENDSELECT

RESULT = シチュ종류
RESULTS = %타이틀%

